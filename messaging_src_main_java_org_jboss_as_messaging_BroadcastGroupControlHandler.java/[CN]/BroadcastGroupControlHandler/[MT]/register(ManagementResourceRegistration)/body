{
  super.register(registry);
  registry.registerOperationHandler(GET_CONNECTOR_PAIRS_AS_JSON,this,new DescriptionProvider(){
    @Override public ModelNode getModelDescription(    Locale locale){
      return MessagingDescriptions.getNoArgSimpleReplyOperation(locale,GET_CONNECTOR_PAIRS_AS_JSON,CommonAttributes.BROADCAST_GROUP,ModelType.STRING,false);
    }
  }
,EnumSet.of(OperationEntry.Flag.READ_ONLY,OperationEntry.Flag.RUNTIME_ONLY));
}
