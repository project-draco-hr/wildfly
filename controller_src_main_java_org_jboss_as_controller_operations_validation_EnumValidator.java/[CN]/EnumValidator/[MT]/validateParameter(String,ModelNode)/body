{
  super.validateParameter(parameterName,value);
  if (value.isDefined()) {
    String tuString=value.asString();
    E enumValue=Enum.valueOf(enumType,tuString.toUpperCase(Locale.ENGLISH));
    if (enumValue == null || !allowedValues.contains(enumValue)) {
      throw new OperationFailedException(new ModelNode().set(String.format("Invalid value %s for %s; legal values are %s",tuString,parameterName,allowedValues)));
    }
  }
}
