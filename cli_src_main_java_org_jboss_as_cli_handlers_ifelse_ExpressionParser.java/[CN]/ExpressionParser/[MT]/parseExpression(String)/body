{
  System.out.println("parsing: '" + input + "'");
  final String prevInput=this.input;
  final int prevPos=this.pos;
  this.input=input;
  this.pos=0;
  Operation op=null;
  while (!isEOL()) {
    op=getNextOperationFor(op);
  }
  if (prevInput != null) {
    this.input=prevInput;
    this.pos+=prevPos + 2;
  }
  System.out.println("parsed: " + op);
  return op;
}
