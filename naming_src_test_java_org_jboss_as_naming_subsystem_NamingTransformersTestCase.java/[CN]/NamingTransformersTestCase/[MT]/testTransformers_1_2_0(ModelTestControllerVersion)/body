{
  KernelServicesBuilder builder=createKernelServicesBuilder(createAdditionalInitialization()).setSubsystemXmlResource("subsystem_with_expressions_compatible_1.2.0.xml");
  builder.createLegacyKernelServicesBuilder(null,version,VERSION_1_2_0).addMavenResourceURL("org.jboss.as:jboss-as-naming:" + version.getMavenGavVersion()).skipReverseControllerCheck();
  KernelServices mainServices=builder.build();
  KernelServices legacyServices=mainServices.getLegacyServices(VERSION_1_2_0);
  assertNotNull(legacyServices);
  checkSubsystemModelTransformation(mainServices,VERSION_1_2_0);
  checkExternalContextEnvironmentBindingTransformationFails(mainServices,VERSION_1_2_0);
}
