{
  if ((manager != null) && stopManager) {
    try {
      manager.stop();
    }
 catch (    Exception e) {
      log.warn(e.getMessage(),e);
    }
  }
  if (cacheContainer != null) {
    try {
      cacheContainer.stop();
    }
 catch (    Exception e) {
      log.warn(e.getMessage(),e);
    }
  }
}
