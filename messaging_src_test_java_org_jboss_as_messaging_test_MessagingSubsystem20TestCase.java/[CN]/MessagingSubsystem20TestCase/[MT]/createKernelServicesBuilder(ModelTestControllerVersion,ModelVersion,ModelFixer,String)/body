{
  KernelServicesBuilder builder=createKernelServicesBuilder(AdditionalInitialization.MANAGEMENT).setSubsystemXmlResource(xmlFileName);
  builder.createLegacyKernelServicesBuilder(createAdditionalInitialization(),controllerVersion,messagingVersion).addMavenResourceURL("org.jboss.as:jboss-as-messaging:" + controllerVersion.getMavenGavVersion()).configureReverseControllerCheck(null,fixer).addMavenResourceURL(HornetQDependencies.getDependencies(controllerVersion));
  return builder;
}
