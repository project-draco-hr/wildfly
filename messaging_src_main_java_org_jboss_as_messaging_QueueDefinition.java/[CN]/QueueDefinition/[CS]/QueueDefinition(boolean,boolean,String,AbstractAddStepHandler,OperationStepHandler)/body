{
  super(PathElement.pathElement(path),MessagingExtension.getResourceDescriptionResolver(CommonAttributes.QUEUE),addHandler,removeHandler);
  this.registerRuntimeOnly=registerRuntimeOnly;
  this.runtimeOnly=runtimeOnly;
  if (!runtimeOnly) {
    ApplicationTypeConfig atc=new ApplicationTypeConfig(MessagingExtension.SUBSYSTEM_NAME,path);
    accessConstraints=new ApplicationTypeAccessConstraintDefinition(atc).wrapAsList();
  }
 else {
    accessConstraints=Collections.emptyList();
  }
}
