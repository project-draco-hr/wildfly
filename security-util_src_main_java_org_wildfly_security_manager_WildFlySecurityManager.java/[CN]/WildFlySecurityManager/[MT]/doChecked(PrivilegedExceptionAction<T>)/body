{
  final ThreadLocal<Boolean> checking=WildFlySecurityManager.CHECKING;
  if (checking.get() == TRUE) {
    try {
      return action.run();
    }
 catch (    RuntimeException e) {
      throw e;
    }
catch (    Exception e) {
      throw new PrivilegedActionException(e);
    }
  }
  checking.set(TRUE);
  try {
    return action.run();
  }
 catch (  RuntimeException e) {
    throw e;
  }
catch (  Exception e) {
    throw new PrivilegedActionException(e);
  }
 finally {
    checking.set(FALSE);
  }
}
