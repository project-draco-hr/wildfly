def test_ensure_ascii_false_returns_unicode(self):
    self.assertEquals(type(json.dumps([], ensure_ascii=False)), unicode)
    self.assertEquals(type(json.dumps(0, ensure_ascii=False)), unicode)
    self.assertEquals(type(json.dumps({}, ensure_ascii=False)), unicode)
    self.assertEquals(type(json.dumps('', ensure_ascii=False)), unicode)
