{
  RegistryService<K,V> service=new RegistryService<>();
  return AsynchronousService.addService(target,CacheServiceNames.REGISTRY.getServiceName(containerName,cacheName),service).addDependency(CacheServiceNames.REGISTRY_FACTORY.getServiceName(containerName,cacheName),RegistryFactory.class,(Injector)service.factory).addDependency(CacheServiceNames.REGISTRY_ENTRY.getServiceName(containerName,cacheName),RegistryEntryProvider.class,service.provider);
}
