{
  InjectedValue<Cache> cache=new InjectedValue<Cache>();
  InjectedValue<Registry.RegistryEntryProvider> providerValue=new InjectedValue<Registry.RegistryEntryProvider>();
  return Collections.<ServiceController<?>>singleton(target.addService(JVM_ROUTE_REGISTRY_SERVICE_NAME,new RegistryService(cache,providerValue)).addDependency(CacheService.getServiceName(DEFAULT_CACHE_CONTAINER,null),Cache.class,cache).addDependency(DistributedCacheManagerFactoryService.JVM_ROUTE_REGISTRY_ENTRY_PROVIDER_SERVICE_NAME,Registry.RegistryEntryProvider.class,providerValue).setInitialMode(ServiceController.Mode.ON_DEMAND).install());
}
