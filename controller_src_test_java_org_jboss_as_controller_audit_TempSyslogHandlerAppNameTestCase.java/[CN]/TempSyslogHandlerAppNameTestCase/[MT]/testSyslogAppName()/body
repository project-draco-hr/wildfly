{
  PathManagerService service=new PathManagerService(){
  }
;
  SyslogAuditLogHandler handler=new SyslogAuditLogHandler("xxx","xxxx",10,service);
  handler.initialize();
  Assert.assertEquals("WildFly",handler.getHandlerAppName());
  System.setProperty("org.jboss.TEMP.audit.log.appName","CustomApp");
  try {
    handler=new SyslogAuditLogHandler("xxx","xxxx",10,service);
    handler.initialize();
    Assert.assertEquals("CustomApp",handler.getHandlerAppName());
  }
  finally {
    System.clearProperty("org.jboss.TEMP.audit.log.appName");
  }
}
