{
  return target.addService(getServiceName(this.name),this).setInitialMode(ServiceController.Mode.ACTIVE).addDependency(ServerEnvironmentService.SERVICE_NAME,ServerEnvironment.class,this.environment).addDependency(DependencyType.OPTIONAL,ServiceName.JBOSS.append("txn","TransactionManager"),TransactionManager.class,this.transactionManager).addDependency(DependencyType.OPTIONAL,ServiceName.JBOSS.append("mbean","server"),MBeanServer.class,this.mbeanServer);
}
