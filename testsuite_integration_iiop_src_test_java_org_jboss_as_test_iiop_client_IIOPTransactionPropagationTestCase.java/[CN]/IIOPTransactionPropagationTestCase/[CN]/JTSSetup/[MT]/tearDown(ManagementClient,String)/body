{
  log.info("JTSSetup.tearDown");
  this.managementClient=managementClient;
  boolean isNeedReload=false;
  if (JACORB_TRANSACTIONS_JTA.equalsIgnoreCase(jacorbTransaction)) {
    setJacorbJTS(false);
    isNeedReload=true;
  }
  if (!isTransactionJTS) {
    setTransactionJTS(false);
  }
  if (isNeedReload) {
    reload();
  }
}
