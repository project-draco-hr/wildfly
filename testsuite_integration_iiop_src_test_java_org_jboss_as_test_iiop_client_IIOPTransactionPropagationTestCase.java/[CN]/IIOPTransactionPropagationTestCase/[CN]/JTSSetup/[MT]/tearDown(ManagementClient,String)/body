{
  log.info("JTSSetup.tearDown");
  this.managementClient=managementClient;
  boolean isNeedReload=false;
  if (IIOP_TRANSACTIONS_JTA.equalsIgnoreCase(iiopTransaction)) {
    setJTS(false);
    isNeedReload=true;
  }
  if (!isTransactionJTS) {
    setTransactionJTS(false);
  }
  if (isNeedReload) {
    reload();
  }
}
