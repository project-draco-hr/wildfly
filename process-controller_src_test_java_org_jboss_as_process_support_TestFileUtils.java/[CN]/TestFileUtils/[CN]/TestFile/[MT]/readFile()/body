{
  List<String> lines=new ArrayList<String>();
  BufferedReader reader=null;
  try {
    reader=new BufferedReader(new FileReader(file));
    String line=reader.readLine();
    while (line != null) {
      lines.add(line);
      line=reader.readLine();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
 finally {
    close(reader);
  }
  return lines;
}
