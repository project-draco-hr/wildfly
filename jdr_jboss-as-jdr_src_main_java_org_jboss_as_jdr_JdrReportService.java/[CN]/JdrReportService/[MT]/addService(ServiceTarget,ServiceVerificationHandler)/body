{
  JdrReportService service=new JdrReportService();
  return target.addService(SERVICE_NAME,service).addDependency(ServerEnvironmentService.SERVICE_NAME,ServerEnvironment.class,service.serverEnvironmentValue).addDependency(Services.JBOSS_SERVER_CONTROLLER,ModelController.class,service.modelControllerValue).addListener(verificationHandler).setInitialMode(ServiceController.Mode.ACTIVE).install();
}
