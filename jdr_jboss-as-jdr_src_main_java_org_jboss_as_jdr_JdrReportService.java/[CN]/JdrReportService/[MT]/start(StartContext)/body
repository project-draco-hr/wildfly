{
  final ThreadFactory threadFactory=new JBossThreadFactory(new ThreadGroup("JdrReportCollector-threads"),Boolean.FALSE,null,"%G - %t",null,null,doPrivileged(GetAccessControlContextAction.getInstance()));
  executorService=Executors.newCachedThreadPool(threadFactory);
  serverEnvironment=serverEnvironmentValue.getValue();
  controllerClient=modelControllerValue.getValue().createClient(executorService);
}
