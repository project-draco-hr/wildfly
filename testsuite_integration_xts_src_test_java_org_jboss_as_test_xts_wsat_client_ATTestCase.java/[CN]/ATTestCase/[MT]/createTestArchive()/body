{
  WebArchive archive=ShrinkWrap.create(WebArchive.class,ARCHIVE_NAME + ".war").addPackage(AT.class.getPackage()).addPackage(ATClient.class.getPackage()).addPackage(EventLog.class.getPackage()).addPackage(BaseFunctionalTest.class.getPackage()).addAsResource("context-handlers.xml").addAsManifestResource(new StringAsset("Dependencies: org.jboss.xts,org.jboss.jts\n"),"MANIFEST.MF").addAsWebInfResource(EmptyAsset.INSTANCE,ArchivePaths.create("beans.xml"));
  return archive;
}
