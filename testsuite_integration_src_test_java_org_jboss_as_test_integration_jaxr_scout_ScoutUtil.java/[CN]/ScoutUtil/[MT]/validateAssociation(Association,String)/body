{
  Organization o=(Organization)a.getSourceObject();
  if (o.getName() == null || o.getName().getValue() == null)   throw new Exception("Source OrgName in association is null");
  if (!o.getName().getValue().equals(sourceOrgName)) {
    throw new Exception("Invalid Source Org in Association");
  }
  o=(Organization)a.getTargetObject();
  if (o.getName() == null || o.getName().getValue() == null)   throw new Exception("Target OrgName in association is null");
  ;
  Concept atype=a.getAssociationType();
  if (atype.getName() == null || atype.getName().getValue() == null)   throw new Exception("Concept stored in Association");
}
