{
  boolean domainFailed=coordinatorResult.isDefined() && coordinatorResult.has(FAILURE_DESCRIPTION);
  if (domainFailed) {
    return true;
  }
  for (  ModelNode hostResult : hostControllerResults.values()) {
    if (hostResult.has(FAILURE_DESCRIPTION)) {
      return true;
    }
  }
  return false;
}
