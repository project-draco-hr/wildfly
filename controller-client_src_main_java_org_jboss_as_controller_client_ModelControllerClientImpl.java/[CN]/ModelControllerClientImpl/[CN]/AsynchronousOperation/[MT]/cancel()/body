{
  try {
    int i=asynchronousId.get().intValue();
    if (i >= 0) {
      return new CancelAsynchronousOperationRequest(i).executeForResult(getConnectionStrategy());
    }
 else     return false;
  }
 catch (  IOException e) {
    throw e;
  }
catch (  Exception e) {
    throw new RuntimeException("Could not cancel request ",e);
  }
}
