{
  CLIWrapper cli=null;
  try {
    cli=new CLIWrapper(connect);
    String command="patch info";
    logger.info("----- sending command to CLI: " + command + " -----");
    cli.sendLine(command);
    String output=cli.readOutput();
    return ModelNode.fromJSONString(output);
  }
  finally {
    if (cli != null) {
      cli.quit();
    }
  }
}
