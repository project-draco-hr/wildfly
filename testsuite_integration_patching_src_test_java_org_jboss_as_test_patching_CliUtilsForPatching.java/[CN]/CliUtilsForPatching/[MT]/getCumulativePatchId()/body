{
  CLIWrapper cli=null;
  try {
    cli=new CLIWrapper(true);
    cli.sendLine("patch info");
    String response=cli.readOutput();
    ModelNode responseNode=ModelNode.fromJSONString(response);
    return responseNode.get("result").get("cumulative-patch-id").asString();
  }
  finally {
    if (cli != null) {
      cli.quit();
    }
  }
}
