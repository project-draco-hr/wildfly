{
  Set set=(Set)ctermLeftJoinPaths.get(pathStr);
  if (set == null) {
    set=new HashSet();
    ctermLeftJoinPaths.put(pathStr,set);
  }
  set.add(path);
  set=(Set)allLeftJoinPaths.get(pathStr);
  if (set == null) {
    set=new HashSet();
    allLeftJoinPaths.put(pathStr,set);
  }
  set.add(path);
}
