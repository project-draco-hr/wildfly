{
  KernelServices kernelServices=createKernelServicesBuilder(TestModelType.STANDALONE).setXmlResource("standalone.xml").createContentRepositoryContent("12345678901234567890").build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),"standalone.xml"),marshalled);
}
