{
  if (this == other)   return true;
  if (getClass() != other.getClass())   return false;
  LocalManagedConnectionFactory otherMcf=(LocalManagedConnectionFactory)other;
  return this.connectionURL.equals(otherMcf.connectionURL) && this.driver.getClass().equals(otherMcf.driver.getClass()) && ((this.userName == null) ? otherMcf.userName == null : this.userName.equals(otherMcf.userName))&& ((this.password == null) ? otherMcf.password == null : this.password.equals(otherMcf.password))&& this.transactionIsolation == otherMcf.transactionIsolation;
}
