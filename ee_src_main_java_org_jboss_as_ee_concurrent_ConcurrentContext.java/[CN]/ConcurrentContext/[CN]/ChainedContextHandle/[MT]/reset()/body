{
  if (resetHandles != null) {
    for (    ContextHandle handle : resetHandles) {
      try {
        handle.reset();
      }
 catch (      Throwable e) {
        EeLogger.ROOT_LOGGER.debug("failed to reset handle",e);
      }
    }
    resetHandles=null;
    ConcurrentContext.popCurrent();
  }
}
