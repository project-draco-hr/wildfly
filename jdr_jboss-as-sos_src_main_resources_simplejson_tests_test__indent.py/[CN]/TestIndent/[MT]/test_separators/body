def test_separators(self):
    lst = [1, 2, 3, 4]
    expect = '[\n1,\n2,\n3,\n4\n]'
    expect_spaces = '[\n1, \n2, \n3, \n4\n]'
    self.assertEquals(expect_spaces, json.dumps(lst, indent=0, separators=(', ', ': ')))
    self.assertEquals(expect, json.dumps(lst, indent=0, separators=(',', ': ')))
    self.assertEquals(expect, json.dumps(lst, indent=0))
