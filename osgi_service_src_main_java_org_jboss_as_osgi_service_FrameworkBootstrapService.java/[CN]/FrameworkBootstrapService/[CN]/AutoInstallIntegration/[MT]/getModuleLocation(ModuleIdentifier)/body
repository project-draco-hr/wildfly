{
  String location="module:" + identifier.getName();
  if ("main".equals(identifier.getSlot()) == false)   location+=":" + identifier.getSlot();
  File repoFile=getModuleRepositoryEntry(identifier);
  if (repoFile == null)   throw new IllegalArgumentException("Cannot obtain repository entry for: " + identifier);
  return repoFile.toURI().toURL();
}
