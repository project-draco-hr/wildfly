{
  super(pathElement,JMXExtension.getResourceDescriptionResolver(CommonAttributes.EXPOSE_MODEL + "." + pathElement.getValue()),new ShowModelAdd(auditLoggerInfo,authorizer,domainName,otherAttributes),new ShowModelRemove(auditLoggerInfo,authorizer));
  this.auditLoggerInfo=auditLoggerInfo;
  this.authorizer=authorizer;
  this.domainName=domainName;
}
