{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  final ViewDescription that=(ViewDescription)o;
  if (componentDescription != that.componentDescription)   return false;
  if (viewClassName != null ? !viewClassName.equals(that.viewClassName) : that.viewClassName != null)   return false;
  return true;
}
