{
  logger.debug("ValueDefImpl[" + id + "].base_value() entered.");
  if (base_value_ref == null)   logger.debug("ValueDefImpl[" + id + "].base_value() returning NULL.");
 else   logger.debug("ValueDefImpl[" + id + "].base_value() returning \""+ base_value_ref.id()+ "\".");
  return base_value_ref;
}
