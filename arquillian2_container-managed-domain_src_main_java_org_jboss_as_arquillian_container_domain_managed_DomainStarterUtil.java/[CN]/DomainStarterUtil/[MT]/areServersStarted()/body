{
  try {
    Map<ServerIdentity,ServerStatus> statuses=domainClient.getServerStatuses();
    for (    ServerStatus status : statuses.values()) {
switch (status) {
case STOPPING:
case STOPPED:
        return false;
default :
      continue;
  }
}
serverStatuses.putAll(statuses);
return true;
}
 catch (Exception ignored) {
}
return false;
}
