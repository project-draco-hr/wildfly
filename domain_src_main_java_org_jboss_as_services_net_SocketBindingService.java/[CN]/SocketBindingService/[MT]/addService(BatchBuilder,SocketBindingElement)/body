{
  SocketBindingService service=new SocketBindingService(element);
  BatchServiceBuilder<SocketBinding> batch=builder.addService(SocketBinding.JBOSS_BINDING_NAME.append(element.getName()),service);
  batch.addDependency(NetworkInterfaceService.JBOSS_NETWORK_INTERFACE.append(element.getInterfaceName()),NetworkInterfaceBinding.class,service.getInterfaceBinding());
  batch.addDependency(SocketBindingManager.SOCKET_BINDING_MANAGER,SocketBindingManager.class,service.getSocketBindings());
  batch.setInitialMode(Mode.ON_DEMAND);
}
