{
  try {
    ByteArrayOutputStream bout=new ByteArrayOutputStream();
    if (attachmentInput != null) {
      int i=attachmentInput.read();
      while (i != -1) {
        bout.write(i);
        i=attachmentInput.read();
      }
    }
    bytes=bout.toByteArray();
  }
 catch (  IOException e) {
    throw new RequestProcessingException(e);
  }
 finally {
  }
}
