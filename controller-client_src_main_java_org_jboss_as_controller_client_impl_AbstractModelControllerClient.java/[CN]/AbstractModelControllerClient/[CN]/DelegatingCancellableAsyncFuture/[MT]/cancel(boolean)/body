{
  if (!activeRequests.containsKey(batchId)) {
    return false;
  }
  try {
    new CancelAsyncRequest().executeForResult(executor,getClientChannelStrategy());
    if (isDone()) {
      isCancelled=false;
    }
    isCancelled=true;
    return isCancelled;
  }
 catch (  Exception e) {
    e.printStackTrace();
    return false;
  }
}
