{
  this.orb=orb;
  try {
    InitialContext context=new InitialContext();
    jsseSecurityDomain=(JSSESecurityDomain)context.lookup(SecurityConstants.JAAS_CONTEXT_ROOT + securityDomain + "/jsse");
    JdkORBLogger.ROOT_LOGGER.debugJSSEDomainRetrieval(securityDomain);
  }
 catch (  NamingException ne) {
    JdkORBLogger.ROOT_LOGGER.failedToObtainJSSEDomain(securityDomain);
  }
  if (jsseSecurityDomain == null)   throw new RuntimeException(JdkORBMessages.MESSAGES.failedToLookupJSSEDomain());
}
