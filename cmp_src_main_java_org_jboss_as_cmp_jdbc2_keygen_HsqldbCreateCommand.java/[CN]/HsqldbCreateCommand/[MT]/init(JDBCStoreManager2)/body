{
  super.init(manager);
  JDBCEntityCommandMetaData metadata=entityBridge.getMetaData().getEntityCommand();
  pkSql=metadata.getAttribute("pk-sql");
  if (pkSql == null) {
    pkSql="CALL IDENTITY()";
  }
  if (log.isDebugEnabled()) {
    log.debug("entity-command generate pk sql: " + pkSql);
  }
}
