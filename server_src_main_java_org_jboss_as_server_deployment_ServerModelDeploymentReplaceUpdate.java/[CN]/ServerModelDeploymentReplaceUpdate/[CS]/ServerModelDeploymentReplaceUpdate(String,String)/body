{
  super(false,true);
  if (newDeployment == null)   throw new IllegalArgumentException("newDeployment is null");
  if (toReplace == null)   throw new IllegalArgumentException("toReplace is null");
  this.newDeployment=newDeployment;
  this.toReplace=toReplace;
  this.newDeploymentRuntimeName=null;
  this.newDeploymentHash=null;
}
