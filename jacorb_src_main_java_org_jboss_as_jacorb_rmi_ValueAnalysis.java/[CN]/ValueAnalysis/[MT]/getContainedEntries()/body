{
  final ArrayList ret=new ArrayList(constants.length + attributes.length + members.length);
  for (int i=0; i < constants.length; ++i)   ret.add(constants[i]);
  for (int i=0; i < attributes.length; ++i)   ret.add(attributes[i]);
  for (int i=0; i < members.length; ++i)   ret.add(members[i]);
  return ret;
}
