{
  return (classType.equals(methodArgumentType)) || (classType.equals("java.lang.Byte") && methodArgumentType.equals("byte")) || (classType.equals("java.lang.Short") && methodArgumentType.equals("short"))|| (classType.equals("java.lang.Integer") && methodArgumentType.equals("int"))|| (classType.equals("java.lang.Long") && methodArgumentType.equals("long"))|| (classType.equals("java.lang.Float") && methodArgumentType.equals("float"))|| (classType.equals("java.lang.Double") && methodArgumentType.equals("double"))|| (classType.equals("java.lang.Boolean") && methodArgumentType.equals("boolean"))|| (classType.equals("java.lang.Character") && methodArgumentType.equals("char"));
}
