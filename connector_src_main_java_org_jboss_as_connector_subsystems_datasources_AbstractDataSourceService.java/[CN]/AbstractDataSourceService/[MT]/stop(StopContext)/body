{
  ExecutorService executorService=executor.getValue();
  Runnable r=new Runnable(){
    @Override public void run(){
synchronized (this) {
        try {
          stopService();
        }
  finally {
          stopContext.complete();
        }
      }
    }
  }
;
synchronized (r) {
    executorService.execute(r);
    stopContext.asynchronous();
  }
}
