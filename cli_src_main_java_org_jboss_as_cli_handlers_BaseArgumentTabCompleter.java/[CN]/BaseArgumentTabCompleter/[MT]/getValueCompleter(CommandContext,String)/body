{
  if (argName != null) {
    for (    CommandArgument arg : getAllArguments(ctx)) {
      if (argName.equals(arg.getFullName())) {
        return arg.getValueCompleter();
      }
    }
    return null;
  }
  for (  CommandArgument arg : getAllArguments(ctx)) {
    try {
      if (arg.getIndex() >= 0 && arg.canAppearNext(ctx)) {
        return arg.getValueCompleter();
      }
    }
 catch (    CommandFormatException e) {
      return null;
    }
  }
  return null;
}
