{
  super(DeploymentOverlayModel.CONTENT_PATH,CommonDescriptions.getResourceDescriptionResolver(ModelDescriptionConstants.DEPLOYMENT_OVERLAY + "." + ModelDescriptionConstants.CONTENT,false),new ContentAdd(contentRepository,remoteRepository),ContentRemove.INSTANCE);
  readContent=new SimpleOperationDefinition(ModelDescriptionConstants.READ_CONTENT,getResourceDescriptionResolver());
  this.contentRepository=contentRepository;
}
