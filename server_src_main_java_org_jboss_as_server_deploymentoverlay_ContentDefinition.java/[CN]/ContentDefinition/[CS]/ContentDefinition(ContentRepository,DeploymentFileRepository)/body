{
  super(DeploymentOverlayModel.CONTENT_PATH,ControllerResolver.getResolver(ModelDescriptionConstants.DEPLOYMENT_OVERLAY,ModelDescriptionConstants.CONTENT),new ContentAdd(contentRepository,remoteRepository),ContentRemove.INSTANCE);
  this.contentRepository=contentRepository;
  readContent=new SimpleOperationDefinition(READ_CONTENT,getResourceDescriptionResolver());
}
