{
  SFSB1 sfsb1=lookup(SFSB1.class);
  Assert.assertFalse(SFSB1.preDestroy);
  sfsb1.doStuff();
  Thread.sleep(2000);
  try {
    sfsb1.doStuff();
    throw new RuntimeException("Expecting NoSuchEjbException");
  }
 catch (  NoSuchEJBException expected) {
  }
  Assert.assertTrue(SFSB1.preDestroy);
}
