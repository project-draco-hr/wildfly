{
  try {
    ctx.getPrefix().toNode("type1","name12");
    assertEquals(Arrays.asList("type2="),fetchCandidates("./"));
    assertEquals(Arrays.asList("type1="),fetchCandidates("/"));
    assertEquals(Arrays.asList("name11","name12"),fetchCandidates("/type1=n"));
  }
  finally {
    ctx.getPrefix().reset();
  }
}
