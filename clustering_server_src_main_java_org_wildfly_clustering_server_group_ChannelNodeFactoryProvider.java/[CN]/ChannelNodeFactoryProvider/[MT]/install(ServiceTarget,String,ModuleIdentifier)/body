{
  InjectedValue<Channel> channel=new InjectedValue<>();
  ServiceController<?> controller=target.addService(getServiceName(cluster),new ChannelNodeFactoryService(channel)).addDependency(GlobalComponentRegistryService.getServiceName(cluster)).addDependency(ChannelService.getServiceName(cluster),Channel.class,channel).setInitialMode(ServiceController.Mode.ON_DEMAND).install();
  return Collections.<ServiceController<?>>singleton(controller);
}
