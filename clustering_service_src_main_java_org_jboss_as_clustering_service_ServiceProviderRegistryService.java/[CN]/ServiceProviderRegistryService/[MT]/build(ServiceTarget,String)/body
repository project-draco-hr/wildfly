{
  new CoreGroupCommunicationService(SCOPE_ID).build(target,container).setInitialMode(ServiceController.Mode.ON_DEMAND).install();
  return target.addService(getServiceName(container),this).addDependency(CacheService.getServiceName(container,null),Cache.class,this.cacheRef).addDependency(CoreGroupCommunicationService.getServiceName(container),GroupMembershipNotifier.class,this.notifierRef);
}
