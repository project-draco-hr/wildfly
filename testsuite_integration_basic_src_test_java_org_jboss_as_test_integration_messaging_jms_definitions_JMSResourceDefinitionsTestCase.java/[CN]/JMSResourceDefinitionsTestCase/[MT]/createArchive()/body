  JavaArchive archive=ShrinkWrap.create(JavaArchive.class,"JMSResourceDefinitionsTestCase.jar").addPackage(MessagingDefiner.class.getPackage()).addAsManifestResource(MessagingDefiner.class.getPackage(),"ejb-jar.xml","ejb-jar.xml").addAsManifestResource(EmptyAsset.INSTANCE,create("beans.xml"));
