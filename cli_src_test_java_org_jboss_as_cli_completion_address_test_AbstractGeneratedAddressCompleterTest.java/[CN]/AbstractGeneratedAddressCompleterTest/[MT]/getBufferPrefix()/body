{
  int bufferLevel=getBufferLevel();
  if (bufferLevel < 2) {
    return "./";
  }
  StringBuilder sb=new StringBuilder("./");
  for (int i=getPrefixLevel() + 1; i < getPrefixLevel() + bufferLevel; ++i) {
    sb.append("link").append(i);
    if (i % 2 == 0) {
      sb.append('/');
    }
 else {
      sb.append('=');
    }
  }
  return sb.toString();
}
