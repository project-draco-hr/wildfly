{
  if (current != null) {
    return current;
  }
  final URL url=getClass().getClassLoader().getResource("META-INF/org.jboss.as.managementConnectionProps");
  if (url != null) {
    InputStream in=null;
    String managementPort;
    String address;
    try {
      in=url.openStream();
      ObjectInputStream inputStream=new ObjectInputStream(in);
      managementPort=(String)inputStream.readObject();
      address=(String)inputStream.readObject();
      if (address == null) {
        address="127.0.0.1";
      }
      if (managementPort == null) {
        managementPort="9999";
      }
      int realPort=Integer.parseInt(managementPort);
      ModelControllerClient modelControllerClient=ModelControllerClient.Factory.create(address,realPort,getCallbackHandler());
      current=new ManagementClient(modelControllerClient,address,realPort);
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
catch (    ClassNotFoundException e) {
      throw new RuntimeException(e);
    }
 finally {
      if (in != null) {
        try {
          in.close();
        }
 catch (        IOException e) {
        }
      }
    }
  }
  return current;
}
