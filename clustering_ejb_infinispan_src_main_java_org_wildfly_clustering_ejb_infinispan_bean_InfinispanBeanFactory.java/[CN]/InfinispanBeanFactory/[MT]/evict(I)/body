{
  BeanKey<I> key=this.createKey(id);
  BeanEntry<G> entry=this.cache.get(key);
  if (entry != null) {
    try {
      this.cache.evict(key);
    }
 catch (    Throwable e) {
      InfinispanEjbLogger.ROOT_LOGGER.failedToPassivateBean(e,id);
    }
    this.groupFactory.evict(entry.getGroupId());
  }
}
