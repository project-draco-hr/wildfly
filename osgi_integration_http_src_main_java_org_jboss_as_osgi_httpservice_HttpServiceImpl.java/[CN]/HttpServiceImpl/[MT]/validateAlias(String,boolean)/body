{
  if (alias == null || !alias.startsWith("/"))   return OSGiMessages.MESSAGES.invalidServletAlias(alias);
  if (alias.length() > 1 && alias.endsWith("/"))   return OSGiMessages.MESSAGES.invalidServletAlias(alias);
  if (exists && !registry.exists(alias))   return OSGiMessages.MESSAGES.aliasMappingDoesNotExist(alias);
  if (!exists && registry.exists(alias))   return OSGiMessages.MESSAGES.aliasMappingAlreadyExists(alias);
  return null;
}
