{
  FileReader fr=null;
  try {
    fr=new FileReader(file);
    char lastChar=0x00;
    char[] temp=new char[1024];
    int read=-1;
    while ((read=fr.read(temp)) > 0) {
      lastChar=temp[read - 1];
    }
    return lastChar != NEW_LINE_CHAR && lastChar != CARRIAGE_RETURN_CHAR;
  }
  finally {
    safeClose(fr);
  }
}
