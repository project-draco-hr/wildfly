{
  FileReader fr=null;
  BufferedReader br=null;
  String temp=null;
  boolean existingUser=false;
  try {
    fr=new FileReader(current);
    br=new BufferedReader(fr);
    while ((temp=br.readLine()) != null) {
      temp=temp.trim();
      if (temp.startsWith(COMMENT)) {
        continue;
      }
 else       if ((temp.split("="))[0].equals(values.userName)) {
        existingUser=true;
        break;
      }
    }
    return existingUser;
  }
  finally {
    safeClose(br);
    safeClose(fr);
  }
}
