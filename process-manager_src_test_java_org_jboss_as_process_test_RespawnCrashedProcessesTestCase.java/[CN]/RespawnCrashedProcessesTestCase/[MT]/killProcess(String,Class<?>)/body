{
  Object id=getProcessId(processName,clazz);
  String osName=System.getProperty("os.name");
  Process proc;
  if (osName.contains("Windows"))   proc=Runtime.getRuntime().exec("taskkill /pid " + id);
 else   proc=Runtime.getRuntime().exec("kill -9 " + id);
  assertEquals(0,proc.waitFor());
  return;
}
