{
switch (getElement()) {
case ANY:
{
      for (      InterfaceCriteria criteria : interfaceCriteria.values()) {
        if (criteria.isAcceptable(networkInterface,address))         return true;
      }
      return false;
    }
case NOT:
{
    for (    InterfaceCriteria criteria : interfaceCriteria.values()) {
      if (criteria.isAcceptable(networkInterface,address))       return false;
    }
    return true;
  }
default :
{
  throw new IllegalStateException(getElement().getLocalName() + " is not a valid simple criteria type");
}
}
}
