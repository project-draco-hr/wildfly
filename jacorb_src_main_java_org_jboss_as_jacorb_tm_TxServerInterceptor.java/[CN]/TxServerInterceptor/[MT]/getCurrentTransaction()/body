{
  Transaction tx=null;
  if (piCurrent != null) {
    try {
      final Any any=piCurrent.get_slot(slotId);
      if (any.type().kind().value() != TCKind._tk_null) {
        tx=ForeignTransaction.instance;
      }
    }
 catch (    InvalidSlot e) {
      throw new RuntimeException("Exception getting slot in TxServerInterceptor: " + e);
    }
  }
  return tx;
}
