{
  Transaction tx=null;
  if (piCurrent != null) {
    try {
      Any any=piCurrent.get_slot(slotId);
      if (any.type().kind().value() != TCKind._tk_null) {
        tx=ForeignTransaction.INSTANCE;
      }
    }
 catch (    InvalidSlot e) {
      throw JacORBMessages.MESSAGES.errorGettingSlotInTxInterceptor(e);
    }
  }
  return tx;
}
