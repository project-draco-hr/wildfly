{
  if (traceEnabled)   log.trace("Intercepting receive_request_service_contexts, " + "operation: " + ri.operation());
  try {
    ServiceContext sc=ri.get_request_service_context(txContextId);
    Any any=codec.decode_value(sc.context_data,PropagationContextHelper.type());
    ri.set_slot(slotId,any);
  }
 catch (  BAD_PARAM e) {
  }
catch (  FormatMismatch e) {
    throw new RuntimeException("Exception decoding context data in " + "TxServerInterceptor: " + e);
  }
catch (  TypeMismatch e) {
    throw new RuntimeException("Exception decoding context data in " + "TxServerInterceptor: " + e);
  }
catch (  InvalidSlot e) {
    throw new RuntimeException("Exception setting slot in " + "TxServerInterceptor: " + e);
  }
}
