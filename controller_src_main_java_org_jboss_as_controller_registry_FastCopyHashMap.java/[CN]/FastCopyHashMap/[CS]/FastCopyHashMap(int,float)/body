{
  if (initialCapacity < 0)   throw ControllerLogger.ROOT_LOGGER.invalidTableSize();
  if (initialCapacity > MAXIMUM_CAPACITY)   initialCapacity=MAXIMUM_CAPACITY;
  if (!(loadFactor > 0F && loadFactor <= 1F))   throw ControllerLogger.ROOT_LOGGER.invalidLoadFactor();
  this.loadFactor=loadFactor;
  init(initialCapacity,loadFactor);
}
