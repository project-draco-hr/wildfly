{
  return new TransformerEntry(){
    @Override public PathTransformation getPathTransformation(){
      return pathTransformation;
    }
    @Override public ResourceTransformer getResourceTransformer(){
      return resourceTransformer.getTransformer();
    }
    @Override public OperationTransformer getOperationTransformer(){
      return defaultTransformer.getTransformer();
    }
  }
;
}
