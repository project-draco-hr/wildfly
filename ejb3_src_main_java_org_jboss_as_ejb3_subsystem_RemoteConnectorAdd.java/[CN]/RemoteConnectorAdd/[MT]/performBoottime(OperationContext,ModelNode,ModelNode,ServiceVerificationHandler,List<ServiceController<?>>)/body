{
  final String connector=model.require(CONNECTOR).asString();
  final ServiceTarget serviceTarget=context.getServiceTarget();
  final RemoteConnectorService service=new RemoteConnectorService();
  newControllers.add(serviceTarget.addService(RemoteConnectorService.SERVICE_NAME,service).addDependency(RemotingServices.ENDPOINT,Endpoint.class,service.getEndpointInjector()).setInitialMode(ServiceController.Mode.ACTIVE).install());
}
