{
  try {
    if (networkInterface.isLoopback()) {
      InetAddress resolved=getAddress();
      return resolved;
    }
  }
 catch (  UnknownHostException e) {
    if (!unknownHostLogged) {
      SERVER_LOGGER.cannotResolveAddress(this.address);
      unknownHostLogged=true;
    }
  }
  return null;
}
