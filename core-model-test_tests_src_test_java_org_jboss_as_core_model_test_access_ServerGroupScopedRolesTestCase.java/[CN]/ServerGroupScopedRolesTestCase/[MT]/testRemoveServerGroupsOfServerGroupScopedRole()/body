{
  testAddServerGroupScopedRole();
  ModelNode operation=Util.createOperation(WRITE_ATTRIBUTE_OPERATION,pathAddress(pathElement(CORE_SERVICE,MANAGEMENT),pathElement(ACCESS,AUTHORIZATION),pathElement(SERVER_GROUP_SCOPED_ROLE,FOO)));
  operation.get(NAME).set(SERVER_GROUPS);
  ModelNode result=execute(operation);
  assertEquals(FAILED,result.get(OUTCOME).asString());
}
