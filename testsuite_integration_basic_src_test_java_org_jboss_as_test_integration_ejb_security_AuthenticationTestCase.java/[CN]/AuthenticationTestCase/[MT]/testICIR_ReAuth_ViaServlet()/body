{
  String result=HttpRequest.get(managementClient.getWebUri() + "/ejb3security/whoAmI?method=doIHaveRole&role=Users&username=user2&password=password2","user1","password1",10,SECONDS);
  assertEquals("true",result);
  result=HttpRequest.get(managementClient.getWebUri() + "/ejb3security/whoAmI?method=doIHaveRole&role=Role1&username=user2&password=password2","user1","password1",10,SECONDS);
  assertEquals("false",result);
  result=HttpRequest.get(managementClient.getWebUri() + "/ejb3security/whoAmI?method=doIHaveRole&role=Role2&username=user2&password=password2","user1","password1",10,SECONDS);
  assertEquals("true",result);
}
