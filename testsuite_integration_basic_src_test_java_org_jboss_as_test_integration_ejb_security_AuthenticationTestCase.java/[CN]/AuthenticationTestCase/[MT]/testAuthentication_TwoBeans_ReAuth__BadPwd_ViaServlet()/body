{
  try {
    HttpRequest.get("http://" + managementClient.getMgmtAddress() + ":8080/ejb3security/whoAmI?method=doubleWhoAmI&username=user2&password=bad_password","user1","password1",10,SECONDS);
    fail("Expected IOException");
  }
 catch (  IOException e) {
    assertTrue(e.getMessage().contains("javax.ejb.EJBAccessException"));
  }
}
