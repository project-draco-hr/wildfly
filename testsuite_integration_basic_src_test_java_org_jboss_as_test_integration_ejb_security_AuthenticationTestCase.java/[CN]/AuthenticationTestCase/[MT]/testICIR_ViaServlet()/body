{
  String result=HttpRequest.get(managementClient.getWebUri() + "/ejb3security/whoAmI?method=doIHaveRole&role=Users","user1","password1",10,SECONDS);
  assertEquals("true",result);
  result=HttpRequest.get(managementClient.getWebUri() + "/ejb3security/whoAmI?method=doIHaveRole&role=Role1","user1","password1",10,SECONDS);
  assertEquals("true",result);
  result=HttpRequest.get(managementClient.getWebUri() + "/ejb3security/whoAmI?method=doIHaveRole&role=Role2","user1","password1",10,SECONDS);
  assertEquals("false",result);
}
