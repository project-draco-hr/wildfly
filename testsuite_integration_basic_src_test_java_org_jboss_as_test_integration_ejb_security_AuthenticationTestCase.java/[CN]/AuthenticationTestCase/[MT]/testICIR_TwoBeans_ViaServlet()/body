{
  String result=HttpRequest.get("http://" + managementClient.getMgmtAddress() + ":8080/ejb3security/whoAmI?method=doubleDoIHaveRole&role=Users","user1","password1",10,SECONDS);
  assertEquals("true,true",result);
  result=HttpRequest.get("http://" + managementClient.getMgmtAddress() + ":8080/ejb3security/whoAmI?method=doubleDoIHaveRole&role=Role1","user1","password1",10,SECONDS);
  assertEquals("true,true",result);
  result=HttpRequest.get("http://" + managementClient.getMgmtAddress() + ":8080/ejb3security/whoAmI?method=doubleDoIHaveRole&role=Role2","user1","password1",10,SECONDS);
  assertEquals("false,false",result);
}
