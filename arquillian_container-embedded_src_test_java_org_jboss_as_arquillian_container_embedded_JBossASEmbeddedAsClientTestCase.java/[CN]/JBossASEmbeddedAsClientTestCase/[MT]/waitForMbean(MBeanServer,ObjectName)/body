{
  long end=System.currentTimeMillis() + 3000;
  do {
    try {
      MBeanInfo info=mbeanServer.getMBeanInfo(name);
      if (info != null) {
        return;
      }
    }
 catch (    Exception e) {
    }
    System.out.println("Not there yet");
    Thread.sleep(100);
  }
 while (System.currentTimeMillis() < end);
}
