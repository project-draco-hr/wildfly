{
  return target.addService(this.getServiceName(),new ValueService<>(new ImmediateValue<DeploymentPolicy>(this))).addDependency(SingletonServiceName.BUILDER.getServiceName(this.containerName,this.cacheName),SingletonServiceBuilderFactory.class,this.factory).addDependency(new ElectionPolicyServiceNameProvider(this.getName()).getServiceName(),SingletonElectionPolicy.class,this.policy).setInitialMode(ServiceController.Mode.ON_DEMAND);
}
