{
  State nextState=update(stateValues);
  if (nextState == null && stateValues.isInteractive() && stateValues.getPassword() != null) {
    nextState=new ConfirmationChoice(theConsole,DomainManagementLogger.ROOT_LOGGER.serverUser(),DomainManagementLogger.ROOT_LOGGER.yesNo(),new DisplaySecret(theConsole,stateValues),null);
  }
  return nextState;
}
