{
  ModelNode endpointConfig=model.get(RemotingEndpointResource.ENDPOINT_PATH.getKey(),RemotingEndpointResource.ENDPOINT_PATH.getValue());
  if (endpointConfig.isDefined()) {
    boolean write=false;
    for (    Property prop : endpointConfig.asPropertyList()) {
      if (prop.getValue().isDefined()) {
        write=true;
        break;
      }
    }
    if (write) {
      RemotingSubsystem20Parser.ENDPOINT_PARSER.persist(writer,model);
    }
  }
}
