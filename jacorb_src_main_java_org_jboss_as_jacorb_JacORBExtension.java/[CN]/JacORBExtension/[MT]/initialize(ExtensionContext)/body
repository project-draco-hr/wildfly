{
  final SubsystemRegistration subsystem=context.registerSubsystem(SUBSYSTEM_NAME);
  final ManagementResourceRegistration registration=subsystem.registerSubsystemModel(JacORBSubsystemDescriptions.SUBSYSTEM);
  registration.registerOperationHandler(ADD,JacORBSubsystemAdd.INSTANCE,JacORBSubsystemDescriptions.SUBSYSTEM_ADD,false);
  registration.registerOperationHandler(DESCRIBE,JacORBSubsystemDescribe.INSTANCE,JacORBSubsystemDescriptions.SUBSYSTEM_DESCRIBE,false,OperationEntry.EntryType.PRIVATE);
  subsystem.registerXMLElementWriter(PARSER);
}
