{
  if (transaction == null)   return initialValue();
  Object value=getValue(transaction);
  if (value == null) {
    value=initialValue();
    if (value == null) {
      value=NULL_VALUE;
    }
    try {
      storeValue(transaction,value);
    }
 catch (    IllegalStateException e) {
      return initialValue();
    }
  }
  if (value == NULL_VALUE) {
    return null;
  }
  return value;
}
