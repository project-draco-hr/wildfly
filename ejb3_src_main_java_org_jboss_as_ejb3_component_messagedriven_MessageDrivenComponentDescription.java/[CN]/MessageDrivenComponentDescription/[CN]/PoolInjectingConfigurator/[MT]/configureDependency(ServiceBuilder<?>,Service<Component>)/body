{
  final MessageDrivenComponentCreateService mdbComponentCreateService=(MessageDrivenComponentCreateService)service;
  final String poolName=this.mdbComponentDescription.getPoolConfigName();
  if (poolName == null) {
    serviceBuilder.addDependency(ServiceBuilder.DependencyType.OPTIONAL,PoolConfigService.DEFAULT_MDB_POOL_CONFIG_SERVICE_NAME,PoolConfig.class,mdbComponentCreateService.getPoolConfigInjector());
  }
 else {
    serviceBuilder.addDependency(PoolConfigService.EJB_POOL_CONFIG_BASE_SERVICE_NAME.append(poolName),PoolConfig.class,mdbComponentCreateService.getPoolConfigInjector());
  }
}
