{
synchronized (this) {
    if (!start)     return;
    StringBuilder sb=new StringBuilder();
    sb.append(Command.RECONNECT_SERVER_MANAGER);
    sb.append('\0');
    sb.append(addr);
    sb.append('\0');
    sb.append(port);
    sb.append('\n');
    StreamUtils.writeString(commandStream,sb.toString());
    commandStream.flush();
  }
}
