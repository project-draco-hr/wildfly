{
  OutputStream curr=null;
synchronized (this) {
    curr=current;
    if (bytesOut != null) {
      if (bufferedBytes == null)       bufferedBytes=new ArrayList<byte[]>();
      bufferedBytes.add(bytesOut.toByteArray());
      bytesOut=null;
      current=null;
    }
  }
  if (curr != null)   curr.flush();
}
