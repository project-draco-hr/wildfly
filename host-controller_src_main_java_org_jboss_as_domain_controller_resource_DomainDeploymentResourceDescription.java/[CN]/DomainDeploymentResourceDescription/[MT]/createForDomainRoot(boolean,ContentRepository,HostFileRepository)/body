{
  return new DomainDeploymentResourceDescription(DeploymentResourceParent.DOMAIN,DeploymentAttributes.DOMAIN_DEPLOYMENT_ADD_DEFINITION,isMaster ? new DeploymentAddHandler(contentRepository) : new DeploymentAddHandler(),isMaster ? DeploymentRemoveHandler.createForMaster(contentRepository) : DeploymentRemoveHandler.createForSlave(fileRepository));
}
