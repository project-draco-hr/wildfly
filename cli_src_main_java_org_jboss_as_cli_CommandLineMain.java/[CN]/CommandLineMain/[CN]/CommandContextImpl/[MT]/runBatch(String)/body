{
  final List<String> batch;
  if (!batchMode) {
    if (batches != null) {
      batch=batches.remove(name);
    }
 else {
      batch=null;
    }
  }
 else {
    batch=currentBatch;
  }
  if (batch == null) {
    return false;
  }
  try {
  }
  finally {
    currentBatch=null;
    batchMode=false;
  }
  return true;
}
