{
  if (console != null) {
    try {
      console.printString(message);
      console.printNewline();
    }
 catch (    IOException e) {
      System.err.println("Failed to print '" + message + "' to the console: "+ e.getLocalizedMessage());
    }
  }
 else {
    System.out.println(message);
  }
}
