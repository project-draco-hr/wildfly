{
  if (outputTarget != null) {
    try {
      for (      String item : col) {
        outputTarget.append(item);
        outputTarget.newLine();
      }
    }
 catch (    IOException e) {
      System.err.println("Failed to print columns '" + col + "' to the console: "+ e.getLocalizedMessage());
    }
    return;
  }
  if (console != null) {
    try {
      console.printColumns(col);
    }
 catch (    IOException e) {
      System.err.println("Failed to print columns '" + col + "' to the console: "+ e.getLocalizedMessage());
    }
  }
 else {
    for (    String item : col) {
      System.out.println(item);
    }
  }
}
