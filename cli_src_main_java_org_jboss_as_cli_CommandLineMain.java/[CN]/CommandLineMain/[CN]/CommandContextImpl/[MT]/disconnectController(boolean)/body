{
  if (this.client != null) {
    StreamUtils.safeClose(client);
    if (loggingEnabled) {
      printLine("Closed connection to " + this.controllerHost + ':'+ this.controllerPort);
    }
    client=null;
    this.controllerHost=null;
    this.controllerPort=-1;
    domainMode=false;
  }
  promptConnectPart=null;
}
