{
  final ServiceName channelServiceName=ChannelService.getServiceName(containerName);
  final InjectedValue<ChannelFactory> channelFactory=new InjectedValue<ChannelFactory>();
  final Service<Channel> service=new ChannelService(cluster,channelFactory);
  return target.addService(channelServiceName,new AsynchronousService<Channel>(service)).addDependency(ChannelFactoryService.getServiceName(stack),ChannelFactory.class,channelFactory).addDependency(EmbeddedCacheManagerService.getServiceName(containerName)).setInitialMode(ServiceController.Mode.ON_DEMAND).install();
}
