{
  final InjectedValue<CoreGroupCommunicationService> service=new InjectedValue<CoreGroupCommunicationService>();
  return target.addService(SharedLocalYieldingClusterLockManagerService.getServiceName(containerName),new SharedLocalYieldingClusterLockManagerService(containerName,service)).addDependency(CoreGroupCommunicationService.getServiceName(containerName),CoreGroupCommunicationService.class,service).setInitialMode(ServiceController.Mode.ON_DEMAND).install();
}
