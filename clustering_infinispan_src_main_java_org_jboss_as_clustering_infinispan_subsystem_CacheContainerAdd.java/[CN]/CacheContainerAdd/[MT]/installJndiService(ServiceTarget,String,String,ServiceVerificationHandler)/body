{
  final ServiceName containerServiceName=EmbeddedCacheManagerService.getServiceName(containerName);
  final ContextNames.BindInfo binding=this.createCacheContainerBinding(jndiName,containerName);
  final BinderService binder=new BinderService(binding.getBindName());
  return target.addService(binding.getBinderServiceName(),binder).addAliases(ContextNames.JAVA_CONTEXT_SERVICE_NAME.append(binding.getBindName())).addDependency(containerServiceName,CacheContainer.class,new ManagedReferenceInjector<CacheContainer>(binder.getManagedObjectInjector())).addDependency(binding.getParentContextServiceName(),ServiceBasedNamingStore.class,binder.getNamingStoreInjector()).setInitialMode(ServiceController.Mode.PASSIVE).install();
}
