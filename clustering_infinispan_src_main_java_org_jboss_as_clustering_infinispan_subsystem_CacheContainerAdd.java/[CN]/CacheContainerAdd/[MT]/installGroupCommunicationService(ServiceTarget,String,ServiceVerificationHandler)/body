{
  final InjectedValue<Channel> channel=new InjectedValue<Channel>();
  final InjectedValue<ModuleLoader> loader=new InjectedValue<ModuleLoader>();
  return target.addService(CoreGroupCommunicationService.getServiceName(containerName),new CoreGroupCommunicationService(GROUP_COMMUNICATION_SERVICE_SCOPE,channel,loader)).addDependency(CacheService.getServiceName(containerName,null)).addDependency(ChannelService.getServiceName(containerName),Channel.class,channel).addDependency(Services.JBOSS_SERVICE_MODULE_LOADER,ModuleLoader.class,loader).setInitialMode(ServiceController.Mode.ON_DEMAND).install();
}
