{
  if (target == null)   throw new IllegalArgumentException("Null target");
  if (moduleID == null)   throw new IllegalArgumentException("Null moduleID");
  if (moduleType == null)   throw new IllegalArgumentException("Null moduleType");
  this.target=target;
  this.moduleID=moduleID;
  this.parentModuleID=parentModuleID;
  this.moduleType=moduleType;
}
