{
  modelComplete.set(true);
  if (modelUpdated) {
    final ModelNode model=modelSource.getModel();
synchronized (model) {
      model.set(localModel);
    }
    BasicModelController.this.persistConfiguration(model,injectedConfigPersisterProvider);
  }
  if (unfinishedCount.get() == 0) {
    handleSuccess();
  }
}
