{
  this(toCopy.getName(),toCopy.getXmlName(),toCopy.getDefaultValue(),toCopy.getType(),toCopy.isAllowNull(),toCopy.isAllowExpression(),toCopy.getMeasurementUnit(),toCopy.getCorrector(),wrapValidator(toCopy.getValidator(),toCopy.isAllowNull(),toCopy.isValidateNull(),toCopy.isAllowExpression(),toCopy.getType()),toCopy.isValidateNull(),toCopy.getAlternatives(),toCopy.getRequires(),toCopy.getAttributeMarshaller(),toCopy.isResourceOnly(),toCopy.getDeprecated(),wrapConstraints(toCopy.getAccessConstraints()),toCopy.getNullSignficant(),toCopy.getParser(),wrapFlags(toCopy.getFlags()));
}
