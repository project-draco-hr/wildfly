{
  if (callerPropagation == null)   throw new IllegalArgumentException("Null callerPropagation");
  if (CALLER_PROPAGATION_NONE.equalsIgnoreCase(callerPropagation))   this.callerPropagation=CALLER_PROPAGATION_NONE;
 else   if (CALLER_PROPAGATION_SUPPORTED.equalsIgnoreCase(callerPropagation))   this.callerPropagation=CALLER_PROPAGATION_SUPPORTED;
 else   throw new IllegalArgumentException("Unknown sascontext callerPropagtion: " + callerPropagation);
}
