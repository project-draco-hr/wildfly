{
  boolean isShutdown=shutdown.getAndSet(true);
  if (isShutdown)   return;
  serverSocketListener.shutdown();
synchronized (processes) {
    for (    ManagedProcess proc : processes.values()) {
      if (proc.isStart()) {
        try {
          proc.stop();
        }
 catch (        IOException e) {
        }
      }
    }
    processes.clear();
  }
}
