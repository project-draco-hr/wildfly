{
  if (shutdown.get()) {
    return;
  }
synchronized (processes) {
    try {
      ManagedProcess serverManagerProcess=processes.get(SERVER_MANAGER_PROCESS_NAME);
      if (serverManagerProcess != null)       serverManagerProcess.down(serverName);
    }
 catch (    IOException e) {
      log.error("Problem notifying ServerManager of down process " + serverName);
    }
  }
}
