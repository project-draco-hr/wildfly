{
  final Map<String,ManagedProcess> processes=this.processes;
synchronized (processes) {
    for (    ManagedProcess process : processes.values()) {
synchronized (process) {
        if (!process.isStart()) {
          continue;
        }
        try {
          process.send(sender,msg,chksum);
        }
 catch (        IOException e) {
        }
      }
    }
  }
}
