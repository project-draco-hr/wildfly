{
  Set<MBeanAttributeInfo> attributes=super.getAttributeInfos();
  attributes.add(createRoMBeanAttributeInfo(ATTR_DEPLOYMENT_DESCRIPTOR,String.class.getName(),"The deployment descriptor"));
  attributes.add(createRoMBeanAttributeInfo(ATTR_SERVER,String.class.getName(),"The server object name descriptor"));
  return attributes;
}
