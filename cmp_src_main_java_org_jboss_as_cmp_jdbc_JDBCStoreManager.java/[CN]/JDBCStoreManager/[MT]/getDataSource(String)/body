{
  final Value<DataSource> value=dataSources.get(name);
  if (value == null) {
    throw CmpMessages.MESSAGES.canNotFindDataSource(name);
  }
  final DataSource dataSource=value.getValue();
  if (dataSource == null) {
    throw CmpMessages.MESSAGES.canNotFindDataSource(name);
  }
  return dataSource;
}
