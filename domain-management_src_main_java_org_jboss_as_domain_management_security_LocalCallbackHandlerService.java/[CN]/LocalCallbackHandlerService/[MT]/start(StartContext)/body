{
  if (defaultUser != null) {
    allowedUsersSet.add(defaultUser);
  }
  if (allowedUsers != null) {
    if ("*".equals(allowedUsers)) {
      allowAll=true;
    }
 else {
      String[] users=allowedUsers.split(",");
      for (      String current : users) {
        allowedUsersSet.add(current);
      }
    }
  }
}
