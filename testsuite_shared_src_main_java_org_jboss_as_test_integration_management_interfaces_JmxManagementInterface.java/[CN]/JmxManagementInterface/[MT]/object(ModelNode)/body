{
switch (node.getType()) {
case BIG_DECIMAL:
    return node.asBigDecimal();
case BIG_INTEGER:
  return node.asBigInteger();
case BOOLEAN:
return node.asBoolean();
case BYTES:
return node.asBytes();
case DOUBLE:
return node.asDouble();
case EXPRESSION:
return node.asExpression();
case INT:
return node.asInt();
case LIST:
return node.asList();
case LONG:
return node.asLong();
case PROPERTY:
return node.asProperty();
case STRING:
return node.asString();
case UNDEFINED:
return null;
default :
throw new UnsupportedOperationException("Can't convert '" + node.getType() + "' to object");
}
}
