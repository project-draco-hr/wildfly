{
  final BatchEnvironment batchEnvironment=new WildFlyBatchEnvironment(classLoaderInjector.getValue(),beanManagerInjector.getOptionalValue(),executorServiceInjector.getValue(),userTransactionInjector.getOptionalValue(),propertiesInjector.getValue());
  BatchEnvironmentFactory.getInstance().add(classLoaderInjector.getValue(),batchEnvironment);
  this.batchEnvironment=batchEnvironment;
}
