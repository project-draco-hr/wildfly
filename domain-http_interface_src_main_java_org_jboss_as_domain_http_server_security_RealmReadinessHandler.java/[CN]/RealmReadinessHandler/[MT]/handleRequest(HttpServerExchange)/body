{
  if (securityRealm == null || securityRealm.isReady() || clientCertPotentiallyPossible(exchange)) {
    next.handleRequest(exchange);
  }
 else {
    try {
      rejectRequest(exchange);
    }
 catch (    IOException e) {
      ROOT_LOGGER.error(e);
      exchange.setResponseCode(500);
      exchange.endExchange();
    }
  }
}
