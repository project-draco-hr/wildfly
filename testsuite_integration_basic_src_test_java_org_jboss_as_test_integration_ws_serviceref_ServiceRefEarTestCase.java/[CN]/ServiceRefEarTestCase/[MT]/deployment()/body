{
  JavaArchive jar=ShrinkWrap.create(JavaArchive.class,"ws-serviceref-example.jar").addClasses(EJB3Bean.class,EndpointInterface.class);
  log.info(jar.toString(true));
  WebArchive war=ShrinkWrap.create(WebArchive.class,"ws-serviceref-example-servlet-client.war").addClasses(EndpointInterface.class,EndpointService.class,ServletClient.class).addAsWebInfResource("ws/serviceref/web.xml","web.xml").addAsWebInfResource("ws/serviceref/jboss-web.xml","jboss-web.xml").addAsWebInfResource("ws/serviceref/wsdl/TestService.wsdl","wsdl/TestService.wsdl");
  log.info(war.toString(true));
  EnterpriseArchive ear=ShrinkWrap.create(EnterpriseArchive.class,"ws-serviceref-example.ear").addAsModule(jar).addAsModule(war);
  log.info(ear.toString(true));
  return ear;
}
