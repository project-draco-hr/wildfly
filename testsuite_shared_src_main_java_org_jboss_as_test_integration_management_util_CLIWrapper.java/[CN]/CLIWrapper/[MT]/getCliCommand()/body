{
  if (cliCommand != null) {
    return cliCommand;
  }
  String asDist=System.getProperty("jboss.dist");
  String asInst=System.getProperty("jboss.inst");
  String javaExec=System.getProperty("java.home") + File.separatorChar + "bin"+ File.separatorChar+ "java";
  if (javaExec.contains(" ")) {
    javaExec="\"" + javaExec + "\"";
  }
  cliCommand=javaExec + " -Djboss.home.dir=" + asInst+ " -Djboss.modules.dir="+ asDist+ "/modules"+ " -Djline.WindowsTerminal.directConsole=false"+ " -jar "+ asDist+ "/jboss-modules.jar"+ " -mp "+ asDist+ "/modules"+ " org.jboss.as.cli"+ " --user="+ username+ " --password="+ password;
  return cliCommand;
}
