{
  consoleOut.reset();
  if (ignoreError) {
    ctx.handleSafe(line);
    return ctx.getExitCode() == 0;
  }
 else {
    try {
      ctx.handle(line);
    }
 catch (    CommandLineException e) {
      Assert.fail("Failed to execute line '" + line + "': "+ e.getLocalizedMessage());
    }
  }
  return true;
}
