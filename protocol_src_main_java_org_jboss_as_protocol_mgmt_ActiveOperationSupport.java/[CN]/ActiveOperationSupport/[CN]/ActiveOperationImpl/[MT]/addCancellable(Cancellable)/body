{
synchronized (this) {
switch (getStatus()) {
case CANCELLED:
      break;
case WAITING:
    final List<Cancellable> cancellables=this.cancellables;
  if (cancellables == CANCEL_REQUESTED) {
    break;
  }
 else {
    ((cancellables == null) ? (this.cancellables=new ArrayList<Cancellable>()) : cancellables).add(cancellable);
  }
default :
return;
}
}
cancellable.cancel();
}
