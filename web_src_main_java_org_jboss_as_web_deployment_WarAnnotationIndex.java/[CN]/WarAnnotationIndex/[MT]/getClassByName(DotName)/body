{
  ClassInfo info=null;
  if (rootIndex != null) {
    info=rootIndex.getClassByName(className);
  }
  if (info == null) {
    for (    final Index index : indexes.values()) {
      info=index.getClassByName(className);
      if (info != null) {
        return info;
      }
    }
  }
  return info;
}
