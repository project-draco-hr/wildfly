{
  try {
    if (log.isTraceEnabled()) {
      log.trace("Before completion callback invoked on Transaction synchronization: " + this + " of stateful component instance: "+ statefulSessionComponentInstance);
    }
synchronized (threadLock) {
      statefulSessionComponentInstance.beforeCompletion();
    }
  }
 catch (  Throwable t) {
    throw handleThrowableInTxSync(statefulSessionComponentInstance,t);
  }
}
