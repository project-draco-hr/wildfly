{
  try {
    if (ROOT_LOGGER.isTraceEnabled()) {
      ROOT_LOGGER.trace("Before completion callback invoked on Transaction synchronization: " + this + " of stateful component instance: "+ statefulSessionComponentInstance);
    }
    if (!statefulSessionComponentInstance.isDiscarded()) {
      statefulSessionComponentInstance.beforeCompletion();
    }
  }
 catch (  Throwable t) {
    throw handleThrowableInTxSync(statefulSessionComponentInstance,t);
  }
}
