{
  final List<ResourceRoot> entries=new ArrayList<ResourceRoot>();
  final ResourceRoot webInfClassesRoot=new ResourceRoot(deploymentRoot.getChild(WEB_INF_CLASSES).getName(),deploymentRoot.getChild(WEB_INF_CLASSES),null);
  ResourceRootTypeMarker.setType(ResourceRootType.MODULE_ROOT,webInfClassesRoot);
  entries.add(webInfClassesRoot);
  createWebInfLibResources(deploymentRoot,entries);
  return entries;
}
