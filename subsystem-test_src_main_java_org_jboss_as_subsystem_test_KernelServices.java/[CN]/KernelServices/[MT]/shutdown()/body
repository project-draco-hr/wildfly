{
  if (container != null) {
    container.shutdown();
    try {
      container.awaitTermination(5,TimeUnit.SECONDS);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
    container=null;
    for (    KernelServices legacyService : legacyServices.values()) {
      legacyService.shutdown();
    }
  }
}
