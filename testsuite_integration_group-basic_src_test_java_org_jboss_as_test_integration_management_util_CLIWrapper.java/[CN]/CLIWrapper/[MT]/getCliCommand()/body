{
  if (cliCommand != null)   return cliCommand;
  String asDist=System.getProperty("jboss.dist");
  String asInst=System.getProperty("jboss.inst");
  cliCommand="java -Djboss.home.dir=" + asInst + " -Djboss.modules.dir="+ asDist+ "/modules"+ " -Djline.WindowsTerminal.directConsole=false"+ " -jar "+ asDist+ "/jboss-modules.jar"+ " -mp "+ asDist+ "/modules"+ " -logmodule org.jboss.logmanager org.jboss.as.cli"+ " --user="+ USERNAME+ " --password="+ PASSWORD;
  return cliCommand;
}
