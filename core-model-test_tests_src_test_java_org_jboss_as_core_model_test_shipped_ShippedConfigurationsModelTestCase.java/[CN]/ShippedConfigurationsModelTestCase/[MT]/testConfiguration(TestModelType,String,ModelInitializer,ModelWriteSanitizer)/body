{
  KernelServices kernelServices=createKernelServicesBuilder(type).setXmlResource(xmlResource).validateDescription().setModelInitializer(initializer,sanitizer).build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),xmlResource),marshalled);
}
