{
  parentBuilder.addChildResource(PATH).getAttributeBuilder().setDiscard(new DiscardAttributeChecker.DiscardAttributeValueChecker(new ModelNode(false)),Constants.CONNECTABLE).setDiscard(DiscardAttributeChecker.UNDEFINED,TRACKING,VALIDATE_ON_MATCH).addRejectCheck(RejectAttributeChecker.DEFINED,Constants.CONNECTABLE,Constants.TRACKING,VALIDATE_ON_MATCH);
}
