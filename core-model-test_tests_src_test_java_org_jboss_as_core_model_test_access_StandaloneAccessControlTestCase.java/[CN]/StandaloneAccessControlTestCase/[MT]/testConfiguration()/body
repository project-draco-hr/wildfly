{
  KernelServices kernelServices=createKernelServicesBuilder(TestModelType.STANDALONE).setXmlResource("constraints.xml").validateDescription().build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),"constraints.xml"),marshalled);
}
