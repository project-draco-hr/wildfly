{
  parent.addChildResource(StorePropertyResource.STORE_PROPERTY_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,StorePropertyResource.VALUE).end();
  parent.addChildResource(StoreWriteBehindResource.STORE_WRITE_BEHIND_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,StoreWriteBehindResource.FLUSH_LOCK_TIMEOUT,StoreWriteBehindResource.MODIFICATION_QUEUE_SIZE,StoreWriteBehindResource.SHUTDOWN_TIMEOUT,StoreWriteBehindResource.THREAD_POOL_SIZE).end();
}
