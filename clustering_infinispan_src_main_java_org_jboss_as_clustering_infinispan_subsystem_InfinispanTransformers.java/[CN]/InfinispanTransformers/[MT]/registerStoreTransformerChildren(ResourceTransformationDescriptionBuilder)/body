{
  parent.addChildResource(StorePropertyResourceDefinition.STORE_PROPERTY_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,StorePropertyResourceDefinition.VALUE).end();
  parent.addChildResource(StoreWriteBehindResourceDefinition.STORE_WRITE_BEHIND_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,StoreWriteBehindResourceDefinition.FLUSH_LOCK_TIMEOUT,StoreWriteBehindResourceDefinition.MODIFICATION_QUEUE_SIZE,StoreWriteBehindResourceDefinition.SHUTDOWN_TIMEOUT,StoreWriteBehindResourceDefinition.THREAD_POOL_SIZE).end();
}
