{
  try {
    dsAddress=createDataSource(false,jndiDs);
    dsXaAddress=createDataSource(true,jndiXaDs);
    StringBuffer sb=cleanStats(dsAddress).append(cleanStats(dsXaAddress));
    if (sb.length() > 0)     fail(sb.toString());
  }
 catch (  Throwable e) {
    removeDss();
    throw new Exception(e);
  }
}
