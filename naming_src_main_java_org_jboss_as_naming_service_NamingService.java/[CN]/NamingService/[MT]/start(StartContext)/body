{
  log.info("Starting Naming Service ");
  try {
    if (supportEvents)     namingStore=new InMemoryNamingStore(new NamingEventCoordinator());
 else     namingStore=new InMemoryNamingStore();
    NamingContext.setActiveNamingStore(namingStore);
  }
 catch (  Throwable t) {
    throw new StartException("Failed to start naming server",t);
  }
}
