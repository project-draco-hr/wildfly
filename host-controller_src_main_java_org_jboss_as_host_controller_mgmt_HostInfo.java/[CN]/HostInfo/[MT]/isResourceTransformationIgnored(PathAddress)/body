{
  if (ignoredResources != null && address.size() > 0) {
    PathElement firstElement=address.getElement(0);
    IgnoredType ignoredType=ignoredResources.get(firstElement.getKey());
    if (ignoredType != null) {
      if (ignoredType.hasName(firstElement.getValue())) {
        return true;
      }
    }
  }
  return false;
}
