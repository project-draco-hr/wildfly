{
  ROOT_LOGGER.debugf("Activating Wise Extension");
  final SubsystemRegistration subsystem=context.registerSubsystem(SUBSYSTEM_NAME,VERSION);
  final ManagementResourceRegistration registration=subsystem.registerSubsystemModel(WISE_SUBSYSTEM_RESOURCE);
  registration.registerOperationHandler(GenericSubsystemDescribeHandler.DEFINITION,GenericSubsystemDescribeHandler.INSTANCE,false);
  subsystem.registerXMLElementWriter(parser);
}
