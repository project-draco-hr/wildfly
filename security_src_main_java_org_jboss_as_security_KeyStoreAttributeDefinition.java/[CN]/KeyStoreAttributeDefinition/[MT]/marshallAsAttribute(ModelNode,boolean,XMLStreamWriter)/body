{
  if (isMarshallable(resourceModel,marshallDefault)) {
    resourceModel=resourceModel.get(getName());
    if (resourceModel.hasDefined(Constants.PASSWORD))     writer.writeAttribute(getName() + "-" + Constants.PASSWORD,resourceModel.get(Constants.PASSWORD).asString());
    if (resourceModel.hasDefined(Constants.TYPE))     writer.writeAttribute(getName() + "-" + Constants.TYPE,resourceModel.get(Constants.TYPE).asString());
    if (resourceModel.hasDefined(Constants.URL))     writer.writeAttribute(getName() + "-" + Constants.URL,resourceModel.get(Constants.URL).asString());
    if (resourceModel.hasDefined(Constants.PROVIDER))     writer.writeAttribute(getName() + "-" + Constants.PROVIDER,resourceModel.get(Constants.PROVIDER).asString());
    if (resourceModel.hasDefined(Constants.PROVIDER_ARGUMENT))     writer.writeAttribute(getName() + "-" + Constants.PROVIDER_ARGUMENT,resourceModel.get(Constants.PROVIDER_ARGUMENT).asString());
  }
}
