{
  theConsole.printf(NEW_LINE);
  theConsole.printf(" * ");
  theConsole.printf(MESSAGES.errorHeader());
  theConsole.printf(" * ");
  theConsole.printf(NEW_LINE);
  theConsole.printf(errorMessage);
  theConsole.printf(NEW_LINE);
  theConsole.printf(NEW_LINE);
  if ((stateValues != null && !stateValues.isInteractive()) && nextState == null) {
    throw new RuntimeException(errorMessage);
  }
  return nextState;
}
