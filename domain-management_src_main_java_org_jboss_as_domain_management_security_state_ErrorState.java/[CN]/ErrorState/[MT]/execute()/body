{
  if ((stateValues == null) || (stateValues != null) && (stateValues.isSilent() == false)) {
    theConsole.printf(NEW_LINE);
    theConsole.printf(" * ");
    theConsole.printf(MESSAGES.errorHeader());
    theConsole.printf(" * ");
    theConsole.printf(NEW_LINE);
    theConsole.printf(errorMessage);
    theConsole.printf(NEW_LINE);
    theConsole.printf(NEW_LINE);
  }
  return nextState;
}
