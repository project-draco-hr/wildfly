{
  String request="<find_business generic='2.0' xmlns='urn:uddi-org:api_v2'>" + "<name xml:lang='en'>" + bizname + "</name></find_business>";
  String response=null;
  try {
    response=rs.makeRegistrySpecificRequest(request);
  }
 catch (  Exception e) {
    Assert.fail(e.getLocalizedMessage());
  }
  if (response == null || "".equals(response))   Assert.fail("Find Business failed");
}
