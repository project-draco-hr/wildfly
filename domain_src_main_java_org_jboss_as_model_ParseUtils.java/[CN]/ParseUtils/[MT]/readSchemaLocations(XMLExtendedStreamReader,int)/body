{
  final List<String> values=reader.getListAttributeValue(idx);
  if ((values.size() & 1) != 0) {
    throw invalidAttributeValue(reader,idx);
  }
  final Iterator<String> it=values.iterator();
  final List<SchemaLocation> locationList=new ArrayList<SchemaLocation>();
  while (it.hasNext()) {
    locationList.add(new SchemaLocation(it.next(),it.next()));
  }
  return locationList;
}
