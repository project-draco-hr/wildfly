{
  DeploymentUtils utils=null;
  try {
    utils=new DeploymentUtils();
    utils.addWarDeployment("ws-example.war",true,EndpointImpl.class.getPackage());
    utils.deploy();
    testWSDL();
    testSOAPCall();
    testWebServiceRef();
  }
  finally {
    utils.undeploy();
    safeClose(utils);
  }
}
