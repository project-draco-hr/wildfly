{
  boolean parent=false;
  for (  Pattern pattern : parentFirst) {
    if (pattern.matcher(className).matches()) {
      parent=true;
      break;
    }
  }
  if (parent) {
    for (    Pattern pattern : childFirst) {
      if (pattern.matcher(className).matches()) {
        return false;
      }
    }
  }
  return parent;
}
