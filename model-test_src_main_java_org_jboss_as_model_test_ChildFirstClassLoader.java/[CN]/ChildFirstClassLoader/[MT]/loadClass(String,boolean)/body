{
  if (loadFromParentOnly(name)) {
    return parent.loadClass(name);
  }
  Class<?> c=findLoadedClass(name);
  if (c == null) {
    try {
      c=findClass(name);
    }
 catch (    ClassNotFoundException e) {
      if (parentExclusionFilter != null && parentExclusionFilter.accept(name)) {
        throw e;
      }
    }
    if (c == null) {
      c=parent.loadClass(name);
    }
    if (c == null) {
      findClass(name);
    }
  }
  if (resolve) {
    resolveClass(c);
  }
  return c;
}
