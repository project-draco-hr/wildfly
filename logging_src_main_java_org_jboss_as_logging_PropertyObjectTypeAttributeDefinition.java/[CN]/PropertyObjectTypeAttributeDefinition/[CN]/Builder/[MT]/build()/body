{
  if (xmlName == null)   xmlName=name;
  ParameterValidator validator=this.validator;
  if (validator == null) {
    validator=new ObjectTypeValidator(allowNull,valueTypes);
  }
  return new PropertyObjectTypeAttributeDefinition(name,xmlName,propertyName,suffix,valueTypes,allowNull,resolver,validator,corrector,alternatives,requires,attributeMarshaller,resourceOnly,deprecated,accessConstraints,nullSignficant,flags);
}
