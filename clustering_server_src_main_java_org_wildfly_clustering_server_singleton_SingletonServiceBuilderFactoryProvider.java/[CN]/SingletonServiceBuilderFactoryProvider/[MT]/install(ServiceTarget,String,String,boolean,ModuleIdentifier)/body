{
  ServiceBuilder<?> builder=target.addService(getServiceName(containerName,cacheName),new SingletonServiceBuilderFactoryService(containerName,cacheName)).setInitialMode(ServiceController.Mode.ACTIVE);
  if (defaultCache) {
    builder.addAliases(getServiceName(containerName,CacheContainer.DEFAULT_CACHE_ALIAS));
  }
  return Collections.<ServiceController<?>>singleton(builder.install());
}
