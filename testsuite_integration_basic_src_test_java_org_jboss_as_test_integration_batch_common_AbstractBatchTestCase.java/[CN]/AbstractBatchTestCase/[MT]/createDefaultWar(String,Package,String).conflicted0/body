{
  return ShrinkWrap.create(WebArchive.class,warName).addPackage(AbstractBatchTestCase.class.getPackage()).addClasses(TimeoutUtil.class).addAsWebInfResource(EmptyAsset.INSTANCE,"beans.xml").addAsWebInfResource(pkg,jobXml,"classes/META-INF/batch-jobs/" + jobXml).setManifest(new StringAsset(Descriptors.create(ManifestDescriptor.class).attribute("Dependencies","org.jboss.msc,org.wildfly.security.manager").exportAsString()));
}
