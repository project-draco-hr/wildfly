{
  final ResourceTransformationDescriptionBuilder child=subsystemBuilder.addChildResource(CONSOLE_HANDLER_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,AUTOFLUSH,TARGET).end().discardOperations();
  loggingProfileBuilder.rejectChildResource(CONSOLE_HANDLER_PATH);
  return registerTransformers(child);
}
