{
  rewritecondition.registerReadWriteAttribute(PATTERN,null,new WriteAttributeHandlers.AttributeDefinitionValidatingHandler(PATTERN));
  rewritecondition.registerReadWriteAttribute(TEST,null,new WriteAttributeHandlers.AttributeDefinitionValidatingHandler(TEST));
  rewritecondition.registerReadWriteAttribute(FLAGS,null,new WriteAttributeHandlers.AttributeDefinitionValidatingHandler(FLAGS));
}
