{
  NamingContext.initializeNamingManager();
  ServiceContainer container=ServiceContainer.Factory.create("naming-test");
  container.addService(testServiceName,new Service<String>(){
    @Override public void start(    StartContext context) throws StartException {
    }
    @Override public void stop(    StopContext context){
    }
    @Override public String getValue() throws IllegalStateException, IllegalArgumentException {
      return "hello world";
    }
  }
).install();
  ObjectFactoryBuilder.INSTANCE.setServiceRegistry(container);
}
