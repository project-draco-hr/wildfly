{
  if (jgroupsSupport != null) {
    jgroupsSupport.restoreProperties();
  }
  for (  DistributableSessionManager<?> manager : managers) {
    if (manager != null) {
      try {
        manager.stop();
      }
 catch (      Exception e) {
        log.warn(e.getMessage(),e);
      }
    }
  }
  for (  CacheContainer cacheContainer : cacheContainers) {
    if (cacheContainer != null) {
      try {
        cacheContainer.stop();
      }
 catch (      Exception e) {
        log.warn(e.getMessage(),e);
      }
    }
  }
}
