{
  try {
    latch.countDown();
    System.err.println("*Test - Server listening on " + server.getLocalPort());
    while (true) {
      ListenerSocketThread t=new ListenerSocketThread(this,server.accept());
      t.start();
synchronized (this) {
        listenerThreads.add(t);
      }
    }
  }
 catch (  SocketException e) {
    System.err.println("*Test - " + this.getName() + " server socket closed");
  }
catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    closeServer();
  }
}
