{
  if (previous == null) {
    if (!hasPrevious(ctx)) {
      return null;
    }
    try {
      previous=new State(col,(RollbackPatch)historyDir.getRollbackXml(ctx).getPatch(ctx),ctx);
    }
 catch (    IOException e) {
      ctx.getErrorHandler().error("Failed to load previous patch",e);
      return null;
    }
  }
  return previous;
}
