{
  if (name == null) {
    addArgument(value);
  }
 else {
    putProperty(name,value);
  }
  lastPropName=name;
  lastPropValue=value;
  separator=SEPARATOR_NONE;
  if (nameValueSeparatorIndex >= 0) {
    this.lastSeparatorIndex=nameValueSeparatorIndex;
  }
  lastChunkIndex=nameValueSeparatorIndex;
}
