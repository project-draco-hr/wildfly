{
  try {
    final DomainControllerEnvironment config=determineEnvironment(args,stdin,stdout,stderr);
    if (config == null) {
      abort(null);
    }
 else {
      final DomainController domainController=new DomainController(config);
      domainController.start();
    }
  }
 catch (  Throwable t) {
    abort(t);
  }
}
