{
  if (xmlName == null)   xmlName=name;
  if (propertyName == null)   propertyName=name;
  return new PropertyAttributeDefinition(name,xmlName,propertyName,resolver,defaultValue,type,allowNull,allowExpression,measurementUnit,corrector,validator,validateNull,alternatives,requires,attributeMarshaller,resourceOnly,deprecated,accessConstraints,flags);
}
