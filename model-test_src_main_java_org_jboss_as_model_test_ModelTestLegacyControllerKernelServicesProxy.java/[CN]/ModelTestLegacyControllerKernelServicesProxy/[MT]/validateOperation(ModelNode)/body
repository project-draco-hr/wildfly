{
  try {
    if (validateOperation == null) {
      validateOperation=childFirstClassLoaderServices.getClass().getMethod("validateOperation",childFirstClassLoader.loadClass(operation.getClass().getName()));
    }
    validateOperation.invoke(childFirstClassLoaderServices,convertModelNodeToChildCl(operation));
  }
 catch (  Exception e) {
    unwrapInvocationTargetRuntimeException(e);
    throw new RuntimeException(e);
  }
}
