{
  Batch batch=this.manager.getBatcher().startBatch();
  try {
    Bean<UUID,K,V> bean=this.manager.findBean(id);
    if (bean == null) {
      batch.close();
      return null;
    }
    V result=bean.acquire();
    result.setContext(batch);
    return result;
  }
 catch (  RuntimeException|Error e) {
    batch.discard();
    throw e;
  }
}
