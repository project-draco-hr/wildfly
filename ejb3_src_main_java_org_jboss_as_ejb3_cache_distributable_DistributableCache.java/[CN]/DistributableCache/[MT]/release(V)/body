{
  try (BatchContext context=this.manager.getBatcher().resume(value.getCacheContext())){
    try (Batch batch=value.getCacheContext()){
      Bean<UUID,K,V> bean=this.manager.findBean(value.getId());
      if (bean != null) {
        if (bean.release()) {
          bean.close();
        }
      }
    }
   }
 }
