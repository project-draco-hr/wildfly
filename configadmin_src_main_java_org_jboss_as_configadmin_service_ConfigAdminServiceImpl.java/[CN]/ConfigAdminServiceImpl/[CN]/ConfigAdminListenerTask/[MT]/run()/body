{
  for (  ConfigAdminListener aux : snapshot) {
    Set<String> pids=aux.getPIDs();
    if (pids == null || pids.contains(pid)) {
      try {
        aux.configurationModified(pid,dictionary);
      }
 catch (      Exception ex) {
        LOGGER.configurationListenerError(ex,aux);
      }
    }
  }
}
