{
  if (fieldValue == null) {
    return null;
  }
  Class valueType=fieldValue.getClass();
  Constructor ctor;
  try {
    ctor=valueType.getConstructor(new Class[]{argType});
  }
 catch (  NoSuchMethodException e) {
    throw new IllegalStateException("Failed to find a ctor in " + valueType + " that takes an instance of "+ argType+ " as an argument.");
  }
  try {
    return ctor.newInstance(new Object[]{fieldValue});
  }
 catch (  Exception e) {
    throw new IllegalStateException("Failed to create an instance of " + valueType + " with the "+ fieldValue+ " as a ctor argument");
  }
}
