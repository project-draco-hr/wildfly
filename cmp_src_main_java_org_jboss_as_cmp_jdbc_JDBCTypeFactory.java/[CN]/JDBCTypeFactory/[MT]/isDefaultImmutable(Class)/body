{
  boolean result=false;
  if (clazz.isPrimitive() || clazz == Boolean.class || clazz == Byte.class || clazz == Short.class || clazz == Integer.class || clazz == Long.class || clazz == Float.class || clazz == Double.class || clazz == Character.class || clazz == String.class || clazz == java.math.BigInteger.class || clazz == java.math.BigDecimal.class) {
    result=true;
  }
  return result;
}
