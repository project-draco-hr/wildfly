{
  if (index >= params.length) {
    throw MESSAGES.wrongParamLength(index,params.length);
  }
  Object o=params[index];
  if (o == null) {
    return null;
  }
  try {
    return clazz.cast(o);
  }
 catch (  Exception e) {
    throw MESSAGES.wrongParamType(index,clazz.getName(),o.getClass().getName());
  }
}
