{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setFetchSize(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,rows);
      s.setFetchSize(rows);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
