{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setEscapeProcessing(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,enable);
      s.setEscapeProcessing(enable);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
