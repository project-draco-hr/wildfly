{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setMaxFieldSize(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,max);
      s.setMaxFieldSize(max);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
