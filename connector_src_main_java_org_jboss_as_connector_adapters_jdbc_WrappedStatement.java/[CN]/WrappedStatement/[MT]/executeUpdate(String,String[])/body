{
  lock();
  try {
    checkTransaction();
    try {
      checkConfiguredQueryTimeout();
      if (spy)       spyLogger.debugf("%s [%s] executeUpdate(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,sql,Arrays.toString(columnNames));
      return s.executeUpdate(sql,columnNames);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
