{
  lock();
  try {
    if (spy)     spyLogger.debugf("%s [%s] isClosed()",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT);
    Statement wrapped=getWrappedObject();
    if (wrapped == null)     return true;
    return wrapped.isClosed();
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
 finally {
    unlock();
  }
}
