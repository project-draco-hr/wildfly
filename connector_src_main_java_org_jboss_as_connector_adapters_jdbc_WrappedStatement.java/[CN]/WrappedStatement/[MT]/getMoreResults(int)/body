{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] getMoreResults(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,current);
      return s.getMoreResults(current);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
