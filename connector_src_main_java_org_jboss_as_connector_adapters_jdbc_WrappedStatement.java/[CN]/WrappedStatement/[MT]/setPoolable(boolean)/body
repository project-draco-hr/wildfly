{
  lock();
  try {
    Statement statement=getUnderlyingStatement();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setPoolable(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,poolable);
      statement.setPoolable(poolable);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
