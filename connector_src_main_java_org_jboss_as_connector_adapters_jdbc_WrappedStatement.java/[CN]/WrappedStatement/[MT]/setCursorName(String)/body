{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setCursorName(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,name);
      s.setCursorName(name);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
