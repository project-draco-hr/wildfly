{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] getUpdateCount()",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT);
      return s.getUpdateCount();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
