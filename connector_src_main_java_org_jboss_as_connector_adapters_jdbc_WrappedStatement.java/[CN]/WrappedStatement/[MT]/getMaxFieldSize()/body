{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] getMaxFieldSize()",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT);
      return s.getMaxFieldSize();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
