{
  lock();
  try {
    checkTransaction();
    try {
      checkConfiguredQueryTimeout();
      if (spy)       spyLogger.debugf("%s [%s] executeQuery(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,sql);
      ResultSet result=s.executeQuery(sql);
      return registerResultSet(result);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
