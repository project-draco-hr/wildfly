{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setFetchDirection(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT,direction);
      s.setFetchDirection(direction);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
