{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] getResultSet()",jndiName,Constants.SPY_LOGGER_PREFIX_STATEMENT);
      ResultSet result=s.getResultSet();
      if (result == null)       return null;
 else       return registerResultSet(result);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
