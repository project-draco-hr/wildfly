{
  SimpleHome simpleHome=getSimpleHome();
  Collection c;
  Timestamp timestampBefore=new Timestamp(1111);
  Timestamp timestampAfter=new Timestamp(8888);
  c=simpleHome.selectDynamic("SELECT OBJECT(s) " + "FROM simple s " + "WHERE s.timestampValue IN(?1, ?2, ?3)",new Object[]{timestampBefore,timestampAfter,timestampValue});
  assertTrue(c.size() == 1);
}
