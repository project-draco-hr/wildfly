{
  if (controlPoint == null) {
    delegate.run();
  }
 else   try {
    if (controlPoint.beginRequest() == RunResult.RUN) {
      try {
        delegate.run();
      }
  finally {
        controlPoint.requestComplete();
      }
      return;
    }
 else {
      throw EeLogger.ROOT_LOGGER.cannotRunScheduledTask(delegate);
    }
  }
 catch (  Exception e) {
    EeLogger.ROOT_LOGGER.failedToRunTask(e);
  }
}
