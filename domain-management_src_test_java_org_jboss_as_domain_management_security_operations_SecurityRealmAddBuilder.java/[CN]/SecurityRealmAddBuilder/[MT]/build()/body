{
  assertNotBuilt();
  buildChildren();
  built=true;
  final ModelNode composite=new ModelNode();
  composite.get(OP).set(COMPOSITE);
  composite.get(OP_ADDR).setEmptyList();
  ModelNode steps=composite.get(STEPS);
  steps.add(getAddRealmOp());
  for (  ModelNode step : additionalSteps) {
    steps.add(step);
  }
  return composite;
}
