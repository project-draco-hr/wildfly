{
  TimerImpl timer=nonPersistentTimers.get(handle);
  if (timer != null) {
    return timer;
  }
  timer=this.persistentWaitingOnTxCompletionTimers.get(handle);
  if (timer != null) {
    return timer;
  }
  TimerHandleImpl timerHandle=(TimerHandleImpl)handle;
  return this.getPersistedTimer(timerHandle);
}
