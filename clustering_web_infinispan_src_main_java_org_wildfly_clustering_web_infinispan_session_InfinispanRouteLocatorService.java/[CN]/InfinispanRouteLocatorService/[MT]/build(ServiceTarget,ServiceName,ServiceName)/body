{
  InfinispanRouteLocatorService service=new InfinispanRouteLocatorService();
  return target.addService(name,service).addDependency(NODE_FACTORY_SERVICE_NAME,NodeFactory.class,service.factory).addDependency(REGISTRY_SERVICE_NAME,Registry.class,service.registry).addDependency(getCacheServiceName(deploymentServiceName),Cache.class,service.cache);
}
