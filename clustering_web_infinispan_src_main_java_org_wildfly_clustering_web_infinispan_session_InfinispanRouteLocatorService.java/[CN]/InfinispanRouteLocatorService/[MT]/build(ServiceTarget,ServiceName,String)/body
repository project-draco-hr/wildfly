{
  InfinispanRouteLocatorService service=new InfinispanRouteLocatorService();
  return target.addService(name,service).addDependency(CacheServiceNames.NODE_FACTORY.getServiceName(InfinispanSessionManagerFactoryBuilder.DEFAULT_CACHE_CONTAINER),NodeFactory.class,service.factory).addDependency(CacheServiceNames.REGISTRY.getServiceName(InfinispanSessionManagerFactoryBuilder.DEFAULT_CACHE_CONTAINER),Registry.class,service.registry).addDependency(getCacheServiceAlias(deploymentName),Cache.class,service.cache);
}
