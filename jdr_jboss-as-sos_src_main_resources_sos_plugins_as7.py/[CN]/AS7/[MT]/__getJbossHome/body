def __getJbossHome(self):
    self.__jbossHome = self.get_jboss_home()
    if (not self.__jbossHome):
        self.addAlert('ERROR: The JBoss installation directory was not supplied.              The JBoss SOS plug-in cannot continue.')
        return False
    self.addAlert(('INFO: The JBoss installation directory supplied to SOS is ' + self.__jbossHome))
    return True
