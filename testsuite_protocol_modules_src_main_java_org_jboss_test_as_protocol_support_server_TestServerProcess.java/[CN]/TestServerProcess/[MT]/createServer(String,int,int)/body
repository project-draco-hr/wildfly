{
  String[] args=new String[]{CommandLineConstants.INTERPROCESS_NAME,serverName,CommandLineConstants.INTERPROCESS_PM_ADDRESS,InetAddress.getLocalHost().getHostAddress(),CommandLineConstants.INTERPROCESS_PM_PORT,String.valueOf(pmPort),CommandLineConstants.INTERPROCESS_SM_ADDRESS,InetAddress.getLocalHost().getHostAddress(),CommandLineConstants.INTERPROCESS_SM_PORT,String.valueOf(smPort)};
  return Arrays.asList(args);
}
