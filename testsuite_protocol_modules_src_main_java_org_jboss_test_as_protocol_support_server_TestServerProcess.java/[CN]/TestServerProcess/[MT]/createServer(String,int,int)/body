{
  String[] args=new String[]{CommandLineConstants.INTERPROCESS_NAME,serverName,CommandLineConstants.INTERPROCESS_PC_ADDRESS,InetAddress.getLocalHost().getHostAddress(),CommandLineConstants.INTERPROCESS_PC_PORT,String.valueOf(pmPort),CommandLineConstants.INTERPROCESS_HC_ADDRESS,InetAddress.getLocalHost().getHostAddress(),CommandLineConstants.INTERPROCESS_HC_PORT,String.valueOf(smPort)};
