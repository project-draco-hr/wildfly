{
  final BatchBuilder builder=updateContext.getBatchBuilder();
  builder.addService(JDBCRARDeployService.NAME,new JDBCRARDeployService()).addDependency(ConnectorServices.RESOURCEADAPTERS_SERVICE).addDependency(ConnectorServices.CONNECTOR_CONFIG_SERVICE).addDependency(ConnectorServices.IRONJACAMAR_MDR).setInitialMode(Mode.ON_DEMAND).install();
  builder.addService(ConnectorServices.DATASOURCES_SERVICE,new DataSourcesService(datasources)).setInitialMode(Mode.ACTIVE).install();
}
