{
  super(location);
  if (name == null)   throw new IllegalArgumentException("name is null");
  this.name=name;
  if (interfaceResolver == null)   throw new IllegalArgumentException("interfaceResolver is null");
  this.interfaceResolver=interfaceResolver;
  if (defaultInterface == null)   throw new IllegalArgumentException("defaultInterface is null");
  if (this.interfaceResolver.resolveRef(defaultInterface) == null) {
    throw new IllegalArgumentException("Unknown interface " + defaultInterface);
  }
  this.defaultInterface=defaultInterface;
  if (includedGroupResolver == null)   throw new IllegalArgumentException("includedGroupResolver is null");
  this.includedGroupResolver=includedGroupResolver;
}
