{
  Connection sqlConn=null;
  CallableStatement stmt=null;
  try {
    sqlConn=ds.getConnection();
    log.info("Dropping table");
    stmt=sqlConn.prepareCall("DROP TABLE WebAppTestTable");
    stmt.execute();
  }
 catch (  SQLException e) {
    e.printStackTrace();
  }
 finally {
    safeClose(stmt);
    safeClose(sqlConn);
  }
  try {
    conn.stop();
    conn.close();
  }
 catch (  JMSException ignore) {
  }
}
