{
  super(id,timerService,calendarTimeout.getFirstTimeout() == null ? null : calendarTimeout.getFirstTimeout().getTime(),0,info,persistent);
  this.calendarTimeout=calendarTimeout;
  Calendar nextTimeout=this.calendarTimeout.getNextTimeout();
  if (nextTimeout != null) {
    this.nextExpiration=nextTimeout.getTime();
  }
  if (timeoutMethod != null) {
    this.autoTimer=true;
    this.timeoutMethod=timeoutMethod;
  }
}
