{
  final JavaArchive jar=ShrinkWrap.create(JavaArchive.class,"sfsb.jar").addClass(SFSBLifecycleCallback.class).addClass(IBeanLifecycleCallback.class).addAsResource(GetCallerPrincipalTestCase.class.getPackage(),"users.properties","users.properties").addAsResource(GetCallerPrincipalTestCase.class.getPackage(),"roles.properties","roles.properties").addAsManifestResource(GetCallerPrincipalTestCase.class.getPackage(),"jboss-ejb3.xml","jboss-ejb3.xml").addAsManifestResource(GetCallerPrincipalTestCase.class.getPackage(),"MANIFEST.MF-bean","MANIFEST.MF");
