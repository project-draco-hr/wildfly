{
  final JavaArchive jar=ShrinkWrap.create(JavaArchive.class,"mdb.jar").addClass(MDBLifecycleCallback.class).addAsResource(GetCallerPrincipalTestCase.class.getPackage(),"users.properties","users.properties").addAsResource(GetCallerPrincipalTestCase.class.getPackage(),"roles.properties","roles.properties").addAsManifestResource(GetCallerPrincipalTestCase.class.getPackage(),"MANIFEST.MF-bean","MANIFEST.MF");
  jar.addPackage(CommonCriteria.class.getPackage());
  log.info(jar.toString(true));
  return jar;
}
