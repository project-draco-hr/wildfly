{
switch (bundle.getState()) {
case Bundle.UNINSTALLED:
    return "UNINSTALLED";
case Bundle.INSTALLED:
  return "INSTALLED";
case Bundle.RESOLVED:
return "RESOLVED";
case Bundle.STARTING:
return "STARTING";
case Bundle.STOPPING:
return "STOPPING";
case Bundle.ACTIVE:
return "ACTIVE";
}
return null;
}
