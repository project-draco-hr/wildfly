{
  LocaleEncodingMetaData localeEncoding=new LocaleEncodingMetaData();
  final int count=reader.getAttributeCount();
  for (int i=0; i < count; i++) {
    final String value=reader.getAttributeValue(i);
    if (reader.getAttributeNamespace(i) != null) {
      continue;
    }
    final Attribute attribute=Attribute.forName(reader.getAttributeLocalName(i));
switch (attribute) {
case ID:
{
        localeEncoding.setId(value);
        break;
      }
default :
    throw unexpectedAttribute(reader,i);
}
}
while (reader.hasNext() && reader.nextTag() != END_ELEMENT) {
final Element element=Element.forName(reader.getLocalName());
switch (element) {
case LOCALE:
  localeEncoding.setLocale(reader.getElementText());
break;
case ENCODING:
localeEncoding.setEncoding(reader.getElementText());
break;
default :
throw unexpectedElement(reader);
}
}
return localeEncoding;
}
