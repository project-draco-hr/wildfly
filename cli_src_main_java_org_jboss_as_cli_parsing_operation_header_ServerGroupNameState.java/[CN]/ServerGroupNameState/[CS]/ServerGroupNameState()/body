{
  super(ID);
  setEnterHandler(GlobalCharacterHandlers.CONTENT_CHARACTER_HANDLER);
  setDefaultHandler(GlobalCharacterHandlers.CONTENT_CHARACTER_HANDLER);
  putHandler('(',GlobalCharacterHandlers.LEAVE_STATE_HANDLER);
  putHandler(',',GlobalCharacterHandlers.LEAVE_STATE_HANDLER);
  putHandler('^',GlobalCharacterHandlers.LEAVE_STATE_HANDLER);
  putHandler('}',GlobalCharacterHandlers.LEAVE_STATE_HANDLER);
  putHandler(';',GlobalCharacterHandlers.LEAVE_STATE_HANDLER);
}
