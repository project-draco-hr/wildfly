{
  try {
    Class clz=Class.forName(className,true,cl);
    Object o=clz.newInstance();
    if (configs != null) {
      Injection injector=new Injection();
      for (      ConfigProperty cpmd : configs) {
        if (cpmd.isValueSet())         injector.inject(cpmd.getConfigPropertyType().getValue(),cpmd.getConfigPropertyName().getValue(),cpmd.getConfigPropertyValue().getValue(),o);
      }
    }
    return o;
  }
 catch (  Throwable t) {
    throw new DeployException("Deployment " + className + " failed",t);
  }
}
