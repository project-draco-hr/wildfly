{
  this.executor.shutdown();
  for (  Future<?> future : this.expirationFutures.values()) {
    future.cancel(false);
  }
  for (  Future<?> future : this.expirationFutures.values()) {
    if (!future.isDone()) {
      try {
        future.get();
      }
 catch (      InterruptedException e) {
        Thread.currentThread().interrupt();
      }
catch (      ExecutionException e) {
      }
    }
  }
  this.expirationFutures.clear();
}
