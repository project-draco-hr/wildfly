{
  String containerName=DEFAULT_CACHE_CONTAINER;
  String templateCacheName=CacheServiceNameFactory.DEFAULT_CACHE;
  String cacheName=this.host;
  new TemplateConfigurationBuilder(containerName,cacheName,templateCacheName).build(target).install();
  new CacheBuilder<>(containerName,cacheName).build(target).install();
  return target.addService(this.getServiceName(),new ValueService<>(this)).addDependency(CacheServiceName.CACHE.getServiceName(containerName,cacheName),Cache.class,this.cache).addDependency(CacheContainerServiceName.AFFINITY.getServiceName(containerName),KeyAffinityServiceFactory.class,this.affinityFactory).addDependency(ServiceName.JBOSS.append("as","service-module-loader"),ModuleLoader.class,this.loader);
}
