{
  if (log.isTraceEnabled()) {
    String msg=pool.size() + "/" + maxSize+ " Free instance:"+ this;
    log.trace(msg);
  }
  boolean destroyIt=false;
synchronized (pool) {
    if (pool.size() < maxSize)     pool.add(obj);
 else     destroyIt=true;
  }
  if (destroyIt)   destroy(obj);
  semaphore.release();
  --inUse;
}
