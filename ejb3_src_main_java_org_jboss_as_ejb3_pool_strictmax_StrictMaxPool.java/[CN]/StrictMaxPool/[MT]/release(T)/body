{
  if (ROOT_LOGGER.isTraceEnabled()) {
    ROOT_LOGGER.tracef("%s/%s Free instance: %s",pool.size(),maxSize,this);
  }
  boolean destroyIt=false;
synchronized (pool) {
    if (pool.size() < maxSize)     pool.add(obj);
 else     destroyIt=true;
  }
  if (destroyIt)   destroy(obj);
  semaphore.release();
  --inUse;
}
