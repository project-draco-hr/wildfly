{
  KernelServicesBuilder builder=createKernelServicesBuilder(AdditionalInitialization.MANAGEMENT).setSubsystemXmlResource("subsystem_1_3.xml");
  builder.createLegacyKernelServicesBuilder(createAdditionalInitialization(),VERSION_1_1_0).addMavenResourceURL("org.jboss.as:jboss-as-messaging:7.1.3.Final").addMavenResourceURL("org.hornetq:hornetq-core:2.2.21.Final").addMavenResourceURL("org.hornetq:hornetq-jms:2.2.21.Final").addMavenResourceURL("org.hornetq:hornetq-ra:2.2.21.Final");
  KernelServices mainServices=builder.build();
  assertTrue(mainServices.isSuccessfulBoot());
  assertTrue(mainServices.getLegacyServices(VERSION_1_1_0).isSuccessfulBoot());
}
