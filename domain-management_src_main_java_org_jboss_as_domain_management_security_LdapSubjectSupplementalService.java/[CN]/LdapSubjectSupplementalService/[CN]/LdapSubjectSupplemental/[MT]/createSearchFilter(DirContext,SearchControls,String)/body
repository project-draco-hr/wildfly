{
  String userDN=getDistinguishedName(searchContext,searchControls,new Object[]{username});
  String[] filter=new String[2];
  if (reverseGroupMember) {
    filter[0]=userFilter;
    filter[1]=userDN;
  }
 else {
    filter[0]=advancedFilter != null ? advancedFilter : "(member={0})";
    filter[1]=userDN;
  }
  return filter;
}
