{
  WebArchive a=ShrinkWrap.create(WebArchive.class,DEPLOYMENT_NAME).addPackage(CustomUITestCase.class.getPackage()).addClasses(SimpleLogin.class,JTATestsBase.class).addAsWebInfResources(CustomUITestCase.class.getPackage(),"web.xml","web.xml").addAsWebInfResource(CustomUITestCase.class.getPackage(),"faces-config.xml","faces-config.xml").addAsWebInfResource(CustomUITestCase.class.getPackage(),"custom.taglib.xml","custom.taglib.xml").addAsWebResource(CustomUITestCase.class.getPackage(),"index.xhtml","index.xhtml");
  log.info(a.toString(true));
  return a;
}
