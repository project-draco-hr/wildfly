{
  return new IndexAdapter(){
    @Override public Collection<ClassInfo> getKnownDirectSubclasses(    DotName className){
      return index.getKnownDirectSubclasses(className);
    }
    @Override public List<AnnotationInstance> getAnnotations(    DotName annotationName){
      return index.getAnnotations(annotationName);
    }
  }
;
}
