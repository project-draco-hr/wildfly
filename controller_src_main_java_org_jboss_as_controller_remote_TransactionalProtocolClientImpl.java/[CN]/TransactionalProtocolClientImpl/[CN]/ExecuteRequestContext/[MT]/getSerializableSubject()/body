{
  if (subject != null) {
    Subject toSend=new Subject();
    Set<Principal> principals=toSend.getPrincipals();
    for (    Principal current : subject.getPrincipals()) {
      if (current instanceof Serializable) {
        principals.add(current);
      }
    }
    toSend.setReadOnly();
    return toSend;
  }
  return null;
}
