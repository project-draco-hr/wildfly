{
  output.writeByte(ModelControllerProtocol.PARAM_SUBJECT_LENGTH);
  if (subject != null) {
    output.writeInt(1);
    Marshaller marshaller=MarshallingUtil.getMarshaller();
    marshaller.start(MarshallingUtil.createByteOutput(output));
    marshaller.writeObject(subject);
    marshaller.finish();
  }
 else {
    output.writeInt(0);
  }
}
