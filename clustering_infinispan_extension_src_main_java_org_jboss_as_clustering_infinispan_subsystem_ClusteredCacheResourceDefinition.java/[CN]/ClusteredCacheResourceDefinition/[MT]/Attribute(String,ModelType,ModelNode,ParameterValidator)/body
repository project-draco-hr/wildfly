{
  this.definition=new SimpleAttributeDefinitionBuilder(name,type).setAllowExpression(true).setAllowNull(defaultValue != null).setDefaultValue(defaultValue).setFlags(AttributeAccess.Flag.RESTART_RESOURCE_SERVICES).setMeasurementUnit((type == ModelType.LONG) ? MeasurementUnit.MILLISECONDS : null).setValidator(validator).build();
}
