{
  try {
    if (!closed) {
      final Throwable t=ControllerClientLogger.ROOT_LOGGER.controllerClientNotClosed();
      t.setStackTrace(allocationStackTrace);
      ControllerClientLogger.ROOT_LOGGER.leakedControllerClient(t);
      StreamUtils.safeClose(this);
    }
  }
  finally {
    super.finalize();
  }
}
