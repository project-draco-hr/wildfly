{
  try {
    if (command != null) {
      for (int i=0; i < command.size(); i++) {
        if (command.get(i).equals(CommandLineConstants.INTERPROCESS_SM_ADDRESS))         smAddress=InetAddress.getByName(command.get(++i));
        if (command.get(i).equals(CommandLineConstants.INTERPROCESS_SM_PORT))         smPort=Integer.valueOf(command.get(++i));
      }
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
