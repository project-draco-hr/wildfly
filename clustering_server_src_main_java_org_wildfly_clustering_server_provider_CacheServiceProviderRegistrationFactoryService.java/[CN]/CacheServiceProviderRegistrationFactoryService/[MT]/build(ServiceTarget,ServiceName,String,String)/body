{
  CacheServiceProviderRegistrationFactoryService service=new CacheServiceProviderRegistrationFactoryService(name);
  return new AsynchronousServiceBuilder<>(name,service).build(target).addDependency(CacheService.getServiceName(containerName,cacheName),Cache.class,service.cache).addDependency(CacheServiceNames.GROUP.getServiceName(containerName,cacheName),Group.class,service.group).addDependency(GroupServiceNames.COMMAND_DISPATCHER.getServiceName(containerName),CommandDispatcherFactory.class,service.dispatcherFactory);
}
