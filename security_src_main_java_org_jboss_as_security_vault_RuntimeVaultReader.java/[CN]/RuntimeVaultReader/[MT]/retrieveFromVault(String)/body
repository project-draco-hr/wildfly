{
  if (isVaultFormat(password)) {
    if (vault == null) {
      throw SecurityMessages.MESSAGES.vaultNotInitializedException();
    }
    try {
      return getValueAsString(password);
    }
 catch (    SecurityVaultException e) {
      throw SecurityMessages.MESSAGES.securityException(e);
    }
  }
  return password;
}
