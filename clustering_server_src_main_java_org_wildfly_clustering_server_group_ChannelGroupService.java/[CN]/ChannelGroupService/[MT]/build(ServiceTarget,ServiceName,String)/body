{
  ChannelGroupService service=new ChannelGroupService();
  return target.addService(name,service).addDependency(GlobalComponentRegistryService.getServiceName(cluster)).addDependency(EmbeddedCacheManagerService.getServiceName(cluster),EmbeddedCacheManager.class,service.manager).addDependency(ChannelNodeFactoryProvider.getServiceName(cluster),ChannelNodeFactory.class,service.factory);
}
