{
  final List<String> refs=new ArrayList<String>();
  final StringBuffer buffer=new StringBuffer();
  do {
    if (buffer.length() > 0) {
      final String ref=buffer.toString().trim();
      if (ref.length() > 0) {
        refs.add(ref);
      }
    }
  }
 while (readLine(is,buffer));
  return refs;
}
