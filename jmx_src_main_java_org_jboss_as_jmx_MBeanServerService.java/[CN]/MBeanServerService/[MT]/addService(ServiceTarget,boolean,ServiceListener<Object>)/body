{
  MBeanServerService service=new MBeanServerService(showModel);
  return batchBuilder.addService(MBeanServerService.SERVICE_NAME,service).addListener(listeners).setInitialMode(ServiceController.Mode.ACTIVE).addDependency(DependencyType.OPTIONAL,Services.JBOSS_SERVER_CONTROLLER,ModelController.class,service.modelControllerValue).install();
}
