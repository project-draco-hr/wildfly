{
  final WebServerService service=new WebServerService(defaultHost);
  context.getBatchBuilder().addService(WebSubsystemElement.JBOSS_WEB,service).addDependency(AbstractPathService.pathNameOf(TEMP_DIR),String.class,service.getPathInjector()).addDependency(DependencyType.OPTIONAL,ServiceName.JBOSS.append("mbean","server"),MBeanServer.class,service.getMbeanServer()).addListener(new UpdateResultHandler.ServiceStartListener<P>(resultHandler,param)).setInitialMode(Mode.ON_DEMAND).install();
}
