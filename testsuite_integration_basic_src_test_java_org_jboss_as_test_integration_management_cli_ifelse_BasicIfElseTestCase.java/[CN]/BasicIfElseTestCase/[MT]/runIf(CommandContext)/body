{
  ctx.handle("if result.value==true of " + this.getReadPropertyReq());
  ctx.handle(this.getWritePropertyReq("\"false\""));
  ctx.handle("else");
  ctx.handle(this.getWritePropertyReq("\"true\""));
  ctx.handle("end-if");
  cliOut.reset();
  ctx.handle(getReadPropertyReq());
  return getValue();
}
