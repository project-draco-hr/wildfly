{
  List<String> result=new ArrayList<String>();
  for (  String serverName : hostModel.getActiveServerNames()) {
    ServerElement server=hostModel.getServer(serverName);
    if (socketGroupName.equals(server.getSocketBindingGroupName()) || socketGroupName.equals(domainModel.getServerGroup(server.getServerGroup()).getSocketBindingGroupName())) {
      result.add(serverName);
    }
  }
  return result;
}
