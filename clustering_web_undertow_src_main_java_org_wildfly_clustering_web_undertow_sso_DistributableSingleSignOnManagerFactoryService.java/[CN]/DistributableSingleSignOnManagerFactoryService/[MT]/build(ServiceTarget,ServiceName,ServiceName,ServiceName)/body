{
  DistributableSingleSignOnManagerFactoryService service=new DistributableSingleSignOnManagerFactoryService();
  return target.addService(name,service).addDependency(managerServiceName,SSOManagerFactory.class,(Injector)service.manager).addDependency(SessionManagerRegistryService.getServiceName(hostServiceName),SessionManagerRegistry.class,service.registry);
}
