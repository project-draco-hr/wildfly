{
  FileChannel channel=null;
  try {
    channel=new FileInputStream(file).getChannel();
    long size=channel.size();
    if (size < ENDLEN) {
      return false;
    }
 else     if (validateEndRecord(file,channel,size - ENDLEN)) {
      return true;
    }
    return scanForEndSig(file,channel);
  }
  finally {
    safeClose(channel);
  }
}
