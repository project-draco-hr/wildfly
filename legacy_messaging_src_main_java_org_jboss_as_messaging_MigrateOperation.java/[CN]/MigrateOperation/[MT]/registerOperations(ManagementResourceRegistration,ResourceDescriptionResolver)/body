{
  registry.registerOperationHandler(new SimpleOperationDefinitionBuilder(MIGRATE,resourceDescriptionResolver).setParameters(ADD_LEGACY_ENTRIES).setReplyParameters(MIGRATION_WARNINGS_ATTR,MIGRATION_ERROR_ATTR).setRuntimeOnly().setAccessConstraints(SensitiveTargetAccessConstraintDefinition.READ_WHOLE_CONFIG).build(),MigrateOperation.MIGRATE_INSTANCE);
  registry.registerOperationHandler(new SimpleOperationDefinitionBuilder(DESCRIBE_MIGRATION,resourceDescriptionResolver).addParameter(ADD_LEGACY_ENTRIES).setReplyParameters(MIGRATION_WARNINGS_ATTR).setRuntimeOnly().setAccessConstraints(SensitiveTargetAccessConstraintDefinition.READ_WHOLE_CONFIG).build(),MigrateOperation.DESCRIBE_MIGRATION_INSTANCE);
}
