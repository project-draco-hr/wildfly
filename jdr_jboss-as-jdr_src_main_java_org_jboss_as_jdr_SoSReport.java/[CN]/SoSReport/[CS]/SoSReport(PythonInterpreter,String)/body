{
  interpreter.exec("import sys");
  interpreter.exec("sys.path.append(\"" + pyLocation + "\")");
  interpreter.exec("from sos.sosreport import SoSReport");
  interpreter.exec("reporter = SoSReport([])");
  this.sosreport=interpreter.get("reporter");
  enableOption("--batch");
  enableOption("--report");
  enableOption("--silent");
}
