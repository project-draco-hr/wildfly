{
  try {
    ServiceContext sc=ri.get_reply_service_context(sasContextId);
    Any msg=codec.decode_value(sc.context_data,SASContextBodyHelper.type());
    SASContextBody contextBody=SASContextBodyHelper.extract(msg);
    if (traceEnabled) {
      log.trace("receive_exception: got SAS reply, type " + contextBody.discriminator());
    }
  }
 catch (  BAD_PARAM e) {
  }
catch (  FormatMismatch e) {
    throw new MARSHAL("Could not parse SAS reply: " + e,MinorCodes.SAS_CSS_FAILURE,CompletionStatus.COMPLETED_MAYBE);
  }
catch (  TypeMismatch e) {
    throw new MARSHAL("Could not parse SAS reply: " + e,MinorCodes.SAS_CSS_FAILURE,CompletionStatus.COMPLETED_MAYBE);
  }
}
