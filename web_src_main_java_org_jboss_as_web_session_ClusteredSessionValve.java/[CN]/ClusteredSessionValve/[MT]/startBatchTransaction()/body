{
  boolean started=false;
  try {
    if (this.tm != null && this.tm.isBatchInProgress() == false) {
      this.tm.startBatch();
      started=true;
    }
  }
 catch (  RuntimeException re) {
    throw re;
  }
catch (  Exception e) {
    throw new ServletException(MESSAGES.failToStartBatchTransaction(e));
  }
  return started;
}
