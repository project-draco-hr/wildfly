{
  if (url.startsWith("IOR:") || url.startsWith("corbaloc:")) {
    return url;
  }
 else {
    InputStream in=null;
    try {
      URL u=new URL(url);
      in=u.openStream();
      if (in != null) {
        BufferedReader bufin=new BufferedReader(new InputStreamReader(in,StandardCharsets.ISO_8859_1));
        String str;
        while ((str=bufin.readLine()) != null) {
          if (str.startsWith("IOR:")) {
            return str;
          }
        }
      }
    }
 catch (    IOException e) {
      NamingException ne=JdkORBMessages.MESSAGES.invalidURLOrIOR(url);
      ne.setRootCause(e);
      throw ne;
    }
 finally {
      try {
        if (in != null) {
          in.close();
        }
      }
 catch (      IOException e) {
        NamingException ne=JdkORBMessages.MESSAGES.invalidURLOrIOR(url);
        ne.setRootCause(e);
        throw ne;
      }
    }
    throw JdkORBMessages.MESSAGES.urlDoesNotContainIOR(url);
  }
}
