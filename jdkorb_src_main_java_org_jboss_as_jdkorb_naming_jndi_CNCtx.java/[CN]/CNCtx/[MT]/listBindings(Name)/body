{
  if (_nc == null)   throw ORBMessages.MESSAGES.notANamingContext(name.toString());
  if (name.size() > 0) {
    try {
      java.lang.Object obj=lookup(name);
      if (obj instanceof CNCtx) {
        return new org.jboss.as.jdkorb.naming.jndi.CNBindingEnumeration((CNCtx)obj,true,_env);
      }
 else {
        throw new NotContextException(name.toString());
      }
    }
 catch (    NamingException ne) {
      throw ne;
    }
catch (    BAD_PARAM e) {
      NamingException ne=new NotContextException(name.toString());
      ne.setRootCause(e);
      throw ne;
    }
  }
  return new org.jboss.as.jdkorb.naming.jndi.CNBindingEnumeration(this,false,_env);
}
