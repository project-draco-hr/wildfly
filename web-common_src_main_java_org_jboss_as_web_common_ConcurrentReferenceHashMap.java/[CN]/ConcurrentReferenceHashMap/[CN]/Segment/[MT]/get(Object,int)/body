{
  if (count != 0) {
    HashEntry<K,V> e=getFirst(hash);
    while (e != null) {
      if (e.hash == hash && keyEq(key,e.key())) {
        Object opaque=e.valueRef;
        if (opaque != null)         return e.dereferenceValue(opaque);
        return readValueUnderLock(e);
      }
      e=e.next;
    }
  }
  return null;
}
