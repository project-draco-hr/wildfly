{
  try {
    if (pkField != null) {
      if (value == null && primaryKey == null) {
        return null;
      }
      if (primaryKey == null) {
        primaryKey=pkClass.newInstance();
      }
      pkField.set(primaryKey,value);
      return primaryKey;
    }
 else {
      return value;
    }
  }
 catch (  Exception e) {
    throw CmpMessages.MESSAGES.errorSettingInstanceField(fieldName,e);
  }
}
