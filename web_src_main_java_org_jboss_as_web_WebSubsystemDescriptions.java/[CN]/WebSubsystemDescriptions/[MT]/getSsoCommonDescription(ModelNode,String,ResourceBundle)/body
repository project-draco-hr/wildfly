{
  node.get(TYPE).set(ModelType.OBJECT);
  node.get(DESCRIPTION).set(bundle.getString("web.virtual-server.sso"));
  node.get(REQUIRED).set(false);
  node.get(type,Constants.CACHE_CONTAINER,TYPE).set(ModelType.STRING);
