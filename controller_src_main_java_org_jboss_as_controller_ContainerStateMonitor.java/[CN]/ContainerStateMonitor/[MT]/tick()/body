{
  int tick=busyServiceCount.decrementAndGet();
synchronized (this) {
    notifyAll();
    if (tick == 0) {
      if (changeReport == null) {
        changeReport=createContainerStateChangeReport();
      }
      if (changeReport != null) {
        final String msg=createChangeReportLogMessage(changeReport);
        changeReport=null;
        log.info(msg);
      }
    }
  }
}
