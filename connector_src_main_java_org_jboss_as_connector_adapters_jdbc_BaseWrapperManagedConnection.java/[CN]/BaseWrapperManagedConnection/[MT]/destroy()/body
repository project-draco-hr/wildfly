{
synchronized (stateLock) {
    destroyed=true;
  }
  cleanup();
  try {
    if (!underlyingAutoCommit)     con.rollback();
  }
 catch (  SQLException ignored) {
    getLog().trace("Ignored error during rollback: ",ignored);
  }
  try {
    con.close();
  }
 catch (  SQLException ignored) {
    getLog().trace("Ignored error during close: ",ignored);
  }
}
