{
  int tryLock=mcf.getUseTryLock().intValue();
  if (tryLock <= 0) {
    lock();
    return;
  }
  try {
    if (!lock.tryLock(tryLock,TimeUnit.SECONDS))     throw new SQLException("Unable to obtain lock in " + tryLock + " seconds: "+ this);
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
    throw new SQLException("Interrupted attempting lock: " + this);
  }
}
