  subsystem.get(ATTRIBUTES,CORE_ENVIRONMENT,VALUE_TYPE,NODE_IDENTIFIER,DEFAULT).set(1);
  subsystem.get(ATTRIBUTES,CORE_ENVIRONMENT,VALUE_TYPE,NODE_IDENTIFIER,REQUIRED).set(false);
