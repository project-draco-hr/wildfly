{
  try {
    return new CompositeType("property",MESSAGES.propertyCompositeType(),new String[]{"name","value"},new String[]{MESSAGES.propertyName(),MESSAGES.propertyValue()},new OpenType[]{SimpleType.STRING,getConverter().getOpenType()});
  }
 catch (  OpenDataException e) {
    throw new RuntimeException(e);
  }
}
