{
  if (auditLogger != null) {
    Subject subject=SubjectUtils.getCurrent();
    AccessAuditContext auditContext=SecurityActions.currentAccessAuditContext();
    auditLogger.logJmxMethodAccess(readOnly,getCallerUserId(subject),auditContext == null ? null : auditContext.getDomainUuid(),auditContext == null ? null : auditContext.getAccessMechanism(),getSubjectInetAddress(subject),methodName,methodSignature,methodParams,error);
  }
}
