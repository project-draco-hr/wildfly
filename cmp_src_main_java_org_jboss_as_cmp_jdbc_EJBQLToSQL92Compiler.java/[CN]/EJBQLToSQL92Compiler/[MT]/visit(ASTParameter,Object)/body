{
  StringBuffer buf=(StringBuffer)data;
  Class type=getParameterType(node.number);
  int ejbqlType=EJBQLTypes.getEJBQLType(type);
  if (ejbqlType == EJBQLTypes.ENTITY_TYPE || ejbqlType == EJBQLTypes.VALUE_CLASS_TYPE || ejbqlType == EJBQLTypes.UNKNOWN_TYPE) {
    throw new IllegalStateException("Can not visit multi-column " + "parameter node. Should have been handled at a higher level.");
  }
  QueryParameter param=new QueryParameter(node.number - 1,typeFactory.getJDBCType(type));
  inputParameters.add(param);
  buf.append('?');
  return data;
}
