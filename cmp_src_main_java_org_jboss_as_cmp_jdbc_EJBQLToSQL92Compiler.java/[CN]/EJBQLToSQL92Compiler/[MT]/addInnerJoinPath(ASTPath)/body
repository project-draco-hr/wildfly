{
  if (path.size() > 1 && path.isCMRField(1)) {
    final String identifier=path.getPath(0);
    final String alias=aliasManager.getAlias(identifier);
    Map paths=(Map)joinPaths.get(alias);
    if (paths == null) {
      paths=new HashMap();
      joinPaths.put(alias,paths);
    }
    path.innerJoin=true;
    paths.put(path,path);
  }
}
