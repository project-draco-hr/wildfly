{
  try {
    final HornetQServer hornetQService=this.hornetQService.getValue();
    hornetQService.deployQueue(new SimpleString(queueConfiguration.getAddress()),new SimpleString(queueConfiguration.getName()),SimpleString.toSimpleString(queueConfiguration.getFilterString()),queueConfiguration.isDurable(),temporary);
  }
 catch (  Exception e) {
    throw new StartException(e);
  }
}
