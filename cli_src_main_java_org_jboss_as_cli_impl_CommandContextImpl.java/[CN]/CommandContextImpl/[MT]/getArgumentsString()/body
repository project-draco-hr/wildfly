{
  if (lineBuffer != null) {
    return lineBuffer.toString();
  }
  if (cmdLine != null && parsedCmd.getOperationName() != null) {
    int cmdNameLength=parsedCmd.getOperationName().length();
    if (cmdLine.length() == cmdNameLength) {
      return null;
    }
 else {
      return cmdLine.substring(cmdNameLength + 1);
    }
  }
  return null;
}
