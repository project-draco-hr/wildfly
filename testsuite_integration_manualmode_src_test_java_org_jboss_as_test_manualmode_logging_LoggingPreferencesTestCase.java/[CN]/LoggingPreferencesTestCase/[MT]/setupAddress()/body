{
  LOGGING_SUBSYSTEM.add(ModelDescriptionConstants.SUBSYSTEM,"logging");
  PROFILE_ADDRESS.add(LOGGING_SUBSYSTEM.get(0)).add("logging-profile",PROFILE_NAME);
