{
  WebArchive archive=ShrinkWrap.create(WebArchive.class,DEPLOYMENT + ".war");
  archive.addClasses(LoggingServlet.class).addAsResource("logging/per-deploy-logging.properties","jboss-logging.properties").setManifest(new StringAsset(Descriptors.create(ManifestDescriptor.class).attribute("Logging-Profile",PROFILE_NAME).exportAsString()));
  return archive;
}
