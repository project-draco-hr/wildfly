{
  lock();
  try {
synchronized (stateLock) {
      if (inManagedTransaction)       inManagedTransaction=false;
    }
    try {
      con.rollback();
    }
 catch (    SQLException e) {
      try {
        checkException(e);
      }
 catch (      Exception e2) {
      }
    }
  }
  finally {
    unlock();
  }
}
