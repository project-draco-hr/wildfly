{
  JAXRS_LOGGER.debug("Activating JAX-RS Extension");
  final SubsystemRegistration subsystem=context.registerSubsystem(SUBSYSTEM_NAME,CURRENT_MODEL_VERSION);
  final ManagementResourceRegistration registration=subsystem.registerSubsystemModel(new SimpleResourceDefinition(JaxrsExtension.SUBSYSTEM_PATH,JaxrsExtension.getResolver(),JaxrsSubsystemAdd.INSTANCE,ReloadRequiredRemoveStepHandler.INSTANCE));
  registration.registerOperationHandler(GenericSubsystemDescribeHandler.DEFINITION,GenericSubsystemDescribeHandler.INSTANCE);
  subsystem.registerXMLElementWriter(parser);
}
