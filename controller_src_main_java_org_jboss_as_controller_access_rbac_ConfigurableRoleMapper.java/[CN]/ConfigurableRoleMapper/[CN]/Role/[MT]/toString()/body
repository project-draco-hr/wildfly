{
  StringBuilder sb=new StringBuilder("[Role name='" + name + "' ");
  sb.append("{Includes = ");
  for (  Principal current : includes) {
    sb.append(current.toString());
  }
  sb.append("}");
  sb.append("{Excludes = ");
  for (  Principal current : excludes) {
    sb.append(current.toString());
  }
  sb.append("}");
  sb.append("]");
  return sb.toString();
}
