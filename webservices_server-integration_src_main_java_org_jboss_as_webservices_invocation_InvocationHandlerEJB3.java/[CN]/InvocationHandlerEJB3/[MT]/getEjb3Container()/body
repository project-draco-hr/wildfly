{
  if (ejbContainer == null) {
synchronized (this) {
      if (ejbContainer == null) {
        final ComponentView ejbView=iocContainer.getBean(ejbName,ComponentView.class);
        if (ejbView == null) {
          throw new WebServiceException("Cannot find ejb: " + ejbName);
        }
        ejbContainer=ejbView.createInstance();
      }
    }
  }
  return ejbContainer;
}
