{
  ModelNode header=headers.get(HEADER_NAME);
  final ModelNode series=header.get(Util.IN_SERIES);
  for (  RolloutPlanGroup group : groups) {
    group.addTo(series);
  }
  if (props != null) {
    for (    String propName : props.keySet()) {
      header.get(propName).set(props.get(propName));
    }
  }
}
