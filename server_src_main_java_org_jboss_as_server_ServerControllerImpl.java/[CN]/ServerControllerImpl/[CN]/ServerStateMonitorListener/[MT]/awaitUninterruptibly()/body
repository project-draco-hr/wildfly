{
  boolean intr=false;
  try {
synchronized (this) {
      while (busyServiceCount.get() > 0) {
        try {
          wait();
        }
 catch (        InterruptedException e) {
          intr=true;
        }
      }
    }
  }
  finally {
    if (intr) {
      Thread.currentThread().interrupt();
    }
  }
}
