{
  if (sortedItems != null) {
    throw MESSAGES.cannotAddMoreItems();
  }
  if (item == null) {
    throw MESSAGES.nullVar("item");
  }
  final T current=items.get(priority);
  if (current != null) {
    throw MESSAGES.priorityAlreadyExists(item,toHexString(priority),current);
  }
  items.put(priority,item);
}
