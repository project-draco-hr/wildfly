{
  return new ManagedReference(){
    private volatile Object instance;
    @Override public void release(){
      this.instance=null;
    }
    @Override public Object getInstance(){
      if (instance != null) {
        return this.instance;
      }
synchronized (this) {
        if (instance == null) {
          this.instance=value.getValue();
        }
      }
      return this.instance;
    }
  }
;
}
