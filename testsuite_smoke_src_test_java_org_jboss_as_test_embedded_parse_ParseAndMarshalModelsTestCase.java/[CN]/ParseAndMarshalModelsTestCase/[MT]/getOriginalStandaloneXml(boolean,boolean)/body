{
  String profile=preview ? (ha ? "standalone-preview-ha.xml" : "standalone-preview.xml") : (ha ? "standalone-ha.xml" : "standalone.xml");
  File f=new File(".").getAbsoluteFile();
  f=f.getParentFile().getParentFile().getParentFile();
  Assert.assertTrue(f.exists());
  f=new File(f,"build");
  Assert.assertTrue(f.exists());
  f=new File(f,"src");
  Assert.assertTrue(f.exists());
  f=new File(f,"main");
  Assert.assertTrue(f.exists());
  f=new File(f,"resources");
  Assert.assertTrue(f.exists());
  f=new File(f,"standalone");
  Assert.assertTrue(f.exists());
  f=new File(f,"configuration");
  Assert.assertTrue(f.exists());
  f=new File(f,profile);
  Assert.assertTrue(f.exists());
  return f;
}
