{
  try {
    for (int i=0; i < commands.length && !cmdCtx.isTerminated(); ++i) {
      cmdCtx.processLine(commands[i]);
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    if (!cmdCtx.isTerminated()) {
      cmdCtx.terminateSession();
    }
    cmdCtx.disconnectController();
  }
}
