{
  String fullcontent=getFullContent("<environment-variables><variable name=\"prop1\" value=\"value1\" bogus=\"bad\"/></environment-variables>");
  try {
    ModelParsingSupport.parseDomainModel(getXMLMapper(),fullcontent);
    fail("Bogus attribute did not cause parsing failure");
