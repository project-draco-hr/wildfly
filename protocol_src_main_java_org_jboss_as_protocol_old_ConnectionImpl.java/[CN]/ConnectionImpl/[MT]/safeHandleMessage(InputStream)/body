{
  try {
    messageHandler.handleMessage(this,pis);
  }
 catch (  RuntimeException e) {
    log.errorf(e,"Failed to read a message");
  }
catch (  IOException e) {
    log.errorf(e,"Failed to read a message");
  }
catch (  NoClassDefFoundError e) {
    log.errorf(e,"Failed to read a message");
  }
catch (  Error e) {
    log.errorf(e,"Failed to read a message");
    throw e;
  }
 finally {
    StreamUtils.safeClose(pis);
  }
}
