{
synchronized (lock) {
    if (sender != this) {
      return;
    }
    sender=null;
    lock.notify();
    if (writeDone)     throw MESSAGES.writeChannelClosed();
    if (readDone) {
      readExecutor.execute(new Runnable(){
        @Override public void run(){
          safeHandleFinished();
        }
      }
);
    }
    CONNECTION_LOGGER.tracef("Sending end of message");
    out.write(CHUNK_END);
  }
}
