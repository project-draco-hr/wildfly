{
  if (dataSourceMappingName == null) {
    return null;
  }
  final JDBCTypeMappingMetaData typeMapping=jdbcApplication.getTypeMappingByName(dataSourceMappingName);
  if (typeMapping == null) {
    throw new RuntimeException("type-mapping is not initialized: " + dataSourceName + " was not deployed or type-mapping was not configured.");
  }
  return typeMapping;
}
