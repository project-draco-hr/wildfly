{
  final SubsystemRegistration registration=context.registerSubsystem(CONNECTOR);
  final ModelNodeRegistration subsystem=registration.registerSubsystemModel(SUBSYSTEM);
  subsystem.registerOperationHandler("add",NewConnectorSubsystemAdd.INSTANCE,SUBSYSTEM_ADD_DESC,false);
  final ModelNodeRegistration archiveValidation=subsystem.registerSubModel(PathElement.pathElement(ARCHIVE_VALIDATION),ARCHIVE_VALIDATION_DESC);
  archiveValidation.registerOperationHandler(ADD,NewArchiveValidationAdd.INSTANCE,ADD_ARCHIVE_VALIDATION_DESC,false);
  archiveValidation.registerOperationHandler(REMOVE,NewArchiveValidationRemove.INSTANCE,REMOVE_ARCHIVE_VALIDATION_DESC,false);
  final ModelNodeRegistration beanValidation=subsystem.registerSubModel(PathElement.pathElement(BEAN_VALIDATION),BEAN_VALIDATION_DESC);
  final ModelNodeRegistration defaulWorkManager=subsystem.registerSubModel(PathElement.pathElement(DEFAULT_WORKMANAGER),DEFAULT_WORKMANAGER_DESC);
  defaulWorkManager.registerOperationHandler(ADD,NewDefaultWorkManagerAdd.INSTANCE,ADD_DEFAULT_WORKMANAGED_DESC,false);
  defaulWorkManager.registerOperationHandler(REMOVE,NewDefaultWorkManagerRemove.INSTANCE,REMOVE_DEFAULT_WORKMANAGED_DESC,false);
}
