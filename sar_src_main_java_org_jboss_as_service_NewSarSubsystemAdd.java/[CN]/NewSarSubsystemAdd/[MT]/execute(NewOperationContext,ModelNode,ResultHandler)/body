{
  if (context instanceof NewBootOperationContext) {
    final NewBootOperationContext bootContext=(NewBootOperationContext)context;
    bootContext.addDeploymentProcessor(Phase.DEPENDENCIES,Phase.DEPENDENCIES_SAR_MODULE,new SarModuleDependencyProcessor());
    bootContext.addDeploymentProcessor(Phase.PARSE,Phase.PARSE_SERVICE_DEPLOYMENT,new ServiceDeploymentParsingProcessor());
    bootContext.addDeploymentProcessor(Phase.INSTALL,Phase.INSTALL_SERVICE_DEPLOYMENT,new ParsedServiceDeploymentProcessor());
  }
  final ModelNode compensatingOperation=new ModelNode();
  compensatingOperation.get(OP).set("remove");
  compensatingOperation.get(OP_ADDR).set(operation.require(OP_ADDR));
  resultHandler.handleResultComplete(compensatingOperation);
  return Cancellable.NULL;
}
