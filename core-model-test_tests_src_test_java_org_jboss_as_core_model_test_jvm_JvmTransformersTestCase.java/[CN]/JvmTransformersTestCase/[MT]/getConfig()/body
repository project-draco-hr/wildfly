{
  FailedOperationTransformationConfig result;
  if (isFailExpressions()) {
    result=new FailedOperationTransformationConfig().addFailedAttribute(ADDRESS,FailedOperationTransformationConfig.ChainedConfig.createBuilder(AGENT_PATH,HEAP_SIZE,JAVA_HOME,MAX_HEAP_SIZE,PERMGEN_SIZE,MAX_PERMGEN_SIZE,STACK_SIZE,OPTIONS,ENVIRONMENT_VARIABLES,ENV_CLASSPATH_IGNORED,AGENT_LIB,JAVA_AGENT,LAUNCH_COMMAND).addConfig(new FailedOperationTransformationConfig.RejectExpressionsConfig(AGENT_PATH,HEAP_SIZE,JAVA_HOME,MAX_HEAP_SIZE,PERMGEN_SIZE,MAX_PERMGEN_SIZE,STACK_SIZE,OPTIONS,ENVIRONMENT_VARIABLES,ENV_CLASSPATH_IGNORED,AGENT_LIB,JAVA_AGENT)).addConfig(new FailedOperationTransformationConfig.NewAttributesConfig(LAUNCH_COMMAND)).build());
  }
 else   if (isFailLaunchCommand()) {
    result=new FailedOperationTransformationConfig().addFailedAttribute(ADDRESS,new FailedOperationTransformationConfig.NewAttributesConfig(LAUNCH_COMMAND));
  }
 else {
    result=FailedOperationTransformationConfig.NO_FAILURES;
  }
  return result;
}
