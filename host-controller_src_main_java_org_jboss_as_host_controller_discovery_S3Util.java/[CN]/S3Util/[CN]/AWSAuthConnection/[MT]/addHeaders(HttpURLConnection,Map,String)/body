{
  if (headers != null) {
    for (Iterator i=headers.keySet().iterator(); i.hasNext(); ) {
      String key=(String)i.next();
      for (Iterator j=((List)headers.get(key)).iterator(); j.hasNext(); ) {
        String value=(String)j.next();
        connection.addRequestProperty(prefix + key,value);
      }
    }
  }
}
