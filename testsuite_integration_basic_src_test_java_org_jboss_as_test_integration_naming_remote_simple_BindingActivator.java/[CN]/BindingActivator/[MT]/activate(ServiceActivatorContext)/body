{
  final BinderService binding=new BinderService("test");
  serviceActivatorContext.getServiceTarget().addService(ContextNames.EXPORTED_CONTEXT_SERVICE_NAME.append("test"),binding).addDependency(ContextNames.EXPORTED_CONTEXT_SERVICE_NAME,ServiceBasedNamingStore.class,binding.getNamingStoreInjector()).addInjection(binding.getManagedObjectInjector(),new ValueManagedReferenceFactory(Values.immediateValue("TestValue"))).install();
  final BinderService nestedBinding=new BinderService("context/test");
  serviceActivatorContext.getServiceTarget().addService(ContextNames.EXPORTED_CONTEXT_SERVICE_NAME.append("context","test"),nestedBinding).addDependency(ContextNames.EXPORTED_CONTEXT_SERVICE_NAME,ServiceBasedNamingStore.class,nestedBinding.getNamingStoreInjector()).addInjection(nestedBinding.getManagedObjectInjector(),new ValueManagedReferenceFactory(Values.immediateValue("TestValue"))).install();
}
