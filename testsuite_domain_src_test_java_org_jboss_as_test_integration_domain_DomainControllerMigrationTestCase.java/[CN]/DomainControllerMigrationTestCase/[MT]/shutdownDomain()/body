{
  try {
    int i=0;
    for (; i < hostUtils.length; i++) {
      try {
        hostUtils[i].stop();
      }
 catch (      Exception e) {
        log.error("Failed closing host util " + i,e);
      }
    }
  }
  finally {
    if (domainControllerClientConfig != null) {
      domainControllerClientConfig.close();
    }
  }
  Assert.assertTrue(warFile.delete());
}
