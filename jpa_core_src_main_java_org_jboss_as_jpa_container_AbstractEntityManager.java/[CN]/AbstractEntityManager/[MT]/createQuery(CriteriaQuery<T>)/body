{
  long start=0;
  if (isTraceEnabled)   start=System.currentTimeMillis();
  try {
    return getEntityManager().createQuery(criteriaQuery);
  }
  finally {
    if (isTraceEnabled) {
      long elapsed=System.currentTimeMillis() - start;
      ROOT_LOGGER.tracef("createQuery took %dms",elapsed);
    }
  }
}
