{
  final ManagedJBossThreadPoolExecutorService executor=this.executor;
  if (executor != null) {
    if (maxThreads < this.maxThreads) {
      executor.setCoreThreads(maxThreads);
      executor.setMaxThreads(maxThreads);
    }
 else {
      executor.setMaxThreads(maxThreads);
      executor.setCoreThreads(maxThreads);
    }
  }
  this.maxThreads=maxThreads;
}
