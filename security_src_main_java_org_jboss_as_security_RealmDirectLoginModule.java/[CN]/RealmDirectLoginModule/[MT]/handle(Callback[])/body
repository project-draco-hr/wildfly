{
  try {
    securityRealm.getCallbackHandler().handle(callbacks);
  }
 catch (  IOException e) {
    throw SecurityMessages.MESSAGES.failureCallingSecurityRealm(e.getMessage());
  }
catch (  UnsupportedCallbackException e) {
    throw SecurityMessages.MESSAGES.failureCallingSecurityRealm(e.getMessage());
  }
}
