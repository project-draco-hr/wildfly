{
  String[] mappings=servlet.findMappings();
  if (log.isTraceEnabled())   log.trace("[getServletName:servletmappings=" + mappings + ":servlet.getName()="+ servlet.getName()+ "]");
  if ("jsp".equals(servlet.getName()) && (mappings != null && mappings[0].indexOf("*.jsp") > -1))   return "";
 else   return servlet.getName();
}
