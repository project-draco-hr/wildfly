{
  registerOperationResource();
  String[] allowedOps=new String[]{ADD,REMOVE,READ_ATTRIBUTE_OPERATION,READ_CHILDREN_NAMES_OPERATION,READ_CHILDREN_RESOURCES_OPERATION,READ_CHILDREN_TYPES_OPERATION,READ_OPERATION_DESCRIPTION_OPERATION,READ_OPERATION_NAMES_OPERATION,READ_RESOURCE_OPERATION,READ_RESOURCE_DESCRIPTION_OPERATION,OP_CONFIG_RO_WRITE,OP_CONFIG_RO_NONE,OP_RUNTIME_RO_WRITE,OP_RUNTIME_RO_NONE};
