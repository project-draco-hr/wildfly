{
  registerOperationResource(resourceSensitivity);
  String[] allowedOps=new String[]{ADD,REMOVE,READ_ATTRIBUTE_OPERATION,READ_CHILDREN_NAMES_OPERATION,READ_CHILDREN_RESOURCES_OPERATION,READ_CHILDREN_TYPES_OPERATION,READ_OPERATION_DESCRIPTION_OPERATION,READ_OPERATION_NAMES_OPERATION,READ_RESOURCE_OPERATION,READ_RESOURCE_DESCRIPTION_OPERATION,UNDEFINE_ATTRIBUTE_OPERATION,WRITE_ATTRIBUTE_OPERATION,OP_CONFIG_RW_ACCESS_READ_WRITE,OP_CONFIG_RW_READ_WRITE,OP_CONFIG_RW_WRITE,OP_CONFIG_RW_READ,OP_CONFIG_RW_NONE,OP_RUNTIME_RW_ACCESS_READ_WRITE,OP_RUNTIME_RW_READ_WRITE,OP_RUNTIME_RW_WRITE,OP_RUNTIME_RW_READ,OP_RUNTIME_RW_NONE,OP_CONFIG_RO_ACCESS_READ_WRITE,OP_CONFIG_RO_READ_WRITE,OP_CONFIG_RO_WRITE,OP_CONFIG_RO_READ,OP_CONFIG_RO_NONE,OP_RUNTIME_RO_ACCESS_READ_WRITE,OP_RUNTIME_RO_READ_WRITE,OP_RUNTIME_RO_WRITE,OP_RUNTIME_RO_READ,OP_RUNTIME_RO_NONE};
  readOperationDescription(StandardRole.ADMINISTRATOR,ONE_ADDR,allowedOps,true);
  readOperationDescription(StandardRole.ADMINISTRATOR,ONE_A_ADDR,allowedOps,true);
}
