{
  long start=0;
  if (log.isTraceEnabled())   start=System.currentTimeMillis();
  try {
    getEntityManager().joinTransaction();
  }
  finally {
    if (log.isTraceEnabled()) {
      long elapsed=System.currentTimeMillis() - start;
      log.tracef("joinTransaction took %dms",elapsed);
    }
  }
}
