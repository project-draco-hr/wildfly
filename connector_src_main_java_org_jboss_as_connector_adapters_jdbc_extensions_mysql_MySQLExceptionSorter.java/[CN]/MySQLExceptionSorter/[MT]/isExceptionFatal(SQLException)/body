{
  if (e.getSQLState() != null) {
    if (e.getSQLState().startsWith("08")) {
      return true;
    }
  }
switch (e.getErrorCode()) {
case 1040:
case 1042:
case 1043:
case 1047:
case 1081:
case 1129:
case 1130:
case 1045:
case 1004:
case 1005:
case 1015:
case 1021:
case 1041:
case 1037:
case 1038:
    return true;
}
return false;
}
