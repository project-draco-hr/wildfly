{
  System.out.println("<<<<@@@@@@@@@@@@@@@@@@@@@@@@@");
  System.out.println("SHARED_STORE_DIR = " + SHARED_STORE_DIR);
  for (  File file : SHARED_STORE_DIR.listFiles()) {
    System.out.println("+ " + file);
    if (file.isDirectory()) {
      for (      File f : file.listFiles()) {
        System.out.println("    + " + f);
      }
    }
  }
  System.out.println("<<<<<@@@@@@@@@@@@@@@@@@@@@@@@@");
}
