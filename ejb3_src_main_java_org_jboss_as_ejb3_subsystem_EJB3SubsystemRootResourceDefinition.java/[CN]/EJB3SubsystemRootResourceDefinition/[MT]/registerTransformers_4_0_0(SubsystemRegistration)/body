{
  final ResourceTransformationDescriptionBuilder builder=TransformationDescriptionBuilder.Factory.createSubsystemInstance();
  builder.getAttributeBuilder().setDiscard(DiscardAttributeChecker.UNDEFINED,SECURITY_DOMAINS);
  builder.getAttributeBuilder().addRejectCheck(RejectAttributeChecker.DEFINED,SECURITY_DOMAINS);
  TransformationDescription.Tools.register(builder.build(),subsystemRegistration,VERSION_4_0_0);
}
