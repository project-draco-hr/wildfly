{
  List<ModelNode> updates=createSubSystem("<bounded-queue-thread-pool name=\"test-pool\" blocking=\"true\" allow-core-timeout=\"true\">" + "   <core-threads count=\"200\" per-cpu=\"15\"/>" + "   <max-threads count=\"100\" per-cpu=\"5\"/>"+ "   <queue-length count=\"300\" per-cpu=\"25\"/>"+ "   <keepalive-time time=\"1000\" unit=\"MILLISECONDS\"/>"+ "   <thread-factory name=\"test-factory\"/>"+ "   <handoff-executor name=\"other\"/>"+ "   <properties>"+ "      <property name=\"propA\" value=\"valueA\"/>"+ "      <property name=\"propB\" value=\"valueB\"/>"+ "   </properties>"+ "</bounded-queue-thread-pool>");
