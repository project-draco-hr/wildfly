{
  final FileInputStream fis;
  final File realFile;
  try {
    realFile=file.getPhysicalFile();
    fis=new FileInputStream(realFile);
  }
 catch (  IOException e) {
    throw new DeploymentUnitProcessingException(e);
  }
  try {
    parse(fis,realFile,mapper,context);
  }
  finally {
    safeClose(fis);
  }
}
