{
  ResourceTransformationDescriptionBuilder builder=TransformationDescriptionBuilder.Factory.createSubsystemInstance();
  builder.getAttributeBuilder().addRejectCheck(RejectAttributeChecker.DEFINED,WeldResourceDefinition.NON_PORTABLE_MODE_ATTRIBUTE,WeldResourceDefinition.REQUIRE_BEAN_DESCRIPTOR_ATTRIBUTE).setDiscard(DiscardAttributeChecker.UNDEFINED,WeldResourceDefinition.NON_PORTABLE_MODE_ATTRIBUTE,WeldResourceDefinition.REQUIRE_BEAN_DESCRIPTOR_ATTRIBUTE).end();
  TransformationDescription.Tools.register(builder.build(),subsystem,ModelVersion.create(1,0,0));
}
