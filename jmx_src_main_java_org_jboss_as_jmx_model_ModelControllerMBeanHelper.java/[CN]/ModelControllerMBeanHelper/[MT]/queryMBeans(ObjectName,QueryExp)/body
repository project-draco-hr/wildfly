{
  return new RootResourceIterator<Set<ObjectInstance>>(accessControlUtil,getRootResourceAndRegistration().getResource(),new ObjectNameMatchResourceAction<Set<ObjectInstance>>(name){
    Set<ObjectInstance> set=new HashSet<ObjectInstance>();
    @Override public boolean onResource(    ObjectName resourceName){
      if (name == null || name.apply(resourceName)) {
        set.add(new ObjectInstance(resourceName,CLASS_NAME));
      }
      return true;
    }
    @Override public Set<ObjectInstance> getResult(){
      if (set.size() == 1 && set.contains(ModelControllerMBeanHelper.createRootObjectName(domain))) {
        return Collections.emptySet();
      }
      return set;
    }
  }
).iterate();
}
