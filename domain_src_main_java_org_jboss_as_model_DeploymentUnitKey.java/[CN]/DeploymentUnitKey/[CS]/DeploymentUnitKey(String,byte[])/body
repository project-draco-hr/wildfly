{
  if (name == null) {
    throw new IllegalArgumentException("name is null");
  }
  if (sha1Hash == null) {
    throw new IllegalArgumentException("sha1Hash is null");
  }
  this.name=name;
  this.sha1Hash=sha1Hash;
  int result=17;
  result+=31 * name.hashCode();
  result+=31 * Arrays.hashCode(sha1Hash);
  this.hashCode=result;
}
