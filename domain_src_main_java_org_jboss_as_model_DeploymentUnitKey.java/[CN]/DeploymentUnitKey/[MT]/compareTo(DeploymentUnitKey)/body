{
  if (sha1Hash.length < o.sha1Hash.length) {
    return -1;
  }
 else   if (sha1Hash.length > o.sha1Hash.length) {
    return 1;
  }
  for (int i=0; i < sha1Hash.length; i++) {
    if (sha1Hash[i] < o.sha1Hash[i]) {
      return -1;
    }
 else     if (sha1Hash[i] > o.sha1Hash[i]) {
      return 1;
    }
  }
  return name.compareTo(o.name);
}
