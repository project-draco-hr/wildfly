{
  URLConnection conn=null;
  InputStream in=null;
  try {
    conn=url.openConnection();
    conn.setDoInput(true);
    in=new BufferedInputStream(conn.getInputStream());
    int i=in.read();
    StringBuilder sb=new StringBuilder();
    while (i != -1) {
      sb.append((char)i);
      i=in.read();
    }
    response=sb.toString();
    return true;
  }
 catch (  Exception e) {
    return false;
  }
 finally {
    safeClose(in);
  }
}
