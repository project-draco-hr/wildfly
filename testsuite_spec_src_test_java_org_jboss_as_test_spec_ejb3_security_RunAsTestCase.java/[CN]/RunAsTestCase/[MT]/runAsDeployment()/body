{
  final WebArchive war=ShrinkWrap.create(WebArchive.class,"ejb3security.war").addPackage(WhoAmI.class.getPackage()).addPackage(WhoAmIBean.class.getPackage()).addPackage(EntryBean.class.getPackage()).addPackage(HttpRequest.class.getPackage()).addClass(Base64.class).addAsResource("ejb3/security/users.properties","users.properties").addAsResource("ejb3/security/roles.properties","roles.properties").addAsWebInfResource("ejb3/security/web.xml","web.xml");
  log.info(war.toString(true));
  return war;
}
