{
  if (_nc == null)   throw new ConfigurationException("Context does not have a corresponding NamingContext");
  try {
    NamingContext nctx=_nc.bind_new_context(path);
    return new CNCtx(_orb,orbTracker,nctx,_env,makeFullName(path));
  }
 catch (  Exception e) {
    throw org.jboss.as.jacorb.naming.jndi.ExceptionMapper.mapException(e,this,path);
  }
}
