{
  if (orb == null || nctx == null)   throw new ConfigurationException("Must supply ORB or NamingContext");
  _orb=orb;
  orbTracker=tracker;
  if (orbTracker != null) {
    orbTracker.incRefCount();
  }
  _nc=nctx;
  _env=env;
  _name=name;
  federation="true".equals(env != null ? env.get(FED_PROP) : null);
}
