{
  super(TransactionExtension.LOG_STORE_PATH,TransactionExtension.getResourceDescriptionResolver(LogStoreConstants.LOG_STORE),new LogStoreAddHandler(resource),NoopOperationStepHandler.WITH_RESULT,OperationEntry.Flag.RESTART_NONE,OperationEntry.Flag.RESTART_NONE);
  this.registerRuntimeOnly=registerRuntimeOnly;
}
