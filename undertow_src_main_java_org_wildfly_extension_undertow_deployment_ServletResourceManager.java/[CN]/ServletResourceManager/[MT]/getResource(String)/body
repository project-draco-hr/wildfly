{
  Resource res=deploymentResourceManager.getResource(path);
  if (res != null) {
    return res;
  }
  String p=path;
  if (p.startsWith("/")) {
    p=p.substring(1);
  }
  if (overlays != null) {
    for (    VirtualFile overlay : overlays) {
      VirtualFile child=overlay.getChild(p);
      if (child.exists()) {
        return new VirtualFileResource(overlay.getPhysicalFile(),child,path);
      }
    }
  }
  return null;
}
