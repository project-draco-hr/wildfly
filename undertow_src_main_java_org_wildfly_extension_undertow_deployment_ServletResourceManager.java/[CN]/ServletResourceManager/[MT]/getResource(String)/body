{
  Resource res=deploymentResourceManager.getResource(path);
  if (res != null) {
    return res;
  }
  String p=path;
  if (p.startsWith("/")) {
    p=p.substring(1);
  }
  for (  VirtualFile overlay : overlays) {
    VirtualFile child=overlay.getChild(p);
    if (child.exists()) {
      URL url=child.toURL();
      return new URLResource(url,url.openConnection());
    }
  }
  return null;
}
