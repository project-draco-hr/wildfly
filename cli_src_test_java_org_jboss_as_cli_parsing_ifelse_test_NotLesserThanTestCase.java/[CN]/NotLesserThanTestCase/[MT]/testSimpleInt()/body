{
  ModelNode node=new ModelNode();
  node.get("result").set(11);
  assertTrue(node,"result >= 11");
  node.get("result").set(111);
  assertTrue(node,"result >= 11");
  node.get("result").set(11);
  assertFalse(node,"result >= 111");
  node.get("result").clear().get("value").set(11);
  assertFalse(node,"result >= 11");
  node.get("result").get("value").set(11);
  assertTrue(node,"result.value >= 1");
  assertFalse(node,"result.value1 >= 1");
  assertFalse(node,"result >= 1");
}
