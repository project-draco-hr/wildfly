{
  if (dir != null && dir.exists()) {
    final File[] files=dir.listFiles();
    if (files != null) {
      for (      final File file : files) {
        if (file.isDirectory()) {
          deleteDirectory(file);
        }
 else {
          DeleteFileAction.delete(file);
        }
      }
    }
    DeleteFileAction.delete(dir);
  }
}
