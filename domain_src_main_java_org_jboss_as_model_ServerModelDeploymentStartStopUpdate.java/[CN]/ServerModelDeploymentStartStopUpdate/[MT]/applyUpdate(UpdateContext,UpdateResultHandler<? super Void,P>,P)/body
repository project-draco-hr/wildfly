{
  if (deploymentElement != null) {
    if (isStart) {
      startStopHandler.deploy(deploymentElement.getUniqueName(),deploymentElement.getRuntimeName(),deploymentElement.getSha1Hash(),updateContext.getBatchBuilder(),updateContext.getServiceContainer(),resultHandler,param);
    }
 else {
      startStopHandler.undeploy(getDeploymentUnitName(),updateContext.getServiceContainer(),resultHandler,param);
    }
  }
 else   if (resultHandler != null) {
    resultHandler.handleSuccess(null,param);
  }
}
