{
  System.out.println("Running");
  final ServiceTarget serviceTarget=serviceActivatorContext.getServiceTarget();
  final LookupService rebindService=new LookupService("BeanWithSimpleInjected");
  serviceTarget.addService(ServiceName.JBOSS.append("BeanWithSimpleInjected","rebind"),rebindService).addDependency(ContextNames.MODULE_CONTEXT_SERVICE_NAME.append("managedbean-example.jar"),Context.class,rebindService.getLookupContextInjector()).addDependency(ServiceName.JBOSS.append("managed","bean","managedbean-example.jar","BeanWithSimpleInjected")).setInitialMode(ServiceController.Mode.ACTIVE).install();
}
