{
  try {
    for (; ; ) {
      if (!socket.isConnected()) {
        break;
      }
      DomainControllerClientProtocol.IncomingCommand.processNext(DomainControllerClientService.this,DomainControllerClientService.this.socketIn);
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
    throw new RuntimeException(t);
  }
 finally {
    closeSocket();
  }
}
