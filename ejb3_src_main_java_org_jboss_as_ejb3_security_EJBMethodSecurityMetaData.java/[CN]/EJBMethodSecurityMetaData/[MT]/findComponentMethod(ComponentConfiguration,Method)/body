{
  final Class<?> componentClass=componentConfiguration.getComponentClass();
  try {
    return componentClass.getMethod(viewMethod.getName(),viewMethod.getParameterTypes());
  }
 catch (  NoSuchMethodException e) {
    throw new RuntimeException("Method named " + viewMethod.getName() + " with params "+ Arrays.toString(viewMethod.getParameterTypes())+ " not found on component class "+ componentClass);
  }
}
