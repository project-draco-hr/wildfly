{
  if (!registrationDone.booleanValue()) {
    if (log.isDebugEnabled()) {
      log.debug("Registration is not done -> stop");
    }
    stop();
  }
 else {
    state=REGISTERED;
    if (createIgnored) {
      try {
        create();
      }
 catch (      Exception e) {
        log.error(e);
      }
    }
  }
}
