{
  final JMSService service=new JMSService();
  target.addService(JMSServices.JMS_MANAGER,service).addDependency(MessagingServices.JBOSS_MESSAGING,HornetQServer.class,service.getHornetQServer()).addDependency(ContextNames.JAVA_CONTEXT_SERVICE_NAME,NamingStore.class,service.getNamingStore()).setInitialMode(Mode.ACTIVE).install();
}
