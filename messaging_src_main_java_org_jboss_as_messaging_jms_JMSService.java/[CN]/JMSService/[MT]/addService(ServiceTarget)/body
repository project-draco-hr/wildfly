{
  final JMSService service=new JMSService();
  target.addService(JMSServices.JMS_MANAGER,service).addDependency(MessagingSubsystemElement.JBOSS_MESSAGING,HornetQServer.class,service.getHornetQServer()).addDependency(JavaContextService.SERVICE_NAME,Context.class,service.getContextInjector()).setInitialMode(Mode.ACTIVE).install();
}
