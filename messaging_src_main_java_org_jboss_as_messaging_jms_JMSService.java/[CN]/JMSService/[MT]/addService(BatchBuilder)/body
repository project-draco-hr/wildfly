{
  try {
    final JMSService service=new JMSService();
    builder.addService(JMSSubsystemElement.JMS_MANAGER,service).addDependency(MessagingSubsystemElement.JBOSS_MESSAGING,HornetQServer.class,service.getHornetQServer()).addInjection(service.getContextInjector(),(Context)new InitialContext()).setInitialMode(Mode.ACTIVE);
  }
 catch (  NamingException e) {
    throw new RuntimeException(e);
  }
}
