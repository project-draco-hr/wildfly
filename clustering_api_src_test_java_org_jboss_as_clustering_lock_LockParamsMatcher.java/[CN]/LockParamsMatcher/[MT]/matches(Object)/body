{
  if (arg instanceof Object[]) {
    Object[] args=(Object[])arg;
    if (args.length == 3) {
      if ("test".equals(args[0]) && node.equals(args[1]) && args[2] instanceof Long) {
        long l=((Long)args[2]).longValue();
        return l >= 0 && l <= timeout;
      }
    }
  }
  return false;
}
