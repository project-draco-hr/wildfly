{
  try {
    final ObjectInputStream in=new ObjectInputStream(new BufferedInputStream(new FileInputStream(inputFile)));
    try {
      classloaderURLs=(List<URL>)in.readObject();
    }
  finally {
      IoUtils.safeClose(in);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
  return this;
}
