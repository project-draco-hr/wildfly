{
  DomainController remoteController=startRemoteSlaveDomainController(masterClient);
  if (remoteController != null) {
    return remoteController;
  }
  if (useCachedDc) {
    return startLocalCopySlaveDomainController(masterClient);
  }
 else {
    throw new StartException("Could not contact master domain controller. No attempt to was made to start up from the cached domain controller since " + CommandLineConstants.CACHED_DC + " was not passed in on the command line");
  }
}
