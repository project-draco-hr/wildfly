{
  context=new StandardContext(){
    private DirContext resources;
    public DirContext getResources(){
      if (resources == null) {
        DirContext orgdirctx=super.getResources();
        if (orgdirctx != null) {
          resources=new JAXRDirContext(new Hashtable<String,Object>(),orgdirctx);
        }
      }
      return resources;
    }
  }
;
}
