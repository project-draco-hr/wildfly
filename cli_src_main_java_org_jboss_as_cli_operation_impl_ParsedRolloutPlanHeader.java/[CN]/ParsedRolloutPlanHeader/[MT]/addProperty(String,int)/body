{
  if (name == null || name.isEmpty()) {
    throw new IllegalArgumentException("Invalid property name: " + name);
  }
  if (props == null) {
    props=new HashMap<String,String>();
  }
  props.put(name,Util.TRUE);
  this.lastGroup=null;
  this.separator=-1;
  this.lastPropertyName=name;
  this.lastChunkIndex=index;
}
