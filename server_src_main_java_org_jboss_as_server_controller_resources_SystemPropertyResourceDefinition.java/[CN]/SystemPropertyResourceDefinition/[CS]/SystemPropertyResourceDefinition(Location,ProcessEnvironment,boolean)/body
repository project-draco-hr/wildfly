{
  super(PathElement.pathElement(SYSTEM_PROPERTY),new ReplaceResourceNameResourceDescriptionResolver(location,SYSTEM_PROPERTY),new SystemPropertyAddHandler(processEnvironment,useBoottime,useBoottime ? ALL_ATTRIBUTES : SERVER_ATTRIBUTES),new SystemPropertyRemoveHandler(processEnvironment));
  this.processEnvironment=processEnvironment;
  this.useBoottime=useBoottime;
}
