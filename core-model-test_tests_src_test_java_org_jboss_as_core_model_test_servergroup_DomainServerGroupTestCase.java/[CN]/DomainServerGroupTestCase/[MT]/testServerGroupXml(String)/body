{
  KernelServices kernelServices=createKernelServicesBuilder(TestModelType.DOMAIN).setXmlResource(resource).setModelInitializer(StandardServerGroupInitializers.XML_MODEL_INITIALIZER,StandardServerGroupInitializers.XML_MODEL_WRITE_SANITIZER).createContentRepositoryContent("12345678901234567890").createContentRepositoryContent("09876543210987654321").build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),resource),marshalled);
  kernelServices=createKernelServicesBuilder(TestModelType.DOMAIN).setXml(marshalled).setModelInitializer(StandardServerGroupInitializers.XML_MODEL_INITIALIZER,StandardServerGroupInitializers.XML_MODEL_WRITE_SANITIZER).createContentRepositoryContent("12345678901234567890").createContentRepositoryContent("09876543210987654321").build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
}
