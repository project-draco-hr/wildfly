{
  try {
    final Xnio xnio=XnioUtil.getXnio();
    final ConnectionChannelThread connectionChannelThread=xnio.createReadChannelThread(new ThreadGroup("Remoting Stream Server connection thread"),OptionMap.EMPTY);
    streamServer=xnio.createStreamServer(getSocketAddress(),connectionChannelThread,connectorValue.getValue(),OptionMap.EMPTY);
  }
 catch (  Exception e) {
    throw new StartException(e);
  }
}
