{
  if (!value.isDefined()) {
    if (!nullable) {
      return "Parameter " + parameterName + " may not be null ";
    }
  }
 else {
    if (value.getType() != type && (!allowExpressions || value.getType() != ModelType.EXPRESSION)) {
      return "Wrong type for " + parameterName + ". Expected "+ type+ " but was "+ value.getType();
    }
  }
  return null;
}
