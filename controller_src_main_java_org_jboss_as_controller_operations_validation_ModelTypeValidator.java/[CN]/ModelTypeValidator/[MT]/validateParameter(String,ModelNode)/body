{
  if (!value.isDefined()) {
    if (!nullable) {
      return "Parameter " + parameterName + " may not be null ";
    }
  }
 else {
    if (!validTypes.contains(value.getType())) {
      return "Wrong type for " + parameterName + ". Expected "+ validTypes.toString()+ " but was "+ value.getType();
    }
  }
  return null;
}
