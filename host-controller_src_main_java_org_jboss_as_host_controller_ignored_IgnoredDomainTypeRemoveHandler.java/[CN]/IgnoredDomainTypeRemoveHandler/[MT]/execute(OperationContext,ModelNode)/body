{
  IgnoreDomainResourceTypeResource resource=IgnoreDomainResourceTypeResource.class.cast(context.removeResource(PathAddress.EMPTY_ADDRESS));
  boolean booting=context.isBooting();
  if (!booting) {
    context.reloadRequired();
  }
  if (context.completeStep() == OperationContext.ResultAction.KEEP) {
    if (booting) {
      resource.publish();
    }
  }
 else   if (!booting) {
    context.revertReloadRequired();
  }
}
