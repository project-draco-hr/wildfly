{
  final WebArchive webArchive=ShrinkWrap.create(WebArchive.class,DEPLOYMENT_NAME + ".war").addClass(TransactionalClient.class).addClass(TransactionalService.class).addClass(TransactionalServiceImpl.class).addAsManifestResource(new StringAsset("Dependencies: org.jboss.xts,org.jboss.jts\n"),"MANIFEST.MF").addAsWebInfResource(EmptyAsset.INSTANCE,ArchivePaths.create("beans.xml"));
  return webArchive;
}
