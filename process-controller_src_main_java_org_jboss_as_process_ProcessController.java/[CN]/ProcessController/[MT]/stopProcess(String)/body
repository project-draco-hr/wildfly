{
synchronized (lock) {
    if (shutdown) {
      return;
    }
    final Map<String,ManagedProcess> processes=this.processes;
    final ManagedProcess process=processes.get(processName);
    if (process == null) {
      ROOT_LOGGER.attemptToStopNonExistentProcess(processName);
      return;
    }
    process.stop();
  }
}
