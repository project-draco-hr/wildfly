{
synchronized (lock) {
    if (shutdown) {
      return;
    }
    ROOT_LOGGER.shuttingDown();
    shutdown=true;
    for (    ManagedProcess process : processes.values()) {
      process.shutdown();
    }
    while (!processes.isEmpty()) {
      try {
        lock.wait();
      }
 catch (      InterruptedException e) {
      }
    }
    ROOT_LOGGER.shutdownComplete();
  }
}
