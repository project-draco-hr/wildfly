{
  WebArchive war1=ShrinkWrap.create(WebArchive.class,"war1.war").addClass(SharedSessionServlet.class);
  WebArchive war2=ShrinkWrap.create(WebArchive.class,"war2.war").addClass(SharedSessionServlet.class);
  EnterpriseArchive ear=ShrinkWrap.create(EnterpriseArchive.class,"sharedSession.ear").addAsModule(war1).addAsModule(war2).addAsManifestResource(SharedSessionTestCase.class.getPackage(),"jboss-all.xml","jboss-all.xml");
  return ear;
}
