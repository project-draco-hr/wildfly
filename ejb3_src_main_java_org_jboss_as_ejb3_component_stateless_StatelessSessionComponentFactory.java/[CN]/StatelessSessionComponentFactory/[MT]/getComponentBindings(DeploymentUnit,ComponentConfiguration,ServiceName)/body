{
  String appName=null;
  String moduleName=deploymentUnit.getName();
  String beanName=componentConfiguration.getName();
  String globalJNDIName=(appName != null ? appName + "/" : "") + moduleName + "/"+ beanName;
  return Arrays.asList(new ComponentBinding(ContextNames.GLOBAL_CONTEXT_SERVICE_NAME,globalJNDIName,ComponentObjectFactory.createReference(componentServiceName,componentConfiguration.getComponentClass())));
}
