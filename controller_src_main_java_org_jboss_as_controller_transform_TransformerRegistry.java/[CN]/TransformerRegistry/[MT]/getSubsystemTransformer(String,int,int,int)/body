{
  List<SubsystemTransformer> transformers=subsystemTransformers.get(name);
  if (transformers == null) {
    return null;
  }
  for (  SubsystemTransformer t : transformers) {
    if (t.getMajorManagementVersion() == majorVersion && t.getMinorManagementVersion() == minorVersion && t.getMicroManagementVersion() == microVersion) {
      return t;
    }
  }
  return null;
}
