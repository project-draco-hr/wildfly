{
  boolean isItSafe=true;
  if (Status.STATUS_COMMITTED != status) {
    String currentThreadName=currentThread();
    boolean isBackgroundReaperThread=currentThreadName != null && currentThreadName.startsWith(ARJUNA_REAPER_THREAD_NAME);
    isItSafe=!isBackgroundReaperThread;
  }
  return isItSafe;
}
