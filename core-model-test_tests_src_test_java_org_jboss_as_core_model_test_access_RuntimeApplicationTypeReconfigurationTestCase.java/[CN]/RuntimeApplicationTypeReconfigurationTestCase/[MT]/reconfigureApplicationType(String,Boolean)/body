{
  PathAddress address=pathAddress(pathElement(CORE_SERVICE,MANAGEMENT),pathElement(ACCESS,AUTHORIZATION),pathElement(CONSTRAINT,APPLICATION_CLASSIFICATION),pathElement(APPLICATION_CLASSIFICATION,applicationType),pathElement(TYPE,CORE));
  ModelNode operation=Util.createOperation(WRITE_ATTRIBUTE_OPERATION,address);
  if (isApplication != null) {
    operation.get(NAME).set(CONFIGURED_APPLICATION);
    operation.get(VALUE).set(isApplication);
    executeWithRoles(operation,StandardRole.SUPERUSER);
  }
}
