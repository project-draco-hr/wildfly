{
  Version.AsVersion version=getVersion(testClass);
  if (support == null) {
    try {
      final MixedDomainTestSupport testSupport=MixedDomainTestSupport.create(testClass.getSimpleName(),version);
      testSupport.start();
      support=testSupport;
    }
 catch (    Exception e) {
      e.printStackTrace();
      throw new RuntimeException(e);
    }
  }
  return support;
}
