{
  if (serverGroup.isDefined()) {
    try {
      final Set<String> specKeys=serverGroup.keys();
      if (!ALLOWED_SERVER_GROUP_CHILDREN.containsAll(specKeys)) {
        throw new OperationFailedException(SERVER_GROUP + " contains unexpected children " + specKeys+ " allowed children are "+ ALLOWED_SERVER_GROUP_CHILDREN);
      }
    }
 catch (    IllegalArgumentException e) {
    }
  }
}
