{
  final SubsystemRegistration subsystem=context.registerSubsystem(SUBSYSTEM_NAME);
  final ModelNodeRegistration registration=subsystem.registerSubsystemModel(SecuritySubsystemProviders.SUBSYSTEM);
  registration.registerOperationHandler(ADD,NewSecuritySubsystemAdd.INSTANCE,SecuritySubsystemProviders.SUBSYSTEM_ADD,false);
  subsystem.registerXMLElementWriter(PARSER);
}
