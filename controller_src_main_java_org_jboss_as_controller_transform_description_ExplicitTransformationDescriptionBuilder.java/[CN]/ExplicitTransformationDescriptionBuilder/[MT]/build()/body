{
  return new AbstractDescription(pathElement,pathAddressTransformer){
    @Override public OperationTransformer getOperationTransformer(){
      return operationTransformer;
    }
    @Override public ResourceTransformer getResourceTransformer(){
      return resourceTransformer;
    }
    @Override public Map<String,OperationTransformer> getOperationTransformers(){
      return Collections.emptyMap();
    }
    @Override public List<TransformationDescription> getChildren(){
      return Collections.emptyList();
    }
  }
;
}
