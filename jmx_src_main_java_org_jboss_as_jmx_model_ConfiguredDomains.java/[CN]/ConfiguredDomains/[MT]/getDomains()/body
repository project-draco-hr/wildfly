{
  if (legacyDomain != null && exprDomain != null) {
    return new String[]{legacyDomain,exprDomain};
  }
 else   if (legacyDomain != null) {
    return new String[]{legacyDomain};
  }
 else   if (exprDomain != null) {
    return new String[]{exprDomain};
  }
 else {
    return new String[]{};
  }
}
