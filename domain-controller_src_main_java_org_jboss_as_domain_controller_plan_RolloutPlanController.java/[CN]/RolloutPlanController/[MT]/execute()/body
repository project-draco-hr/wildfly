{
  this.rootTask.run();
  Result result=null;
  for (  ServerUpdatePolicy policy : updatePolicies.values()) {
    if (policy.isFailed()) {
      result=(result == null || result == Result.FAILED) ? Result.FAILED : Result.PARTIAL;
    }
 else {
      result=(result == null || result == Result.SUCCESS) ? Result.SUCCESS : Result.PARTIAL;
    }
  }
  return result;
}
