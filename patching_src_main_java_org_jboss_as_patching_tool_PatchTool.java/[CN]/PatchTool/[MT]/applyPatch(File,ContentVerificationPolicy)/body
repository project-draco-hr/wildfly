{
  try {
    final InputStream is=new FileInputStream(file);
    try {
      return runner.executeDirect(is,policy);
    }
  finally {
      if (is != null)       try {
        is.close();
      }
 catch (      IOException e) {
        PatchLogger.ROOT_LOGGER.debugf(e,"failed to close input stream");
      }
    }
  }
 catch (  IOException e) {
    throw new PatchingException(e);
  }
}
