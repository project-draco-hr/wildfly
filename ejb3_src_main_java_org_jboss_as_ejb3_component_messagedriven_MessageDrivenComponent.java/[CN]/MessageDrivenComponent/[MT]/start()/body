{
  if (resourceAdapter == null)   throw MESSAGES.resourceAdapterNotSpecified(this);
  super.start();
  try {
    resourceAdapter.endpointActivation(endpointFactory,activationSpec);
  }
 catch (  ResourceException e) {
    throw new RuntimeException(e);
  }
  if (this.pool != null) {
    this.pool.start();
  }
}
