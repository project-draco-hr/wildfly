{
  final JMSQueueService service=new JMSQueueService(name,selector,durableDefault(),jndiBindings());
  final ServiceName serviceName=JMSSubsystemElement.JMS_QUEUE_BASE.append(name);
  context.getBatchBuilder().addService(serviceName,service).addDependency(JMSSubsystemElement.JMS_MANAGER,JMSServerManager.class,service.getJmsServer()).addListener(new UpdateResultHandler.ServiceStartListener<P>(handler,param)).setInitialMode(Mode.ACTIVE);
}
