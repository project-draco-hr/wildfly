{
  String name=binding.getBindName();
  BinderService binder=new BinderService(name);
  ServiceBuilder<ManagedReferenceFactory> builder=this.target.addService(binding.getBinderServiceName(),binder).addAliases(ContextNames.JAVA_CONTEXT_SERVICE_NAME.append(name)).addDependency(targetServiceName,targetClass,new ManagedReferenceInjector<T>(binder.getManagedObjectInjector())).addDependency(binding.getParentContextServiceName(),ServiceBasedNamingStore.class,binder.getNamingStoreInjector()).setInitialMode(ServiceController.Mode.PASSIVE);
  return builder;
}
