{
  if (vault == null) {
    throw new SecurityVaultException("Vault has not been initialized.");
  }
  String[] tokens=tokens(vaultString);
  byte[] sharedKey=null;
  if (tokens.length > 2) {
    sharedKey=tokens[3].getBytes(StandardCharsets.UTF_8);
  }
  return vault.retrieve(tokens[1],tokens[2],sharedKey);
}
