{
  final ResourceTransformationDescriptionBuilder child=subsystemBuilder.addChildResource(SIZE_ROTATING_HANDLER_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,AUTOFLUSH,APPEND,FILE,MAX_BACKUP_INDEX,ROTATE_SIZE).end();
  loggingProfileBuilder.discardChildResource(SIZE_ROTATING_HANDLER_PATH);
  return registerTransformers(child);
}
