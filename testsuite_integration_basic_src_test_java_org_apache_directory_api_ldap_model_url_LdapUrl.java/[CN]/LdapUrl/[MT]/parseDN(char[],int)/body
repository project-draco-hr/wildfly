{
  int end=pos;
  for (int i=pos; (i < chars.length) && (chars[i] != '?'); i++) {
    end++;
  }
  try {
    String dnStr=new String(chars,pos,end - pos);
    dn=new Dn(decode(dnStr));
  }
 catch (  LdapUriException ue) {
    return -1;
  }
catch (  LdapInvalidDnException de) {
    return -1;
  }
  return end;
}
