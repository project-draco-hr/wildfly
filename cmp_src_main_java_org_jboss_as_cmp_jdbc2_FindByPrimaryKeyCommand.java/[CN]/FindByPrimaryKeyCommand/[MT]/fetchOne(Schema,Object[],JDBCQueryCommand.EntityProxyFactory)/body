{
  Object pk=args[0];
  if (pk == null) {
    throw MESSAGES.nullArgumentForFindByPrimaryKey();
  }
  Object instance;
  boolean cached=entity.getTable().hasRow(pk);
  if (!cached) {
    instance=super.executeFetchOne(args,factory);
    if (instance == null) {
      throw MESSAGES.instanceNotFound(entity.getEntityName(),pk);
    }
  }
 else {
    instance=pk;
  }
  return instance;
}
