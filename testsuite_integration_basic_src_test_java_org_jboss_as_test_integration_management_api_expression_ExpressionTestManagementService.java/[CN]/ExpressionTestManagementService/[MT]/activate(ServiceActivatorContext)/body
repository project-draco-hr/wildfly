{
  log.info("Activating service " + ExpressionTestManagementService.class.getName());
  final GetModelControllerService service=new GetModelControllerService();
  context.getServiceTarget().addService(ServiceName.of("expresion-test-management","client","getter"),service).addDependency(Services.JBOSS_SERVER_CONTROLLER,ModelController.class,service.modelControllerValue).install();
}
