{
  testManager=TestProcessUtils.createStreamManager(new TestProcessController(){
    @Override public void stopProcess(    String processName){
      master.stopProcess(processName);
    }
    @Override public void startProcess(    String processName){
      master.startProcess(processName);
    }
  }
);
  master=new ProcessManagerMaster(InetAddress.getLocalHost(),12967);
  master.start();
  TestFileUtils.cleanFiles();
}
