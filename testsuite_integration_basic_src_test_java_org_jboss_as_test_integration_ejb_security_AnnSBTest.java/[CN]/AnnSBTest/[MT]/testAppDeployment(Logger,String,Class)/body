{
  try {
    createSecurityDomain();
  }
 catch (  Exception e) {
    LOG.warn("Problems during creation of security domain",e);
  }
  LOG.info("Security domain: ejb3-tests created");
  final JavaArchive jar=ShrinkWrap.create(JavaArchive.class,MODULE + ".jar").addClass(SB_TO_TEST).addClass(SimpleAuthorizationRemote.class).addClass(ParentAnnOnlyCheck.class).addClass(AnnOnlyCheckSLSBForInjection.class).addClass(AnnOnlyCheckSFSBForInjection.class).addAsResource("ejb3/security/users.properties","users.properties").addAsResource("ejb3/security/roles.properties","roles.properties").addAsManifestResource("ejb3/security/EMPTY_MANIFEST.MF","MANIFEST.MF");
  LOG.info(jar.toString(true));
  return jar;
}
