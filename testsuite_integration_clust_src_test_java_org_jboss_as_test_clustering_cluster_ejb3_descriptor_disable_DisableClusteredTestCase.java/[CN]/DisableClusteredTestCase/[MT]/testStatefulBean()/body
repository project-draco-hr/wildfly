{
  previousSelector=EJBClientContextSelector.setup(PROPERTIES_FILENAME);
  DisableClusteredRemote stateful=(DisableClusteredRemote)context.lookup("ejb:/" + ARCHIVE_NAME + "//DisableClusteredAnnotationStateful!"+ DisableClusteredRemote.class.getName()+ "?stateful");
  String node1=stateful.getNodeState();
  log.info("Called node name: " + node1);
  if (node1.equals(NODE_1)) {
    container.stop(CONTAINER_1);
    node1Running=false;
  }
 else {
    container.stop(CONTAINER_2);
    node2Running=false;
  }
  try {
    stateful.getNodeState();
    Assert.fail("No failover should be provided but it was.");
  }
 catch (  Exception good) {
  }
}
