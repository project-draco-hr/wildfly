{
  if (operations == null) {
    return;
  }
  for (  ModelNode operation : operations) {
    try {
      validateOperation(operation);
    }
 catch (    RuntimeException e) {
      if (exitOnError) {
        throw e;
      }
 else {
        System.out.println("---- Operation validation error:");
        System.out.println(e.getMessage());
      }
    }
  }
}
