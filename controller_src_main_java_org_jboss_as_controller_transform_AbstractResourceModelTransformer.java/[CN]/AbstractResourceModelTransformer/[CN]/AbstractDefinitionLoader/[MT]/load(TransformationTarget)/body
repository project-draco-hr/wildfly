{
  ModelNode model=null;
  try {
    final InputStream is=openStream(target);
    try {
      if (is == null) {
        return null;
      }
      model=ModelNode.fromStream(is);
    }
 catch (    IOException e) {
      ControllerLogger.ROOT_LOGGER.cannotReadTargetDefinition(e);
    }
 finally {
      if (is != null) {
        try {
          is.close();
        }
 catch (        IOException e) {
        }
      }
    }
  }
 catch (  IOException e) {
    ControllerLogger.ROOT_LOGGER.cannotReadTargetDefinition(e);
  }
  if (model != null) {
    return new LegacyResourceDefinition(model);
  }
 else {
    return null;
  }
}
