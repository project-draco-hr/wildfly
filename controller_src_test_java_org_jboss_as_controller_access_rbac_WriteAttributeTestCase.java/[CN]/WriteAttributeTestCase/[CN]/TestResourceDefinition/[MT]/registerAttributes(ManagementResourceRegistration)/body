{
  super.registerAttributes(resourceRegistration);
  AttributeDefinition attributeDefinition=SimpleAttributeDefinitionBuilder.create(UNCONSTRAINED_READWRITE_ATTRIBUTE,ModelType.STRING).setDefaultValue(new ModelNode(OLD_VALUE_OF_UNCONSTRAINED_READWRITE_ATTRIBUTE)).build();
  resourceRegistration.registerReadWriteAttribute(attributeDefinition,null,new ModelOnlyWriteAttributeHandler(attributeDefinition));
  attributeDefinition=SimpleAttributeDefinitionBuilder.create(SENSITIVE_CONSTRAINED_READWRITE_ATTRIBUTE,ModelType.STRING).setDefaultValue(new ModelNode(OLD_VALUE_OF_SENSITIVE_CONSTRAINED_READWRITE_ATTRIBUTE)).setAccessConstraints(MY_SENSITIVE_CONSTRAINT).build();
  resourceRegistration.registerReadWriteAttribute(attributeDefinition,null,new ModelOnlyWriteAttributeHandler(attributeDefinition));
}
