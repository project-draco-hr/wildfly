{
  ModelNode operation=Util.createOperation(IS_CALLER_IN_ROLE,pathAddress(pathElement(CORE_SERVICE,MANAGEMENT),pathElement(ACCESS,AUTHORIZATION),pathElement(ROLE_MAPPING,role)));
  ModelNode result=executeOperation(client,operation,Outcome.SUCCESS);
  assertEquals("role " + role,expectedOutcome,result.get(RESULT).asBoolean());
}
