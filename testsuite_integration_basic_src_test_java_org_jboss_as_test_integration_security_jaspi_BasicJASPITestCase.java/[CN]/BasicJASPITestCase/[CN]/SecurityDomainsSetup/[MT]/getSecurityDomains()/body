{
  final SecurityDomain sd1=new SecurityDomain.Builder().name(SECURITY_DOMAIN_NAME).jaspiAuthn(new JaspiAuthn.Builder().loginModuleStacks(new LoginModuleStack.Builder().name("lmStack").loginModules(new SecurityModule.Builder().name("UsersRoles").build()).build()).authnModules(new AuthnModule.Builder().name(HTTPBasicServerAuthModule.class.getName()).flag(Constants.REQUIRED).loginModuleStackRef("lmStack").build()).build()).build();
  return new SecurityDomain[]{sd1};
}
