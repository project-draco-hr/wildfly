{
  InfinispanSSOManagerFactory<A,D> service=new InfinispanSSOManagerFactory<>();
  return target.addService(name,service).addDependency(CacheService.getServiceName(containerName,cacheName),Cache.class,service.cache).addDependency(KeyAffinityServiceFactoryService.getServiceName(containerName),KeyAffinityServiceFactory.class,service.affinityFactory);
}
