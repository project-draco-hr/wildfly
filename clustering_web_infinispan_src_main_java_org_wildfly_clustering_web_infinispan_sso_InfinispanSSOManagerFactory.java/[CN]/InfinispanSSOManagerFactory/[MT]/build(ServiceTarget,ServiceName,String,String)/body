{
  InfinispanSSOManagerFactory<A,D> service=new InfinispanSSOManagerFactory<>();
  return target.addService(name,service).addDependency(CacheServiceName.CACHE.getServiceName(containerName,cacheName),Cache.class,service.cache).addDependency(CacheContainerServiceName.AFFINITY.getServiceName(containerName),KeyAffinityServiceFactory.class,service.affinityFactory);
}
