{
  if (moduleSpec != null)   return moduleSpec;
  if (startException != null)   throw new ModuleLoadException(startException.getCause());
  try {
    log.debugf("waiting for: %s",identifier);
    if (latch.await(2000,TimeUnit.MILLISECONDS) == false)     throw new ModuleLoadException("Timeout waiting for module service: " + identifier);
  }
 catch (  InterruptedException e) {
  }
  return moduleSpec;
}
