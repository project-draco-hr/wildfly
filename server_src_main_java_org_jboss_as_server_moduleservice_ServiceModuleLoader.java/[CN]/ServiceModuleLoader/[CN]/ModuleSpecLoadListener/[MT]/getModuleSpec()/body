{
  if (moduleSpec != null)   return moduleSpec;
  if (startException != null)   throw new ModuleLoadException(startException.getCause());
  try {
    log.tracef("waiting for: %s",identifier);
    if (latch.await(2000,TimeUnit.MILLISECONDS) == false)     throw ServerMessages.MESSAGES.timeoutWaitingForModuleService(identifier);
  }
 catch (  InterruptedException e) {
  }
  return moduleSpec;
}
