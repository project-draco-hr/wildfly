{
  if (s == null || s.length() == 0) {
    return false;
  }
  char[] c=s.toCharArray();
  if (!Character.isJavaIdentifierStart(c[0])) {
    return false;
  }
  for (int i=1; i < c.length; i++) {
    if (!(Character.isJavaIdentifierPart(c[i]) || c[i] == '-')) {
      return false;
    }
  }
  return true;
}
