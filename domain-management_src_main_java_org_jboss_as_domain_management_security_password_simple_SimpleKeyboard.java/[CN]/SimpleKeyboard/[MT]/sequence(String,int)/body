{
  int len=word.length();
  if (start >= len) {
    throw new IllegalArgumentException("Start index greater than word length");
  }
  int sequenceLength=0;
  for (int index=start; index + 1 < len; index++) {
    if (siblings(word,index)) {
      sequenceLength++;
    }
 else {
      return sequenceLength;
    }
  }
  return sequenceLength;
}
