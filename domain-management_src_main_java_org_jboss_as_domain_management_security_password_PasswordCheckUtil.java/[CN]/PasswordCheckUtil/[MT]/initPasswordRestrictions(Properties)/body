{
  try {
    String forbiddens=props.getProperty(_PROPERTY_FORBIDDEN);
    if (forbiddens == null) {
      return;
    }
    String[] values=forbiddens.split(",");
    for (    String v : values) {
      if (v != null && v.length() > 0) {
        this.passwordValuesRestrictions.add(new ValueRestriction(v));
      }
    }
  }
 catch (  Exception e) {
  }
}
