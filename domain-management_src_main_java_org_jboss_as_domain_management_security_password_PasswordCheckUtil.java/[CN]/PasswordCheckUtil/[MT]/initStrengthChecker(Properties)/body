{
  try {
    String stringClassName=props.getProperty(_PROPERTY_CHECKER);
    if (stringClassName == null) {
      this.simple();
      return;
    }
    Class<PasswordStrengthChecker> clazz=(Class<PasswordStrengthChecker>)PasswordCheckUtil.class.forName(stringClassName);
    this.passwordStrengthChecker=clazz.newInstance();
  }
 catch (  Exception e) {
    this.simple();
  }
}
