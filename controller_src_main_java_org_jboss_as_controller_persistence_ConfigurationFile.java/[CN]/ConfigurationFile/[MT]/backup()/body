{
  if (!doneBootup.get()) {
    return;
  }
  try {
    moveFile(mainFile,getVersionedFile(mainFile));
    int seq=sequence.get();
    if (seq > CURRENT_HISTORY_LENGTH) {
      File delete=getVersionedFile(mainFile,seq - CURRENT_HISTORY_LENGTH);
      if (delete.exists()) {
        delete.delete();
      }
    }
  }
 catch (  IOException e) {
    throw MESSAGES.failedToBackup(e,mainFile);
  }
}
