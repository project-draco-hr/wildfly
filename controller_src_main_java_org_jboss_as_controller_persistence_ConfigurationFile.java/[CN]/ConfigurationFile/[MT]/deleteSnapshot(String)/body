{
  if (prefix.equals("all")) {
    if (snapshotsDirectory.exists() && snapshotsDirectory.isDirectory()) {
      for (      String curr : snapshotsDirectory.list()) {
        new File(snapshotsDirectory,curr).delete();
      }
    }
  }
 else {
    findSnapshotWithPrefix(prefix,true).delete();
  }
}
