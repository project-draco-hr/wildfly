{
  if (doneBootup.get()) {
    throw new IllegalArgumentException("Cannot change files in already booted server");
  }
  final File file=new File(configurationDir,name);
  if (!file.exists()) {
    throw new IllegalArgumentException(file.getAbsolutePath() + " does not exist");
  }
  mainFile=file;
  bootFile=file;
}
