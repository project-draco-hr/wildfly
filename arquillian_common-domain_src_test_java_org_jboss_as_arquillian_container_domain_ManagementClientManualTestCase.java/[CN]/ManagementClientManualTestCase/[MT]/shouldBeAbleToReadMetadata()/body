{
  String uniqueName=null;
  try {
    uniqueName=deployer.deploy(ShrinkWrap.create(EnterpriseArchive.class,"test1.ear").addAsModule(ShrinkWrap.create(WebArchive.class).addClass(ManualTestServlet.class)),"main-server-group");
    HTTPContext context=client.getHTTPDeploymentMetaData(new Server("server-one","master","main-server-group",false),"test1.ear");
    System.out.println(context);
  }
  finally {
    if (uniqueName != null) {
      deployer.undeploy(uniqueName,"main-server-group");
    }
  }
}
