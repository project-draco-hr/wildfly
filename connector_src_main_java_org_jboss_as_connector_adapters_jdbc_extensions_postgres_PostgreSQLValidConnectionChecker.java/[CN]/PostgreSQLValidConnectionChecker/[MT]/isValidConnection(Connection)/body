{
  Statement stmt=null;
  SQLException sqe=null;
  try {
    stmt=c.createStatement();
    stmt.execute("");
  }
 catch (  Exception e) {
    if (e instanceof SQLException) {
      sqe=(SQLException)e;
    }
  }
 finally {
    if (stmt != null) {
      try {
        stmt.close();
      }
 catch (      SQLException e) {
      }
    }
  }
  return sqe;
}
