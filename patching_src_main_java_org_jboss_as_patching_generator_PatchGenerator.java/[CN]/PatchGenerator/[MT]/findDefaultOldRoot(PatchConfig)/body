{
  if (newRoot == null) {
    return null;
  }
  File rootParent=newRoot.getParentFile();
  if (rootParent == null) {
    return null;
  }
  for (  String appliesTo : patchConfig.getAppliesTo()) {
    File root=new File(rootParent,appliesTo);
    if (!root.exists()) {
      continue;
    }
    boolean[] modIgnored=new boolean[2];
    if (modIgnored[0] && modIgnored[1]) {
      return root;
    }
  }
  return null;
}
