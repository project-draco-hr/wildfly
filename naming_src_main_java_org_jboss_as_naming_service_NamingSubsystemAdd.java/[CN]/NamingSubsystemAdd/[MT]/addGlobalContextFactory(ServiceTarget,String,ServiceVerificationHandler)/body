{
  final GlobalContextService eeContextService=new GlobalContextService(contextName);
  return target.addService(ContextNames.JAVA_CONTEXT_SERVICE_NAME.append(contextName),eeContextService).addDependency(ContextNames.JAVA_CONTEXT_SERVICE_NAME,NamingStore.class,eeContextService.getJavaContextInjector()).addListener(verificationHandler).setInitialMode(ServiceController.Mode.ACTIVE).install();
}
