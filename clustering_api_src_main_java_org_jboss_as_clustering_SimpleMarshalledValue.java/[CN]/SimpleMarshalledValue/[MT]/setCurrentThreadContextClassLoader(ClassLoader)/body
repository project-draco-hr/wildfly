{
  if (System.getSecurityManager() == null) {
    Thread.currentThread().setContextClassLoader(loader);
  }
 else {
    PrivilegedAction<Void> action=new PrivilegedAction<Void>(){
      @Override public Void run(){
        Thread.currentThread().setContextClassLoader(loader);
        return null;
      }
    }
;
    AccessController.doPrivileged(action);
  }
}
