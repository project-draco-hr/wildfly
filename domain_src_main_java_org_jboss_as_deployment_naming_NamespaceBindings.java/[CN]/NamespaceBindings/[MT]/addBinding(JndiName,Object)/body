{
  final Object existing=bindings.putIfAbsent(name,value);
  if (existing != null && !existing.equals(value)) {
    final StringBuilder builder=new StringBuilder("Duplicate JNDI bindings for '").append(name).append("' are not compatible.  [").append(existing).append("] != [").append(value).append("]");
    throw new DuplicateBindingException(builder.toString());
  }
  return existing == null;
}
