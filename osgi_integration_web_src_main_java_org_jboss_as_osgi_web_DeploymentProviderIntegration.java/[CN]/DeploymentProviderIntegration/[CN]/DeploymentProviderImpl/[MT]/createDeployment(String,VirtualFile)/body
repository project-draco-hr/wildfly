{
  if (!location.startsWith(WebExtension.WEBBUNDLE_PREFIX))   return defaultHandler.createDeployment(location,rootFile);
  Manifest manifest=WebBundleURIParser.parse(location);
  OSGiMetaData metadata=OSGiMetaDataBuilder.load(manifest);
  String symbolicName=metadata.getBundleSymbolicName();
  Deployment dep=DeploymentFactory.createDeployment(rootFile,location,symbolicName,null);
  dep.putAttachment(MANIFEST_KEY,manifest);
  dep.putAttachment(OSGI_METADATA_KEY,metadata);
  return dep;
}
