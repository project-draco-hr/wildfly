{
  if (safeToClose(status)) {
    try {
      if (JPA_LOGGER.isDebugEnabled())       JPA_LOGGER.debugf("%s: closing entity managersession",getEntityManagerDetails(manager));
      manager.close();
    }
 catch (    Exception ignored) {
      if (JPA_LOGGER.isDebugEnabled())       JPA_LOGGER.debugf(ignored,"ignoring error that occurred while closing EntityManager for %s (",scopedPuName);
    }
  }
}
