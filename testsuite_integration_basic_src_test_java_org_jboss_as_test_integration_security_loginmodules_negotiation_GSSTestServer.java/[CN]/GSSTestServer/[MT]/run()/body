{
  try {
    start();
  }
 catch (  LoginException e) {
    LOGGER.error("LoginException: ",e);
    throw new RuntimeException(e);
  }
catch (  PrivilegedActionException e) {
    LOGGER.error("PrivilegedActionException: ",e);
    throw new RuntimeException(e);
  }
catch (  IOException e) {
    LOGGER.error("IOException: ",e);
    throw new RuntimeException(e);
  }
}
