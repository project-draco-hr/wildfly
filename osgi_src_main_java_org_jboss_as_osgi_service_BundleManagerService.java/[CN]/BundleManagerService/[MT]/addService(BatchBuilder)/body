{
  BundleManagerService service=new BundleManagerService();
  batchBuilder.addService(BundleManagerService.SERVICE_NAME,service).addDependency(Configuration.SERVICE_NAME,Configuration.class,service.injectedConfig).addDependency(ClassifyingModuleLoaderService.SERVICE_NAME,ClassifyingModuleLoaderService.class,service.injectedModuleLoader).addDependency(SocketBinding.JBOSS_BINDING_NAME.append("osgi-http"),SocketBinding.class,service.osgiHttpServerPortBinding).setInitialMode(Mode.ON_DEMAND).install();
}
