{
  if (workingDir == null) {
    if (workingDir == null) {
      workingDir=new File(".");
      workingDir=new File(workingDir,"target");
      workingDir=new File(workingDir,"apacheds_working").getCanonicalFile();
      if (!workingDir.exists()) {
        workingDir.mkdirs();
      }
    }
  }
  for (  File current : workingDir.listFiles()) {
    current.delete();
  }
}
