{
  final WebArchive war=ShrinkWrap.create(WebArchive.class,"ejb3security.war").addPackage(EntryBean.class.getPackage()).addClass(Util.class).addAsResource("ejb3/security/users.properties","users.properties").addAsResource("ejb3/security/roles.properties","roles.properties");
  log.info(war.toString(true));
  return war;
}
