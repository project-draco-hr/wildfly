{
switch (stage) {
case MODEL:
case IMMEDIATE:
    super.addStep(response,operation,step,stage);
  break;
case RUNTIME:
if (runtimeOps != null) {
  ParsedBootOp parsedOp=new ParsedBootOp(operation,step,response);
  runtimeOps.add(parsedOp);
}
 else {
  super.addStep(response,operation,step,stage);
}
break;
default :
primaryContext.addStep(response,operation,step,stage);
}
}
