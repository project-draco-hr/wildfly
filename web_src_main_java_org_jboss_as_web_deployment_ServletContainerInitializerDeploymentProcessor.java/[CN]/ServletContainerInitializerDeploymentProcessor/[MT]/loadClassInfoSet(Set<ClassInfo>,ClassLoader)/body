{
  Set<Class<?>> classes=new HashSet<Class<?>>();
  for (  ClassInfo classInfo : classInfos) {
    Class<?> type;
    try {
      type=classLoader.loadClass(classInfo.name().toString());
      classes.add(type);
    }
 catch (    Exception e) {
      WebLogger.WEB_LOGGER.cannotLoadDesignatedHandleTypes(classInfo,e);
    }
  }
  return classes;
}
