{
  boolean finalize=true;
  Throwable toThrow=null;
  try {
    if (step.resultHandler != null) {
      if (!hasMoreSteps()) {
        completeStep();
      }
 else {
        finalize=false;
      }
    }
  }
 catch (  RuntimeException re) {
    toThrow=re;
  }
catch (  Error e) {
    toThrow=e;
  }
 finally {
    if (finalize) {
      step.finalizeStep(toThrow);
    }
 else {
      throwThrowable(toThrow);
    }
  }
}
