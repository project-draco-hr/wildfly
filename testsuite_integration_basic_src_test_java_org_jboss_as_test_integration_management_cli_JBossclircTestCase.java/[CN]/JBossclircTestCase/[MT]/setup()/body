{
  ensureRemoved(TMP_JBOSS_CLI_RC);
  BufferedWriter writer=null;
  try {
    writer=new BufferedWriter(new FileWriter(TMP_JBOSS_CLI_RC));
    writer.write("set " + VAR_NAME + "="+ VAR_VALUE);
    writer.newLine();
  }
 catch (  IOException e) {
    fail(e.getLocalizedMessage());
  }
 finally {
    if (writer != null) {
      try {
        writer.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
