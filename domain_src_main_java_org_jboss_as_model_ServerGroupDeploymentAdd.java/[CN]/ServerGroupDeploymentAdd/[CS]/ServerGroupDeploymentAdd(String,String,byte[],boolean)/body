{
  if (uniqueName == null)   throw new IllegalArgumentException("uniqueName is null");
  if (runtimeName == null)   throw new IllegalArgumentException("runtimeName is null");
  if (hash == null)   throw new IllegalArgumentException("hash is null");
  this.uniqueName=uniqueName;
  this.runtimeName=runtimeName;
  this.hash=hash;
  this.start=start;
}
