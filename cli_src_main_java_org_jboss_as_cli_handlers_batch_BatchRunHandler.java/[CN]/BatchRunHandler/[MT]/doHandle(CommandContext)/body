{
  boolean failed=false;
  try {
    super.doHandle(ctx);
    ctx.printLine("The batch executed successfully");
  }
 catch (  CommandLineException e) {
    failed=true;
    throw e;
  }
 finally {
    if (!failed) {
      if (ctx.getBatchManager().isBatchActive()) {
        ctx.getBatchManager().discardActiveBatch();
      }
    }
  }
}
