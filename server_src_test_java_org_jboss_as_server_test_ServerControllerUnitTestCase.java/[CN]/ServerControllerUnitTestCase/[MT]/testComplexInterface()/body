{
  final ModelNode operation=new ModelNode();
  operation.get(ModelDescriptionConstants.OP).set("add");
  operation.get(ModelDescriptionConstants.OP_ADDR).add("interface","complex");
  populateCritieria(operation,Nesting.TOP,InterfaceDescription.LINK_LOCAL_ADDRESS,InterfaceDescription.LOOPBACK);
  populateCritieria(operation.get("not"),Nesting.NOT,InterfaceDescription.PUBLIC_ADDRESS,InterfaceDescription.SITE_LOCAL_ADDRESS,InterfaceDescription.VIRTUAL,InterfaceDescription.UP,InterfaceDescription.MULTICAST,InterfaceDescription.LOOPBACK_ADDRESS,InterfaceDescription.POINT_TO_POINT);
  populateCritieria(operation.get("any"),Nesting.ANY);
  final ModelControllerClient client=controller.createClient(Executors.newCachedThreadPool());
  executeForResult(client,operation);
}
