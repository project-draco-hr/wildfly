{
  try {
    int bytesTotal=cliStream.available();
    if (bytesTotal > 0) {
      final byte[] bytes=new byte[bytesTotal];
      cliStream.read(bytes);
      cliOutBuf.append(new String(bytes));
    }
  }
 catch (  IOException e) {
    fail("Failed to read command's output: " + e.getLocalizedMessage());
  }
}
