{
  ROOT_LOGGER.starting(name);
  try {
    EndpointPublisherImpl publisher=new EndpointPublisherImpl(hostInjector.getValue(),true);
    wsctx=publisher.publish(ctx.getChildTarget(),context,loader,urlPatternToClassName,jbwmd,wsmd);
  }
 catch (  Exception e) {
