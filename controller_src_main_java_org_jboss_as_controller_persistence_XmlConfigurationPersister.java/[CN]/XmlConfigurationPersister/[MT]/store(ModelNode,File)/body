{
  try {
    final FileOutputStream fos=new FileOutputStream(file);
    try {
      BufferedOutputStream output=new BufferedOutputStream(fos);
      marshallAsXml(model,output);
      output.flush();
      fos.getFD().sync();
      output.close();
    }
  finally {
      safeClose(fos);
    }
  }
 catch (  Exception e) {
    throw MESSAGES.failedToStoreConfiguration(e);
  }
}
