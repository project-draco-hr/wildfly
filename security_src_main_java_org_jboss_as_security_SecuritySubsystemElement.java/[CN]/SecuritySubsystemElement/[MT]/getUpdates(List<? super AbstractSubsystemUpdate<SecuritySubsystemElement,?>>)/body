{
  if (securityManagement != null) {
    AddSecurityManagementUpdate update=securityManagement.asUpdate();
    list.add(update);
  }
  if (subjectFactory != null) {
    AddSubjectFactoryUpdate update=subjectFactory.asUpdate();
    list.add(update);
  }
  if (jaas != null) {
    AddJaasUpdate update=jaas.asUpdate();
    list.add(update);
  }
}
