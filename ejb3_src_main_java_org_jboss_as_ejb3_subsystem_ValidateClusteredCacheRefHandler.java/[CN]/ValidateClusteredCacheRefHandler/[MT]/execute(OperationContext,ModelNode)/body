{
  if (context.getProcessType().isServer()) {
    ModelNode model=context.readResource(PathAddress.EMPTY_ADDRESS).getModel();
    if (model.hasDefined(DEFAULT_CLUSTERED_SFSB_CACHE.getName()) && !model.get(DEFAULT_CLUSTERED_SFSB_CACHE.getName()).equals(model.get(DEFAULT_SFSB_CACHE.getName()))) {
      throw EjbMessages.MESSAGES.inconsistentAttributeNotSupported(DEFAULT_CLUSTERED_SFSB_CACHE.getName(),DEFAULT_SFSB_CACHE.getName());
    }
  }
  context.stepCompleted();
}
