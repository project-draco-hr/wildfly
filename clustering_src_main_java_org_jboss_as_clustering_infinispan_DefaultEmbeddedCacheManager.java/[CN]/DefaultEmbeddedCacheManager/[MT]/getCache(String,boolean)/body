{
  SwitchContext context=start ? switcher.getSwitchContext(DefaultEmbeddedCacheManager.class.getClassLoader()) : null;
  try {
    return new DelegatingCache<K,V>(this.container.<K,V>getCache(this.getCacheName(cacheName)));
  }
  finally {
    if (context != null) {
      context.reset();
    }
  }
}
