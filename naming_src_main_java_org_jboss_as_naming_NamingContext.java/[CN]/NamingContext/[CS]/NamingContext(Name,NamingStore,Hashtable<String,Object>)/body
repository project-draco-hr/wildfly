{
  if (prefix == null) {
    throw MESSAGES.nullVar("Naming prefix");
  }
  this.prefix=prefix;
  if (namingStore == null) {
    throw MESSAGES.nullVar("NamingStore");
  }
  this.namingStore=namingStore;
  if (environment != null) {
    this.environment=new Hashtable<String,Object>(environment);
  }
 else {
    this.environment=new Hashtable<String,Object>();
  }
}
