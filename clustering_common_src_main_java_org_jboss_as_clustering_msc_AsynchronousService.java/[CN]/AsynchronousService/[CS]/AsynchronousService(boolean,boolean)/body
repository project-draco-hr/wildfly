{
  this.startAsynchronously=startAsynchronously;
  this.stopAsynchronously=stopAsynchronously;
  final ThreadFactory factory=new JBossThreadFactory(new ThreadGroup(String.format("%s lifecycle",this.getClass().getSimpleName())),Boolean.FALSE,null,"%G - %t",null,null,AccessController.getContext());
  this.executor=Executors.newCachedThreadPool(factory);
}
