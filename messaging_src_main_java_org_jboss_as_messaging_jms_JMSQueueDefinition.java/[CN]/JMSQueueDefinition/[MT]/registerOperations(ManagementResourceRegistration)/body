{
  super.registerOperations(registry);
  if (registerRuntimeOnly) {
    JMSQueueControlHandler.INSTANCE.registerOperations(registry);
    if (!deployed) {
      SimpleOperationDefinition op=new SimpleOperationDefinition(ConnectionFactoryAddJndiHandler.ADD_JNDI,getResourceDescriptionResolver(),ConnectionFactoryAddJndiHandler.JNDI_BINDING);
      registry.registerOperationHandler(op,JMSQueueAddJndiHandler.INSTANCE);
    }
  }
}
