{
  if (OUTDATED_NAMESPACES.containsKey(xsdName)) {
    throw new RuntimeException("Default configs are not in line with most recent schemas " + xsdName + " has been superseded by "+ OUTDATED_NAMESPACES.get(xsdName));
  }
  final File file=JBOSS_SCHEMAS_MAP.get(xsdName);
  URL url=null;
  try {
    if (file != null) {
      url=file.toURI().toURL();
    }
  }
 catch (  IOException e) {
    url=null;
  }
  final ClassLoader classLoader=Thread.currentThread().getContextClassLoader();
  if (url == null)   url=classLoader.getResource("docs/schema" + xsdName);
  if (url == null)   url=classLoader.getResource("docs/" + xsdName);
  if (url == null)   url=classLoader.getResource("schema/" + xsdName);
  if (url == null)   url=classLoader.getResource(xsdName);
  assertNotNull(xsdName + " not found",url);
  return url;
}
