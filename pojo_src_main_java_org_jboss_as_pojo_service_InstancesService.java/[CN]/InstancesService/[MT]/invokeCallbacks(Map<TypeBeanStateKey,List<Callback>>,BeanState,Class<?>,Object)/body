{
synchronized (clazz) {
    TypeBeanStateKey key=new TypeBeanStateKey(clazz,state);
    List<Callback> callbacks=map.get(key);
    if (callbacks != null) {
      for (      Callback c : callbacks) {
        try {
          c.dispatch(bean);
        }
 catch (        Throwable t) {
          log.warn("Error invoking callback: " + c,t);
        }
      }
    }
  }
}
