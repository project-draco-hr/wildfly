{
  Transaction tx=this.getTransaction();
  try {
    if (tx.getStatus() == Status.STATUS_MARKED_ROLLBACK) {
      tx.rollback();
    }
 else {
      tx.commit();
    }
  }
 catch (  RollbackException|HeuristicMixedException|HeuristicRollbackException|SystemException e) {
    throw new CacheException(e);
  }
}
