{
  File[] logs=null;
  try {
    logs=file.getParentFile().listFiles();
  }
 catch (  NullPointerException ignore) {
  }
  if (logs != null) {
    for (    File f : logs) {
      if (f.getName().contains(logFile.getName())) {
        f.delete();
        log.info("Deleted: " + f.getAbsolutePath());
        if (f.exists()) {
          Assert.fail("Unable to delete file: " + f.getName());
        }
      }
    }
  }
}
