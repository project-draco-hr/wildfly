{
  try {
    processViewChange(newView);
  }
 catch (  InterruptedException ex) {
    Thread.currentThread().interrupt();
    CoreGroupCommunicationService.this.log.methodFailure(ex,"ViewAccepted");
  }
catch (  Exception ex) {
    CoreGroupCommunicationService.this.log.methodFailure(ex,"ViewAccepted");
  }
}
