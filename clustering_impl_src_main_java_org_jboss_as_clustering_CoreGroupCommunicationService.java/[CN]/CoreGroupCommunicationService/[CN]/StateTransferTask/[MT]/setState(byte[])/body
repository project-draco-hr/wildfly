{
  try {
    if (state == null) {
      CoreGroupCommunicationService.this.log.debug("transferred state for service " + serviceName + " is null (may be first member in cluster)");
    }
 else {
      ByteArrayInputStream bais=new ByteArrayInputStream(state);
      setStateInternal(bais);
      bais.close();
    }
    this.isStateSet=true;
  }
 catch (  Throwable t) {
    recordSetStateFailure(t);
  }
 finally {
synchronized (this) {
      notifyAll();
    }
  }
}
