{
  super(ID);
  setEnterHandler(new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws OperationFormatException {
      if (ctx.getCharacter() == '/') {
        ctx.leaveState();
      }
 else {
        GlobalCharacterHandlers.CONTENT_CHARACTER_HANDLER.handle(ctx);
      }
    }
  }
);
  setDefaultHandler(GlobalCharacterHandlers.CONTENT_CHARACTER_HANDLER);
  putHandler('=',new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws OperationFormatException {
      ctx.leaveState();
    }
  }
);
  putHandler('/',new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws OperationFormatException {
      ctx.leaveState();
    }
  }
);
  final CharacterHandler colonHandler=new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws OperationFormatException {
      ctx.leaveState();
      ctx.enterState(opState);
    }
  }
;
  putHandler(':',colonHandler);
}
