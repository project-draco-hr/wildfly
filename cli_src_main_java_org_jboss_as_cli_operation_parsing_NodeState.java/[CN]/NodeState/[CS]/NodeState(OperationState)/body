{
  super(ID);
  setEnterHandler(new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws CommandFormatException {
      if (ctx.getCharacter() == '/') {
        ctx.leaveState();
      }
 else {
        getHandler(ctx.getCharacter()).handle(ctx);
      }
    }
  }
);
  setDefaultHandler(GlobalCharacterHandlers.CONTENT_CHARACTER_HANDLER);
  putHandler('=',new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws CommandFormatException {
      ctx.leaveState();
    }
  }
);
  putHandler('/',new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws CommandFormatException {
      ctx.leaveState();
    }
  }
);
  final CharacterHandler colonHandler=new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws CommandFormatException {
      ctx.leaveState();
      ctx.enterState(opState);
    }
  }
;
  putHandler(':',colonHandler);
  enterState('"',QuotesState.QUOTES_EXCLUDED);
  enterState('\\',EscapeCharacterState.INSTANCE);
}
