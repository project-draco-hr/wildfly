{
  Order o;
  LineItem l1;
  LineItem l2;
  try {
    o=orderHome.create();
    l1=lineItemHome.create();
    l2=lineItemHome.create();
  }
 catch (  CreateException ex) {
    throw new EJBException(ex);
  }
  l1.setOrder(o);
  Collection c=o.getLineItems();
  try {
    c.add(l2);
  }
 catch (  IllegalStateException ex) {
    return true;
  }
  return false;
}
