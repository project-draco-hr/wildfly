{
  BatchedWork work=new BatchedWork(){
    @Override public void execute(    BatchBuilder batchBuilder) throws Exception {
      Object service=loadClass(moduleId,EchoTargetService.class.getName()).newInstance();
      batchBuilder.addService(EchoTargetService.SERVICE_NAME,(Service<?>)service).addAliases(ServiceName.of(Constants.JBOSGI_PREFIX,Echo.class.getName())).setInitialMode(Mode.ACTIVE).install();
    }
  }
;
  runWithLatchedBatch(work);
}
