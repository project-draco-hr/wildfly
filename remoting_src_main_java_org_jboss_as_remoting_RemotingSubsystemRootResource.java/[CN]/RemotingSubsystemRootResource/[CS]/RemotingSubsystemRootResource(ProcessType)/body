{
  super(PathElement.pathElement(ModelDescriptionConstants.SUBSYSTEM,RemotingExtension.SUBSYSTEM_NAME),RemotingExtension.getResourceDescriptionResolver(RemotingExtension.SUBSYSTEM_NAME),processType.isServer() ? RemotingSubsystemAdd.SERVER : RemotingSubsystemAdd.DOMAIN,RemotingSubsystemRemove.INSTANCE);
