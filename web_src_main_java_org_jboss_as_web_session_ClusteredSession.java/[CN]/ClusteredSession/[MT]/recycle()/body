{
  if (!isValid) {
    SessionInvalidationTracker.clearInvalidatedSession(id,manager);
  }
  attributes.clear();
  setAuthType(null);
  creationTime=0L;
  expiring=false;
  id=null;
  lastAccessedTime=0L;
  maxInactiveInterval=-1;
  notes.clear();
  setPrincipal(null);
  isNew=false;
  isValid=false;
  firstAccess=true;
  manager=null;
  listeners.clear();
  support=new PropertyChangeSupport(this);
  invalidationPolicy=ReplicationTrigger.ACCESS;
  outdatedTime=0;
  sessionAttributesDirty=false;
  sessionMetadataDirty=false;
  realId=null;
  version.set(0);
  hasActivationListener=null;
  lastReplicated=0;
  maxUnreplicatedInterval=0;
  alwaysReplicateTimestamp=true;
  this.notificationPolicy=null;
  this.clusterStatus=null;
}
