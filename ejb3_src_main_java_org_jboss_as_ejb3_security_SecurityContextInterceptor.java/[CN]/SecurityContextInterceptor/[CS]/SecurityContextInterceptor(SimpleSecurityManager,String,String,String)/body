{
  this.pushAction=new PrivilegedAction<Void>(){
    @Override public Void run(){
      securityManager.push(securityDomain,runAs,runAsPrincipal);
      return null;
    }
  }
;
  this.popAction=new PrivilegedAction<Void>(){
    @Override public Void run(){
      securityManager.pop();
      return null;
    }
  }
;
}
