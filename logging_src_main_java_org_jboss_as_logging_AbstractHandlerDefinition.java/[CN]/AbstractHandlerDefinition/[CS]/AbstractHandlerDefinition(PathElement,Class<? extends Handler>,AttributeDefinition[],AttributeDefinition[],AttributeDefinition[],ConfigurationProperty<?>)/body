{
  super(path,HANDLER_RESOLVER,new HandlerOperations.HandlerAddOperationStepHandler(type,addAttributes,constructionProperties),HandlerOperations.REMOVE_HANDLER);
  this.writableAttributes=writableAttributes;
  writeHandler=new HandlerOperations.LogHandlerWriteAttributeHandler(this.writableAttributes);
  this.readOnlyAttributes=readOnlyAttributes;
}
