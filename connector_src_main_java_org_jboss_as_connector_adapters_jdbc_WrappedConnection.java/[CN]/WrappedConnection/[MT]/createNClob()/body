{
  lock();
  try {
    Connection c=getUnderlyingConnection();
    try {
      if (spy)       spyLogger.debugf("%s [%s] createNClob()",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION);
      return c.createNClob();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
