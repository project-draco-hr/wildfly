{
  lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setCatalog(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,catalog);
      mc.getConnection().setCatalog(catalog);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
