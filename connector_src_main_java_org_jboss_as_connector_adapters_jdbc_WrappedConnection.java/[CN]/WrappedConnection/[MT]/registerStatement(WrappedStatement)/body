{
  if (trackStatements == BaseWrapperManagedConnectionFactory.TRACK_STATEMENTS_FALSE_INT)   return;
synchronized (this) {
    if (statements == null)     statements=new HashMap<WrappedStatement,Throwable>(1);
    if (trackStatements == BaseWrapperManagedConnectionFactory.TRACK_STATEMENTS_TRUE_INT)     statements.put(ws,new Throwable("STACKTRACE"));
 else     statements.put(ws,null);
  }
}
