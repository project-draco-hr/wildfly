{
  lock();
  try {
    Connection c=getUnderlyingConnection();
    try {
      if (spy)       spyLogger.debugf("%s [%s] getClientInfo(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,name);
      return c.getClientInfo(name);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
