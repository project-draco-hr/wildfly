{
  lock();
  try {
    Connection c=getUnderlyingConnection();
    try {
      if (spy)       spyLogger.debugf("%s [%s] isValid(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,timeout);
      return c.isValid(timeout);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
