{
  lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] releaseSavepoint(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,savepoint);
      mc.getConnection().releaseSavepoint(savepoint);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
