{
  lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] getMetaData()",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION);
      return mc.getConnection().getMetaData();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
