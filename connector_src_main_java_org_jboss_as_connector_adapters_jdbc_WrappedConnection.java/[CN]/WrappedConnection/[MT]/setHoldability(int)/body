{
  lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setHoldability(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,holdability);
      mc.getConnection().setHoldability(holdability);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
