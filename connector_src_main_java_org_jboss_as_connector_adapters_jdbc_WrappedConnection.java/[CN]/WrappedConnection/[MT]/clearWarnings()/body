{
  lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] clearWarnings()",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION);
      mc.getConnection().clearWarnings();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
