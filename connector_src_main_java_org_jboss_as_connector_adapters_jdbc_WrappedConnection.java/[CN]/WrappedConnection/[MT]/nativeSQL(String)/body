{
  lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] nativeSQL(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,sql);
      return mc.getConnection().nativeSQL(sql);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
