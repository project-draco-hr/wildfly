{
  lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setTypeMap(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,typeMap);
      mc.getConnection().setTypeMap(typeMap);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
