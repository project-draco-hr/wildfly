{
  lock();
  try {
    Connection c=getUnderlyingConnection();
    try {
      if (spy)       spyLogger.debugf("%s [%s] createArrayOf(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CONNECTION,typeName,Arrays.toString(elements));
      return c.createArrayOf(typeName,elements);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
