{
  ModelNode model=null;
  if (address != null) {
    model=operationControllerContext.getModelProvider().getModel();
synchronized (model) {
      model=model.clone();
    }
    if (isRemove) {
      address.remove(model);
    }
 else {
      address.navigate(model,true).set(subModel);
    }
  }
  return model;
}
