{
  int counter=0;
  final BufferedReader reader=new BufferedReader(new FileReader(file));
  try {
    String line=reader.readLine();
    while (line != null) {
      if (DATE_STAMP_PATTERN.matcher(line).find()) {
        counter++;
      }
      line=reader.readLine();
    }
  }
  finally {
    IoUtils.safeClose(reader);
  }
  return counter;
}
