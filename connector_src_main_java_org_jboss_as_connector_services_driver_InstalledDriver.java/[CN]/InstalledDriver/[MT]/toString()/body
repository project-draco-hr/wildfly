{
  StringBuilder sb=new StringBuilder();
  if (profile != null) {
    sb.append(profile).append(":");
  }
  if (moduleName != null) {
    sb.append(moduleName);
  }
 else {
    sb.append(deploymentUnitName);
  }
  sb.append(':');
  sb.append(driverClassName);
  sb.append('#');
  sb.append(majorVersion);
  sb.append('#');
  sb.append(minorVersion);
  return sb.toString();
}
