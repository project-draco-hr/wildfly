{
  serviceTarget.addService(serviceName,service).addDependency(TxnServices.JBOSS_TXN_TRANSACTION_MANAGER,service.transactionManager).addDependency(hqServiceName,HornetQServer.class,service.hornetQService).addDependency(HornetQActivationService.getHornetQActivationServiceName(hqServiceName)).addDependency(JMSServices.getJmsManagerBaseServiceName(hqServiceName)).setInitialMode(ServiceController.Mode.PASSIVE).install();
}
