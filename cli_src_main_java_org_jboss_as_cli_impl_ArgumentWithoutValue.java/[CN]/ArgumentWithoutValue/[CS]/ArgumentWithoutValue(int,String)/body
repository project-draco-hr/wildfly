{
  if (names == null || names.length < 1) {
    throw new IllegalArgumentException("There must be at least one non-null default name.");
  }
  this.defaultName=names[0];
  if (defaultName == null) {
    throw new IllegalArgumentException("There must be at least one non-null default name.");
  }
  this.names=names;
  this.index=index;
}
