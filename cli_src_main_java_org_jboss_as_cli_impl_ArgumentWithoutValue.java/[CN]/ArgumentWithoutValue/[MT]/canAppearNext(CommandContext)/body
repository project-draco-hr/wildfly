{
  ParsedOperationRequest args=ctx.getParsedArguments();
  if (exclusive) {
    return !args.hasProperties();
  }
  if (isPresent(args)) {
    return false;
  }
  for (  CommandArgument arg : cantAppearAfter) {
    if (arg.isPresent(args)) {
      return false;
    }
  }
  if (requiredPreceding != null) {
    for (    CommandArgument arg : requiredPreceding) {
      if (arg.isPresent(args)) {
        return true;
      }
    }
    return false;
  }
  return true;
}
