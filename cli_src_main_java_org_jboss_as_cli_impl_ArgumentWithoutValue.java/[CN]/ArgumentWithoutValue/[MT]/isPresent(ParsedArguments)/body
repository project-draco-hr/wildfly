{
  if (!args.hasArguments()) {
    return false;
  }
  if (index >= 0 && index < args.getOtherArguments().size()) {
    return true;
  }
  if (names != null && names.length > 0) {
    if (names.length == 1) {
      return args.hasArgument(names[0]);
    }
    for (    String name : names) {
      if (args.hasArgument(name)) {
        return true;
      }
    }
  }
  return false;
}
