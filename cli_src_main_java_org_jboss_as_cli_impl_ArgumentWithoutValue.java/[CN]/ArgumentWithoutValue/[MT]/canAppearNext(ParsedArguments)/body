{
  if (exclusive) {
    return !args.hasArguments();
  }
  if (isPresent(args)) {
    return false;
  }
  for (  CommandArgument arg : cantAppearAfter) {
    if (arg.isPresent(args)) {
      return false;
    }
  }
  if (requiredPreceding != null) {
    return requiredPreceding.isPresent(args);
  }
  return true;
}
