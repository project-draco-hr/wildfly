{
  builder.authentication().ldap().setConnection(MASTER_CONNECTION_NAME).setBaseDn(BASE_DN).setUsernameFilter(USERNAME_FILTER).build().build().authorization().ldap().setConnection(MASTER_CONNECTION_NAME).usernameFilter().setBaseDn(BASE_DN).setRecursive(false).setAttribute(USERNAME_FILTER).build().principalToGroup().setIterative(true).build().build().build();
}
