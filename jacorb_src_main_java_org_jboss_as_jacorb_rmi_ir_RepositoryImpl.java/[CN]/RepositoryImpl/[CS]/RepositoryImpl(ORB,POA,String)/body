{
  super(DefinitionKind.dk_Repository,null);
  this.orb=orb;
  this.poa=poa;
  try {
    oid=(name).getBytes("UTF-8");
  }
 catch (  UnsupportedEncodingException ex) {
    throw JacORBMessages.MESSAGES.unsupportedUTF8Encoding();
  }
  oidPrefix=name + ":";
  anonOidPrefix=oidPrefix + "anon";
  repository=this;
}
