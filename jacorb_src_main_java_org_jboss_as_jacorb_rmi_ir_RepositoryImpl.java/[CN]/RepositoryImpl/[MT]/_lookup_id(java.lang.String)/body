{
  logger.debug("RepositoryImpl._lookup_id(\"" + search_id + "\") entered.");
  if (search_id.startsWith("RMI:["))   return (ValueBoxDefImpl)sequenceIdMap.get(search_id);
  String name=scopedName(search_id);
  logger.debug("RepositoryImpl._lookup_id(): scopedName=\"" + scopedName(search_id) + "\"");
  LocalContained ret=(name == null) ? null : _lookup(name);
  logger.debug("RepositoryImpl._lookup_id(): returning " + ((ret == null) ? "null" : "NOT null"));
  return ret;
}
