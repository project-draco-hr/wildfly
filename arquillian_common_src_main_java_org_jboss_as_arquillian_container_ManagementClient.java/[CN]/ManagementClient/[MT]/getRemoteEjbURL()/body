{
  if (ejbUri == null) {
    if (rootNode == null) {
      try {
        readRootNode();
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
    String socketBinding=rootNode.get("subsystem").get("remoting").get("connector").get("remoting-connector").get("socket-binding").asString();
    ejbUri=getBinding("remote",socketBinding);
  }
  return ejbUri;
}
