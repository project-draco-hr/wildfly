{
  StandaloneServer server=null;
  try {
    ServerEnvironment config=determineEnvironment(args,props);
    if (config == null) {
      abort(null);
    }
 else {
      if (config.isStandalone()) {
        server=new StandaloneServer(config);
      }
 else {
        throw new IllegalStateException();
      }
      server.start();
      return server;
    }
  }
 catch (  Throwable t) {
    abort(t);
  }
  return null;
}
