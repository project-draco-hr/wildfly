{
  AbstractServer server=null;
  try {
    ServerEnvironment config=determineEnvironment(args,stdin,stdout,stderr);
    if (config == null) {
      abort(null);
    }
 else {
      if (config.isStandalone()) {
        server=new StandaloneServer(config);
        server.start(null);
      }
 else {
        server=new Server(config);
      }
    }
  }
 catch (  Throwable t) {
    abort(t);
  }
}
