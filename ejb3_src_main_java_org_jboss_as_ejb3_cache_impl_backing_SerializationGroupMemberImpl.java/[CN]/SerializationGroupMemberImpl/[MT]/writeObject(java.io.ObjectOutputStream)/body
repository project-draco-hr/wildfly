{
  try {
    if (groupId != null) {
      setGroup(null);
    }
    out.defaultWriteObject();
    if (groupId == null) {
      out.writeObject(value);
    }
  }
 catch (  IOException e) {
    e.printStackTrace(System.err);
    throw e;
  }
catch (  Throwable e) {
    e.printStackTrace(System.err);
    throw new IOException(e);
  }
}
