{
  try {
    lock.lockInterruptibly();
  }
 catch (  InterruptedException e) {
    throw EjbMessages.MESSAGES.lockAcquisitionInterrupted(e,this);
  }
  if (!groupLockHeld && group != null) {
    try {
      group.lock();
      groupLockHeld=true;
    }
  finally {
      if (!groupLockHeld)       lock.unlock();
    }
  }
}
