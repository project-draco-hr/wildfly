{
  boolean success=lock.tryLock();
  if (success) {
    success=(groupLockHeld || group == null);
    if (!success) {
      try {
        success=groupLockHeld=group.tryLock();
        if (!success) {
          log.tracef("Member %s cannot lock serialization group %s",id,groupId);
        }
      }
  finally {
        if (!success)         lock.unlock();
      }
    }
  }
  return success;
}
