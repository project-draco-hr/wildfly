{
  list.add(createAddOperation());
  while (reader.hasNext() && reader.nextTag() != END_ELEMENT) {
    final Element element=Element.forName(reader.getLocalName());
switch (element) {
case JPA:
{
        parseJPA(reader,list);
        break;
      }
default :
{
      throw ParseUtils.unexpectedElement(reader);
    }
}
}
}
