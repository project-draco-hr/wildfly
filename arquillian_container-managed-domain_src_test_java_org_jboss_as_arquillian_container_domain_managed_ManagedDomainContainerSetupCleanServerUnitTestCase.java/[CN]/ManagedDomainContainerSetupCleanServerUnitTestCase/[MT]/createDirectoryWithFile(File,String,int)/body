{
  File dir=new File(server,name);
  if (!dir.exists()) {
    dir.mkdirs();
  }
  File file=new File(dir,String.valueOf(id));
  try {
    file.createNewFile();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  Assert.assertTrue(file.exists());
  return dir.getAbsoluteFile();
}
