{
  ClassLoader classLoader=this.cache.getClassLoader();
  ContextClassLoaderSwitcher.SwitchContext context=(classLoader != null) ? switcher.getSwitchContext(classLoader) : null;
  try {
    return super.handleDefault(ctx,command);
  }
  finally {
    if (context != null) {
      context.reset();
    }
  }
}
