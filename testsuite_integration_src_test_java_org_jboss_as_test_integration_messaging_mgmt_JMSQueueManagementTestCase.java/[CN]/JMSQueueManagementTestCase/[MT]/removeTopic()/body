{
  if (conn != null) {
    conn.stop();
  }
  if (session != null) {
    session.close();
  }
  if (conn != null) {
    conn.close();
  }
  if (consumerConn != null) {
    consumerConn.stop();
  }
  if (consumerSession != null) {
    consumerSession.close();
  }
  if (consumerConn != null) {
    consumerConn.close();
  }
  adminSupport.removeJmsQueue(getQueueName());
  adminSupport.removeJmsQueue(getOtherQueueName());
}
