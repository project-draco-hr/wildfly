{
  context.startSubsystemElement(Namespace.CURRENT.getUri(),false);
  ModelNode node=context.getModelNode();
  if (node.get(MOD_CLUSTER_CONFIG).isDefined() && node.get(MOD_CLUSTER_CONFIG).has(CONFIGURATION))   writeModClusterConfig(writer,node.get(MOD_CLUSTER_CONFIG).get(CONFIGURATION));
 else   writeModClusterConfig(writer,node);
  writer.writeEndElement();
}
