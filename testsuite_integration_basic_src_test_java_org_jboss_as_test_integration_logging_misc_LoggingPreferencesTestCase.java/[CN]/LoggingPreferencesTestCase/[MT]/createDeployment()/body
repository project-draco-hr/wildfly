{
  JavaArchive jar=ShrinkWrap.create(JavaArchive.class);
  jar.addClasses(LoggingBean.class);
  jar.addAsManifestResource(EmptyAsset.INSTANCE,"beans.xml");
  jar.addAsResource(LoggingBean.class.getPackage(),"jboss-logging.properties","META-INF/jboss-logging.properties");
  jar.setManifest(new StringAsset(Descriptors.create(ManifestDescriptor.class).attribute("Logging-Profile","non-existing-profile").exportAsString()));
  return jar;
}
