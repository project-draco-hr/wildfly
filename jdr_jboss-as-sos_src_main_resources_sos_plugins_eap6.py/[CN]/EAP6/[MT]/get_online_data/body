def get_online_data(self):
    '\n        This function co-locates calls to the management api that gather\n        information from a running system.\n        '
    for (caller, outfile) in [(Request(resource='/', parameters={'recursive': 'true', }), 'configuration.json'), (Request(resource='/core-service/service-container', operation='dump-services'), 'dump-services.json'), (Request(resource='/subsystem/modcluster', operation='read-proxies-configuration'), 'cluster-proxies-configuration.json')]:
        self.addStringAsFile(self.query(caller), filename=outfile)
