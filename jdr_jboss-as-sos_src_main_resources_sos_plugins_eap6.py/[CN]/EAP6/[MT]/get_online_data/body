def get_online_data(self):
    '\n        This function co-locates calls to the management api that gather\n        information from a running system.\n        '
    for (caller, outfile) in [(Request(resource='/core-service/platform-mbean/type/threading', operation='dump-all-threads', parameters={'locked-synchronizers': 'true', 'locked-monitors': 'true', }), 'threaddump.json'), (Request(resource='/', parameters={'recursive': 'true', }), 'configuration.json')]:
        self.addStringAsFile(self.query(caller), filename=outfile)
