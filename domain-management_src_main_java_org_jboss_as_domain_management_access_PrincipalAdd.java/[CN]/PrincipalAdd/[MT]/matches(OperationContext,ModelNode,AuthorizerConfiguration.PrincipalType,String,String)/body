{
  final AuthorizerConfiguration.PrincipalType valuePrincipalType=PrincipalResourceDefinition.getPrincipalType(context,value);
  if (principalType != valuePrincipalType) {
    return false;
  }
  final String valueName=PrincipalResourceDefinition.getName(context,value);
  if (name.equals(valueName) == false) {
    return false;
  }
  final String valueRealm=PrincipalResourceDefinition.getRealm(context,value);
  if ((realm == null && valueRealm != null) || (realm != null && realm.equals(valueRealm) == false)) {
    return false;
  }
  return true;
}
