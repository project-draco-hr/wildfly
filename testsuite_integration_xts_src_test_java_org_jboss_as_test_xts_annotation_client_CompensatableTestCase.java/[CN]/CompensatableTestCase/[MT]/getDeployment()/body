  final WebArchive webArchive=ShrinkWrap.create(WebArchive.class,DEPLOYMENT_NAME + ".war").addClass(CompensatableClient.class).addClass(CompensatableService.class).addClass(CompensatableServiceImpl.class).addAsWebInfResource(EmptyAsset.INSTANCE,ArchivePaths.create("beans.xml"));
