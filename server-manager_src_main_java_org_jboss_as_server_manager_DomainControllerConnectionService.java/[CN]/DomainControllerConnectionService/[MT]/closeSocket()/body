{
  if (connected.compareAndSet(true,false)) {
    if (socket == null)     return;
    try {
      socket.shutdownOutput();
    }
 catch (    IOException ignored) {
    }
    try {
      socket.shutdownInput();
    }
 catch (    IOException ignored) {
    }
    try {
      socket.close();
    }
 catch (    IOException ignored) {
    }
  }
}
