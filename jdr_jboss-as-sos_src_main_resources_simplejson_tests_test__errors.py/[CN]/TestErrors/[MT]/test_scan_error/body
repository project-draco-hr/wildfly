def test_scan_error(self):
    err = None
    for t in (str, unicode):
        try:
            json.loads(t('{"asdf": "'))
        except json.JSONDecodeError as e:
            err = e
        else:
            self.fail('Expected JSONDecodeError')
        self.assertEquals(err.lineno, 1)
        self.assertEquals(err.colno, 9)
