{
  super(processType,runningModeControl,persister,processState == null ? new ControlledProcessState(true) : processState,rootDescriptionProvider,null,ExpressionResolver.DEFAULT);
  this.persister=persister;
  this.transformerRegistry=transformerRegistry;
  this.validateOpsFilter=validateOpsFilter;
  this.runningModeControl=runningModeControl;
}
