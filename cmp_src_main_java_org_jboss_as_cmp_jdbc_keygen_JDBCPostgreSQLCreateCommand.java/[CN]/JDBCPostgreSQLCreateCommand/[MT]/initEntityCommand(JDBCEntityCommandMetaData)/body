{
  super.initEntityCommand(entityCommand);
  sequence=entityCommand.getAttribute("sequence");
  if (sequence == null) {
    sequence=entity.getQualifiedTableName() + '_' + SQLUtil.getColumnNamesClause(pkField,new StringBuffer(20))+ "_seq";
  }
  sequenceSQL="SELECT currval('" + sequence + "')";
  if (debug) {
    log.debug("SEQUENCE SQL is :" + sequenceSQL);
  }
}
