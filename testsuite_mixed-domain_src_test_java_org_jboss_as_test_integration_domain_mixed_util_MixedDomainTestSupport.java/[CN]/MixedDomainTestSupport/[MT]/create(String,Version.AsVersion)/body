{
  OldVersionCopier oldVersionCopier=OldVersionCopier.expandOldVersions();
  File dir=oldVersionCopier.getVersionDir(version);
  return new MixedDomainTestSupport(testClass,"master-config/domain.xml","master-config/host.xml","slave-configs/" + dir.getName() + "/domain/configuration/host-slave.xml",dir.getAbsolutePath());
}
