{
  try {
    for (int i=0; i < futures.length; i++) {
      try {
        futures[i].get(timeout,TimeUnit.MILLISECONDS);
      }
 catch (      ExecutionException e) {
        throw e.getCause();
      }
    }
  }
 catch (  Exception e) {
    throw e;
  }
catch (  Error e) {
    throw e;
  }
catch (  Throwable t) {
    throw new RuntimeException(t);
  }
}
