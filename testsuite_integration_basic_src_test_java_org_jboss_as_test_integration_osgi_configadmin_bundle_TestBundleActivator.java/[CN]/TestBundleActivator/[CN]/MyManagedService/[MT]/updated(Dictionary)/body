{
  if (props == null)   return;
  String f=(String)props.get("file");
  if (f == null)   return;
  String v=(String)props.get("value");
  if (v == null)   return;
  OutputStream fos=null;
  try {
    fos=new FileOutputStream(f);
    fos.write(v.getBytes());
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
 finally {
    if (fos != null) {
      try {
        fos.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
}
