{
  Writer writer=resp.getWriter();
  try {
    Connection con=bean.getDataSource().getConnection();
    DatabaseMetaData metaData=con.getMetaData();
    writer.write(metaData.getDriverName());
    con.close();
  }
 catch (  Exception ex) {
    writer.write(ex.toString());
    ex.printStackTrace();
  }
  writer.close();
}
