{
  if (!isPresent(args)) {
    return false;
  }
  if (index >= 0 && index < args.getOtherProperties().size()) {
    return true;
  }
  if (fullName.equals(args.getLastParsedPropertyName())) {
    return false;
  }
  if (shortName != null && shortName.equals(args.getLastParsedPropertyName())) {
    return false;
  }
  return true;
}
