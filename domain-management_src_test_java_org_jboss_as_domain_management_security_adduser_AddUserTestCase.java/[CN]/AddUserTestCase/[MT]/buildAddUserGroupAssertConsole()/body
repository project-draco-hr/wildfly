{
  AssertConsoleBuilder consoleBuilder=new AssertConsoleBuilder().expectedDisplayText(DomainManagementLogger.ROOT_LOGGER.addedUser(values.getUserName(),values.getUserFiles().get(0).getCanonicalPath())).expectedDisplayText(AddUser.NEW_LINE).expectedDisplayText(DomainManagementLogger.ROOT_LOGGER.addedGroups(values.getUserName(),values.getGroups(),values.getGroupFiles().get(0).getCanonicalPath())).expectedDisplayText(AddUser.NEW_LINE);
  consoleMock.setResponses(consoleBuilder);
  return consoleBuilder;
}
