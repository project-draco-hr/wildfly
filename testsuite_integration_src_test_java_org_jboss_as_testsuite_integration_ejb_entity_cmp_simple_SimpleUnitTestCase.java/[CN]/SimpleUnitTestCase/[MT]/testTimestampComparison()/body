{
  SimpleHome simpleHome=getSimpleHome();
  Collection c;
  Timestamp timestampBefore=new Timestamp(1111);
  Timestamp timestampAfter=new Timestamp(8888);
  c=simpleHome.selectDynamic("SELECT OBJECT(s) " + "FROM simple s " + "WHERE s.timestampValue >= ?1 AND s.timestampValue <= ?2",new Object[]{timestampBefore,timestampAfter});
  assertTrue(c.size() == 1);
}
