{
  SimpleHome simpleHome=getSimpleHome();
  Collection c;
  java.util.Date utilDateBefore=new java.util.Date(100);
  java.util.Date utilDateAfter=new java.util.Date(2000);
  c=simpleHome.selectDynamic("SELECT OBJECT(s) " + "FROM simple s " + "WHERE s.utilDateValue BETWEEN ?1 AND ?2",new Object[]{utilDateBefore,utilDateAfter});
  assertTrue(c.size() == 1);
}
