{
  File bundleXMLFile=new File(dir,"patches.xml");
  FileOutputStream fos=new FileOutputStream(bundleXMLFile);
  try {
    PatchBundleXml.marshal(fos,new BundledPatch(){
      @Override public List<BundledPatchEntry> getPatches(){
        return patches;
      }
    }
);
  }
  finally {
    safeClose(fos);
  }
}
