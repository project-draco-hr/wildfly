{
  try {
    final long firstTry=System.currentTimeMillis();
    HttpRequest.get(spec,10,TimeUnit.SECONDS);
    while (System.currentTimeMillis() - firstTry <= 1000) {
      try {
        Thread.sleep(500);
      }
 catch (      InterruptedException e) {
        break;
      }
 finally {
        HttpRequest.get(spec,10,TimeUnit.SECONDS);
      }
    }
    Assert.fail(spec + " is still available.");
  }
 catch (  Exception e) {
  }
}
