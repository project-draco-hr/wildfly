{
  StatelessObjectFactory<MockBean> factory=new MockFactory();
  ThreadLocalPool<MockBean> pool=new ThreadLocalPool<MockBean>(factory);
  pool.start();
  for (int i=0; i < 10; i++) {
    MockBean bean=pool.get();
    pool.release(bean);
    bean=null;
  }
  assertEquals(1,MockBean.getPostConstructs());
}
