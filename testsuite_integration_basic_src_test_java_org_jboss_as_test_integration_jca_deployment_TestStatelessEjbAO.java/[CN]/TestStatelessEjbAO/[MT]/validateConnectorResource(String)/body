{
  boolean rval=false;
  try {
    InitialContext ctx=new InitialContext();
    Object obj=ctx.lookup(jndiName);
    if (obj == null) {
      rval=false;
    }
 else {
      rval=true;
    }
  }
 catch (  Exception e) {
    debug("Fail to access connector resource: " + jndiName);
    e.printStackTrace();
  }
  return rval;
}
