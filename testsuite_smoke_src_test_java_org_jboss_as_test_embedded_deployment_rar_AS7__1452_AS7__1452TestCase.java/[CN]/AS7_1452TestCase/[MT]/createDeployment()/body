{
  String deploymentName="as7_1452.rar";
  ResourceAdapterArchive raa=ShrinkWrap.create(ResourceAdapterArchive.class,deploymentName);
  JavaArchive ja=ShrinkWrap.create(JavaArchive.class,UUID.randomUUID().toString() + ".jar");
  ja.addClasses(ConfigPropertyResourceAdapter.class,ConfigPropertyManagedConnectionFactory.class,ConfigPropertyManagedConnection.class,ConfigPropertyConnectionFactory.class,ConfigPropertyManagedConnectionMetaData.class,ConfigPropertyConnectionFactoryImpl.class,ConfigPropertyConnection.class,ConfigPropertyConnectionImpl.class,ConfigPropertyAdminObjectImpl.class,ConfigPropertyAdminObjectInterface.class,AS7_1452TestCase.class);
  raa.addAsLibrary(ja);
  raa.addAsManifestResource("rar/" + deploymentName + "/META-INF/ra.xml","ra.xml");
  return raa;
}
