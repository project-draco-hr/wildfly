{
  try {
    AccessAuditContext.doAs(subject,new PrivilegedAction<Void>(){
      @Override public Void run(){
        execute();
        return null;
      }
    }
);
  }
 catch (  Throwable t) {
    DomainControllerLogger.ROOT_LOGGER.debugf(t,"failed to process task %s",tasks.iterator().next().getOperation());
  }
}
