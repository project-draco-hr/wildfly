{
  if (blocking) {
    return create(CommonAttributes.BLOCKING_QUEUELESS_THREAD_POOL,ThreadsServices.STANDARD_THREAD_FACTORY_RESOLVER,null,ThreadsServices.EXECUTOR,registerRuntimeOnly);
  }
 else {
    return create(CommonAttributes.QUEUELESS_THREAD_POOL,ThreadsServices.STANDARD_THREAD_FACTORY_RESOLVER,ThreadsServices.STANDARD_HANDOFF_EXECUTOR_RESOLVER,ThreadsServices.EXECUTOR,registerRuntimeOnly);
  }
}
