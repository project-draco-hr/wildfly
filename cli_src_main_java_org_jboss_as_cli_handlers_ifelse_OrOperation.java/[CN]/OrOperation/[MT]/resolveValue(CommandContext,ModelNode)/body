{
  for (  Operand operand : getOperands()) {
    final Object value=operand.resolveValue(ctx,response);
    if (!(value instanceof Boolean)) {
      throw new CommandFormatException("Expected boolean value from " + operand + " but received "+ value);
    }
    if (((Boolean)value)) {
      return true;
    }
  }
  return false;
}
