{
  KernelServices kernelServices=createKernelServicesBuilder(TestModelType.STANDALONE).setXmlResource("ssl-standalone.xml").validateDescription().build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),"ssl-standalone.xml"),marshalled);
}
