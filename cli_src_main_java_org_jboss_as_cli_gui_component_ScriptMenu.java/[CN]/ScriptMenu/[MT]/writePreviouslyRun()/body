{
  int i=0;
  for (  File file : previouslyRun) {
    if (!file.exists())     continue;
    try {
      prefs.put("Script" + i,file.getCanonicalPath());
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
    i++;
  }
  for (int j=i; j < SCRIPT_LIST_SIZE; j++) {
    prefs.put("Script" + j,"");
  }
}
