{
  Batcher batcher=this.manager.getBatcher();
  boolean started=batcher.startBatch();
  try {
    Session<LocalSessionContext> session=this.manager.findSession(this.getSessionId(sessionId));
    if (session != null) {
      session.invalidate();
    }
  }
  finally {
    if (started) {
      batcher.endBatch(true);
    }
  }
}
