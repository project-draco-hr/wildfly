{
  Enumeration<String> aliases=keyStore.aliases();
  StringBuilder sb=new StringBuilder("{");
  while (aliases.hasMoreElements()) {
    String current=aliases.nextElement();
    if (keyStore.isKeyEntry(current)) {
      if (sb.length() > 1) {
        sb.append(", ");
      }
      sb.append(current);
    }
  }
  return sb.append("}").toString();
}
