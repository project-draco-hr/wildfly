{
  String type=event.getType();
  if (type.equals(InstanceEvent.BEFORE_FILTER_EVENT) || type.equals(InstanceEvent.BEFORE_DESTROY_EVENT) || type.equals(InstanceEvent.BEFORE_INIT_EVENT)) {
    selector.activate();
  }
 else   if (type.equals(InstanceEvent.AFTER_FILTER_EVENT) || type.equals(InstanceEvent.AFTER_DESTROY_EVENT) || type.equals(InstanceEvent.AFTER_INIT_EVENT)) {
    selector.deactivate();
  }
}
