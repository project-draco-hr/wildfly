{
  final SubsystemRegistration subsystem=context.registerSubsystem(SUBSYSTEM_NAME);
  final ModelNodeRegistration registration=subsystem.registerSubsystemModel(NewEJB3SubsystemProviders.SUBSYSTEM);
  registration.registerOperationHandler(ADD,NewEjb3SubsystemAdd.INSTANCE,NewEJB3SubsystemProviders.SUBSYSTEM_ADD,false);
  subsystem.registerXMLElementWriter(parser);
}
