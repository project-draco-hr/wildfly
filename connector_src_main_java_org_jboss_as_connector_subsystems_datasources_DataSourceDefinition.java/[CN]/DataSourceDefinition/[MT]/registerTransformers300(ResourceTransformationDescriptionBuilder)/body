{
  ResourceTransformationDescriptionBuilder builder=parentBuilder.addChildResource(PATH_DATASOURCE);
  builder.getAttributeBuilder().setDiscard(new DiscardAttributeChecker.DiscardAttributeValueChecker(new ModelNode(true)),ENLISTMENT_TRACE).setDiscard(new DiscardAttributeChecker.DiscardAttributeValueChecker(new ModelNode(LEGACY_MCP)),MCP).addRejectCheck(RejectAttributeChecker.DEFINED,ENLISTMENT_TRACE).addRejectCheck(RejectAttributeChecker.DEFINED,MCP).end();
}
