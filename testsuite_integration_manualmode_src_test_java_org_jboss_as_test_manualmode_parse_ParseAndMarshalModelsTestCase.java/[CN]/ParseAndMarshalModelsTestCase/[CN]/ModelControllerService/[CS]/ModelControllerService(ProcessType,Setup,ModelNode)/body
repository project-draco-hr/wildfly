{
  super(processType,new RunningModeControl(RunningMode.ADMIN_ONLY),new NullConfigurationPersister(),new ControlledProcessState(true),ResourceBuilder.Factory.create(PathElement.pathElement("root"),new NonResolvingResourceDescriptionResolver()).build(),null,ExpressionResolver.TEST_RESOLVER,AuditLogger.NO_OP_LOGGER);
