{
  try {
    context.execute(new Runnable(){
      @Override public void run(){
        try {
          cacheImplementation.clearAll();
          cacheImplementation=null;
          if (executorService != null) {
            executorService.shutdown();
            executorService=null;
          }
        }
  finally {
          context.complete();
        }
      }
    }
);
  }
  finally {
    context.asynchronous();
  }
}
