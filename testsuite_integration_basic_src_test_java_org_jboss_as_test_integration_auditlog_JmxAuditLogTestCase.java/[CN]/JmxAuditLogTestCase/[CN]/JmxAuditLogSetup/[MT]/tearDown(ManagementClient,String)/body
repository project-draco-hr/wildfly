  op=new ModelNode();
  op.get(OP).set(REMOVE);
  op.get(OP_ADDR).add(SUBSYSTEM,JMX);
  op.get(OP_ADDR).add(CONFIGURATION,AUDIT_LOG);
  applyUpdate(managementClient.getControllerClient(),op,false);
  if (logFile.exists()) {
    logFile.delete();
  }
}
