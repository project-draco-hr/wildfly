{
  ConfigurationBuilder builder=super.createConfigurationBuilder();
  GroupsConfigurationBuilder groupsBuilder=builder.clustering().hash().read(this.hash).consistentHashFactory(this.consistentHashStrategy.createConsistentHashFactory(this.container.getValue().transport().hasTopologyInfo())).groups().enabled();
  for (  Grouper<?> grouper : ServiceLoader.load(Grouper.class,this.getClassLoader())) {
    groupsBuilder.addGrouper(grouper);
  }
  return builder;
}
