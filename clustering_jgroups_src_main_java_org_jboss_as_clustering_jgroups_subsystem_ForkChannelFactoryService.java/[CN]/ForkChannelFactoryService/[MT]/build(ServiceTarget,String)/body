{
  ForkChannelFactoryService service=new ForkChannelFactoryService();
  return target.addService(ChannelFactoryService.getServiceName(channel),service).addDependency(ConnectedChannelService.getServiceName(channel),Channel.class,service.parentChannel).addDependency(ChannelService.getFactoryServiceName(channel),ChannelFactory.class,service.parentFactory);
}
