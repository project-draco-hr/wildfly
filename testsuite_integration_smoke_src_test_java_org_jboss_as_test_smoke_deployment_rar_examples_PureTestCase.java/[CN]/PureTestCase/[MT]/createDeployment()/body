{
  setUp();
  String deploymentName="pure.rar";
  ResourceAdapterArchive raa=ShrinkWrap.create(ResourceAdapterArchive.class,deploymentName);
  JavaArchive ja=ShrinkWrap.create(JavaArchive.class,"multiple.jar");
  ja.addClasses(PureInflowResourceAdapter.class,PureTestCase.class,AbstractMgmtTestBase.class,MgmtOperationException.class,XMLElementReader.class,XMLElementWriter.class);
  raa.addAsLibrary(ja);
  raa.addAsManifestResource("rar/" + deploymentName + "/META-INF/ra.xml","ra.xml").addAsManifestResource(new StringAsset("Dependencies: org.jboss.as.controller-client,org.jboss.dmr,org.jboss.as.cli,javax.inject.api,org.jboss.as.connector\n"),"MANIFEST.MF");
  return raa;
}
