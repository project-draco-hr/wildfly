{
  File resolved=null;
  for (  final File root : mp) {
    final File moduleRoot=PatchContentLoader.getModulePath(root,item);
    final File moduleXml=new File(moduleRoot,"module.xml");
    if (moduleXml.exists()) {
      resolved=moduleRoot;
      break;
    }
  }
  Assert.assertEquals(item.toString(),reference,resolved);
}
