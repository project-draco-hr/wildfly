{
  this.definition=new StringListAttributeDefinition.Builder(name).setAttributeParser(AttributeParsers.COLLECTION).setCapabilityReference(new CapabilityReference(RequiredCapability.OUTBOUND_SOCKET_BINDING,Capability.OUTBOUND_SOCKET_BINDING)).setFlags(AttributeAccess.Flag.RESTART_RESOURCE_SERVICES).setMinSize(1).build();
}
