{
  if (addedContent == null)   throw new IllegalArgumentException("addedContent is null");
  if (removedContent == null)   throw new IllegalArgumentException("removedContent is null");
  if (deploymentSets == null)   throw new IllegalArgumentException("deploymentSets is null");
  this.addedContent.addAll(addedContent);
  this.removedContent.addAll(removedContent);
  this.deploymentSets.addAll(deploymentSets);
  this.globalRollback=globalRollback;
}
