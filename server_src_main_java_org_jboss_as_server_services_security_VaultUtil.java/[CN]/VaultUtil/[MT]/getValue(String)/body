{
  if (!isVaultFormat(vaultString))   throw new IllegalArgumentException("vaultString is of the wrong format:" + vaultString);
  String[] tokens=tokens(vaultString);
  SecurityVault vault=SecurityVaultFactory.get();
  if (!vault.isInitialized())   throw new SecurityVaultException("Vault is not initialized");
  return vault.retrieve(tokens[1],tokens[2],tokens[3].getBytes());
}
