{
  try {
    if (validateOps) {
      new OperationValidator(rootRegistration).validateOperations(bootOperations);
    }
    super.boot(persister.bootOperations);
  }
 catch (  Exception e) {
    error=e;
  }
catch (  Throwable t) {
    error=new Exception(t);
  }
 finally {
    latch.countDown();
  }
}
