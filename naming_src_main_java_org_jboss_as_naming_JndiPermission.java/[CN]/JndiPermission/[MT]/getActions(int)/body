{
  StringBuilder sb=new StringBuilder();
  boolean insertComma=false;
  for (int n=0; n < ACTION_COUNT; n++) {
    int action=1 << n;
    if ((mask & action) == action) {
      if (insertComma)       sb.append(',');
      sb.append(ALL_ACTIONS[n]);
      insertComma=true;
    }
  }
  return sb.toString();
}
