{
  for (  InterfaceCriteria current : criteria) {
    Validation validation=getValidation(current);
    if (validation == null) {
      continue;
    }
    for (    InterfaceCriteria candidate : criteria) {
      if (current == candidate) {
        continue;
      }
      String error=validation.validate(current,candidate);
      if (error != null) {
        return error;
      }
    }
  }
  return null;
}
