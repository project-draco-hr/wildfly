{
  try {
    theKeyManagers=createKeyManagers();
  }
 catch (  UnrecoverableKeyException e) {
    throw DomainManagementLogger.ROOT_LOGGER.unableToStart(e);
  }
catch (  NoSuchAlgorithmException e) {
    throw DomainManagementLogger.ROOT_LOGGER.unableToStart(e);
  }
catch (  KeyStoreException e) {
    throw DomainManagementLogger.ROOT_LOGGER.unableToStart(e);
  }
}
