{
  int chainSize=0;
  for (int index=0; index < this.passwordLength - 1; index++) {
    if (this.keyboard.siblings(this.password,index)) {
      chainSize+=this.keyboard.sequence(this.password,index);
    }
 else {
    }
  }
  if (chainSize > 0) {
    this.result.negative(chainSize * SEQUENTIAL_WEIGHT);
  }
}
