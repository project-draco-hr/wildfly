{
  CallableStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] getSQLXML(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterIndex);
    return statement.getSQLXML(parameterIndex);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
