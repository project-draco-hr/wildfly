{
  checkState();
  try {
    if (spy)     spyLogger.debugf("%s [%s] registerOutParameter(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName,sqlType,scale);
    cs.registerOutParameter(parameterName,sqlType,scale);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
