{
  CallableStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] setNCharacterStream(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName,value,length);
    statement.setNCharacterStream(parameterName,value,length);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
