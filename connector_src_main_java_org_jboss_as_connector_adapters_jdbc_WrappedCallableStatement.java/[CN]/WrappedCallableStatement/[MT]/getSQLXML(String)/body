{
  CallableStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] getSQLXML(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName);
    return statement.getSQLXML(parameterName);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
