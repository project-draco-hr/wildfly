{
  CallableStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] getNString(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterIndex);
    return statement.getNString(parameterIndex);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
