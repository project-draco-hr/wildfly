{
  checkState();
  try {
    if (spy)     spyLogger.debugf("%s [%s] setObject(%s, %s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName,value,sqlType,scale);
    cs.setObject(parameterName,value,sqlType,scale);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
