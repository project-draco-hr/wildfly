{
  CallableStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] setNClob(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName,value);
    statement.setNClob(parameterName,value);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
