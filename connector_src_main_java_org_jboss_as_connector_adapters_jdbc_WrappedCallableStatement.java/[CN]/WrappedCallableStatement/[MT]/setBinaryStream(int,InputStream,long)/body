{
  PreparedStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] setBinaryStream(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterIndex,x,length);
    statement.setBinaryStream(parameterIndex,x,length);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
