{
  CallableStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] getNCharacterStream(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterIndex);
    return statement.getNCharacterStream(parameterIndex);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
