{
  checkState();
  try {
    if (spy)     spyLogger.debugf("%s [%s] registerOutParameter(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName,sqlType,typeName);
    cs.registerOutParameter(parameterName,sqlType,typeName);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
