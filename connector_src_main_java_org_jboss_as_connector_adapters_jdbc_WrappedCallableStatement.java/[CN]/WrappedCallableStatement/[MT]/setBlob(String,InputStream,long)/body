{
  CallableStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] setBlob(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName,inputStream,length);
    statement.setBlob(parameterName,inputStream,length);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
