{
  checkState();
  try {
    if (spy)     spyLogger.debugf("%s [%s] setBinaryStream(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterName,stream,length);
    cs.setBinaryStream(parameterName,stream,length);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
