{
  PreparedStatement statement=getUnderlyingStatement();
  try {
    if (spy)     spyLogger.debugf("%s [%s] setAsciiStream(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_CALLABLE_STATEMENT,parameterIndex,x);
    statement.setAsciiStream(parameterIndex,x);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
