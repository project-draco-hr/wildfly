{
  if (startLatch != null) {
    startLatch.countDown();
  }
  try {
    if (timeout > 0) {
      awaitlatch.await(timeout,TimeUnit.MILLISECONDS);
    }
 else {
      awaitlatch.await();
    }
    if (toThrow != null) {
      throw toThrow;
    }
    return answer;
  }
  finally {
    if (endLatch != null) {
      endLatch.countDown();
    }
  }
}
