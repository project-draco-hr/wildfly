{
  int level=0;
  while (cl != null) {
    String msg="Servlet ClassLoader[" + level + "]: "+ cl.getClass().getName()+ ':'+ cl.hashCode();
    out.println(msg);
    URL[] urls=getClassLoaderURLs(cl);
    msg="  URLs:";
    out.println(msg);
    for (int u=0; u < urls.length; u++) {
      msg="  [" + u + "] = "+ urls[u];
      out.println(msg);
    }
    cl=cl.getParent();
    level++;
  }
}
