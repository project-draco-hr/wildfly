{
  if (System.getSecurityManager() != null) {
    AccessController.doPrivileged(new PrivilegedAction<Void>(){
      @Override public Void run(){
        SecurityContext sc=getSecurityContext();
        if (sc != null) {
          sc.getData().put(AUTH_EXCEPTION_KEY,null);
        }
        return null;
      }
    }
);
  }
 else {
    SecurityContext sc=getSecurityContext();
    if (sc != null) {
      sc.getData().put(AUTH_EXCEPTION_KEY,null);
    }
  }
}
