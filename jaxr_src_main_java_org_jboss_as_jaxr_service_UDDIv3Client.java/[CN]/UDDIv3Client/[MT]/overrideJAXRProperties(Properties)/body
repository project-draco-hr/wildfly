{
  properties.setProperty(JAXRConstants.QUERYMANAGER,"org.apache.juddi.v3.client.transport.wrapper.UDDIInquiryService#inquire");
  properties.setProperty(JAXRConstants.LIFECYCLEMANAGER,"org.apache.juddi.v3.client.transport.wrapper.UDDIPublicationService#publish");
  properties.setProperty(JAXRConstants.SECURITYMANAGER,"org.apache.juddi.v3.client.transport.wrapper.UDDISecurityService#secure");
  if (!properties.containsKey(JAXRConstants.UDDI_NAMESPACE_PROPERTY_NAME))   properties.setProperty(JAXRConstants.UDDI_NAMESPACE_PROPERTY_NAME,"urn:uddi-org:api_v3");
  properties.setProperty(JAXRConstants.JAXR_FACTORY_IMPLEMENTATION,"org.apache.ws.scout.transport.LocalTransport");
  SecurityActions.setSystemProperty(MANAGER_NAME,"jaxr");
  SecurityActions.setSystemProperty(NODE_NAME,"jaxr-client");
}
