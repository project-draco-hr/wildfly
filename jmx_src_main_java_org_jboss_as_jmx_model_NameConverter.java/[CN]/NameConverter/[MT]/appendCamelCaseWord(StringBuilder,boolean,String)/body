{
  if (parts.length == 1) {
    if (!isStart) {
      sb.append(Character.toUpperCase(parts[0].charAt(0)));
      sb.append(parts[0].substring(1));
    }
 else {
      sb.append(parts[0]);
    }
  }
 else {
    for (int i=0; i < parts.length; i++) {
      final boolean isCurrentStart=isStart && i == 0;
      appendCamelCaseWord(sb,isCurrentStart,parts[i]);
    }
  }
}
