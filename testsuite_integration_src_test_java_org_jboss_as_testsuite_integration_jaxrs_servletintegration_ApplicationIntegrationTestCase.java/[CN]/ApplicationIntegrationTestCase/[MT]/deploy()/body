{
  WebArchive war=ShrinkWrap.create(WebArchive.class,"jaxrsapp.war");
  war.addClasses(ApplicationIntegrationTestCase.class,HelloWorldResource.class,HelloWorldApplication.class);
  war.addWebResource(WebXml.get("<servlet-mapping>\n" + "        <servlet-name>" + HelloWorldApplication.class.getName() + "</servlet-name>\n"+ "        <url-pattern>/hello/*</url-pattern>\n"+ "    </servlet-mapping>\n"+ "\n"),"web.xml");
  return war;
}
