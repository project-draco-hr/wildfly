{
  streamWriter.writeComment("!!! NOTE !!!\n\n" + "This file is generated and managed by the\n" + "Domain Controller and should only be edited when\n"+ "it is offline.");
  streamWriter.writeStartElement(NAMESPACE_1_0,"domain");
  final String id=getId();
  if (id != null)   streamWriter.writeAttribute("id",id);
  streamWriter.writeEndElement();
}
