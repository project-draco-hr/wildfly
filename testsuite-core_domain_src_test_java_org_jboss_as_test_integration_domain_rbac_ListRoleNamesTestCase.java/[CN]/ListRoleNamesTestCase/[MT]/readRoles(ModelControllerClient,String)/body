{
  ModelNode operation=Util.createOperation(READ_ATTRIBUTE_OPERATION,pathAddress(pathElement(CORE_SERVICE,MANAGEMENT),pathElement(ACCESS,ModelDescriptionConstants.AUTHORIZATION)));
  operation.get(NAME).set(attribute);
  ModelNode result=RbacUtil.executeOperation(client,operation,Outcome.SUCCESS);
  Set<String> roles=new HashSet<String>();
  for (  ModelNode roleNode : result.get(RESULT).asList()) {
    roles.add(roleNode.asString());
  }
  return roles;
}
