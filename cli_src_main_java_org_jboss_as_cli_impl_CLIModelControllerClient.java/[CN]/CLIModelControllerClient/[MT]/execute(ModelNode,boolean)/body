{
  final ModelNode response=super.execute(operation);
  if (!Util.isSuccess(response)) {
    return response;
  }
  if (awaitClose) {
synchronized (lock) {
      if (strategy != null) {
        try {
          lock.wait(5000);
        }
 catch (        InterruptedException e) {
        }
        StreamUtils.safeClose(strategy);
        strategy=null;
      }
    }
  }
  return response;
}
