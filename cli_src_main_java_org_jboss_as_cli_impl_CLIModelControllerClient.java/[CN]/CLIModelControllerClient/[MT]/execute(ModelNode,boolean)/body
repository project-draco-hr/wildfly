{
  final ModelNode response=super.execute(operation);
  if (!Util.isSuccess(response)) {
    return response;
  }
  if (awaitClose) {
synchronized (this) {
      try {
        if (strategy == null) {
          throw new IOException("Connection has been closed.");
        }
        strategy.getChannel().getConnection().awaitClosed();
      }
 catch (      InterruptedException e) {
      }
    }
  }
  return response;
}
