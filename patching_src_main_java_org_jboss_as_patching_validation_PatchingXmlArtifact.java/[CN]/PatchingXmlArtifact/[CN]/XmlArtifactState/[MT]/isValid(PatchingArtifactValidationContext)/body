{
  if (patch != null) {
    return true;
  }
  try {
    final PatchMetadataResolver resolver=PatchXml.parse(xmlFile,context.getCurrentPatchIdentity());
    patch=artifact.resolveMetaData(resolver);
    return true;
  }
 catch (  Exception e) {
    context.getErrorHandler().addError(artifact,this);
  }
  return false;
}
