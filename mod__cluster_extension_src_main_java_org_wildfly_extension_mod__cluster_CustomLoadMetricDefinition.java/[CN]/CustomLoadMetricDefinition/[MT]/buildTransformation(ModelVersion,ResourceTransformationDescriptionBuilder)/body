{
  if (ModClusterModel.VERSION_1_2_0.requiresTransformation(version)) {
    builder.addChildResource(ModClusterExtension.CUSTOM_LOAD_METRIC_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,CLASS,WEIGHT).addRejectCheck(CapacityCheckerAndConverter.INSTANCE,CAPACITY).setValueConverter(CapacityCheckerAndConverter.INSTANCE,CAPACITY).end();
  }
}
