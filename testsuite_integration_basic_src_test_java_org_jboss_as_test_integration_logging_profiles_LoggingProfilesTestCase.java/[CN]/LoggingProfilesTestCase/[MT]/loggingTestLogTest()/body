{
  BufferedReader br=new BufferedReader(new InputStreamReader(new FileInputStream(logFile),Charset.forName("UTF-8")));
  String line;
  while ((line=br.readLine()) != null) {
    if (line.contains("LoggingServlet is logging")) {
      br.close();
      Assert.fail("LoggingServlet messages should be presented only in files specified in profiles, but found: " + line);
    }
  }
  br.close();
}
