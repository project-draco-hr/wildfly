{
  Assert.assertTrue("dummy-profile2.log was not created",dummyLog2.exists());
  BufferedReader br=new BufferedReader(new InputStreamReader(new FileInputStream(dummyLog2),Charset.forName("UTF-8")));
  String line;
  while ((line=br.readLine()) != null) {
    if (!line.contains("DummyProfile2: LoggingServlet is logging")) {
      Assert.fail("dummy-profile2 should not contains this line: " + line);
    }
  }
}
