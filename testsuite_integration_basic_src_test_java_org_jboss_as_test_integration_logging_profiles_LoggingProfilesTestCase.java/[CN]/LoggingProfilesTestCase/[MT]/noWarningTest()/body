{
  BufferedReader br=new BufferedReader(new InputStreamReader(new FileInputStream(logFile),StandardCharsets.UTF_8));
  String line;
  while ((line=br.readLine()) != null) {
    if (line.contains("WFLYLOG0010")) {
      br.close();
      Assert.fail("Every deployment should have defined its own logging profile. But found this line in logs: " + line);
    }
  }
  br.close();
}
