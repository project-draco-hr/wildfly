{
  if (container != null) {
    container.shutdown();
    try {
      container.awaitTermination(5,TimeUnit.SECONDS);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
    container=null;
    if (legacyServices != null) {
      for (      ModelTestKernelServices legacyService : legacyServices.values()) {
        legacyService.shutdown();
      }
    }
  }
}
