{
  long end=System.currentTimeMillis() + timeoutMs;
  while (System.currentTimeMillis() < end) {
    try {
      checkProcessManagerConnection(pm,name,false);
      return;
    }
 catch (    AssertionFailedError e) {
      Thread.sleep(100);
    }
  }
  checkProcessManagerConnection(pm,name,false);
}
