{
  ResourceTransformationDescriptionBuilder remoteService=parent.addChildResource(EJB3SubsystemModel.REMOTE_SERVICE_PATH);
  remoteService.getAttributeBuilder().setDiscard(new DiscardAttributeChecker.DiscardAttributeValueChecker(new ModelNode(BeanManagerFactoryBuilderConfiguration.DEFAULT_CONTAINER_NAME)),CLIENT_MAPPINGS_CLUSTER_NAME).addRejectCheck(RejectAttributeChecker.DEFINED,CLIENT_MAPPINGS_CLUSTER_NAME).setDiscard(DiscardAttributeChecker.ALWAYS,EXECUTE_IN_WORKER).end();
}
