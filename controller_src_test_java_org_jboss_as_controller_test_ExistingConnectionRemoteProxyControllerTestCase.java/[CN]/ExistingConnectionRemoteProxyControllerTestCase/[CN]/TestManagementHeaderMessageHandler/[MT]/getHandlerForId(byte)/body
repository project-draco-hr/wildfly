{
  return new MessageHandler(){
    @Override public void handleShutdown(    Connection connection) throws IOException {
    }
    @Override public void handleMessage(    Connection connection,    InputStream dataStream) throws IOException {
    }
    @Override public void handleFinished(    Connection connection) throws IOException {
    }
    @Override public void handleFailure(    Connection connection,    IOException e) throws IOException {
    }
  }
;
}
