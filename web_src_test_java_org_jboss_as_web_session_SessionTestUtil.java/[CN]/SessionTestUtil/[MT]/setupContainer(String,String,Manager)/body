{
  Engine engine=new MockEngine();
  engine.setName(JVM_ROUTE_CACHE_NAME);
  engine.setJvmRoute(jvmRoute);
  Host host=new MockHost();
  host.setName("localhost");
  engine.addChild(host);
  StandardContext context=new StandardContext();
