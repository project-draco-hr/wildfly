{
  if (path != null) {
    File f=new File(path);
    if (f.exists()) {
      if (f.isDirectory()) {
        File[] children=f.listFiles();
        for (        File child : children) {
          try {
            cleanFilesystem(child.getCanonicalPath());
          }
 catch (          IOException e) {
            log.warn("Can't clean any possible children of " + f);
          }
        }
      }
      if (!f.delete()) {
        f.delete();
      }
    }
  }
}
