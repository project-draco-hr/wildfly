{
  super((JDBCStoreManager)cmpField.getManager(),cmpField.getFieldName(),cmpField.getFieldType(),cmpField.getJDBCType(),cmpField.isReadOnly(),cmpField.getReadTimeOut(),cmpField.getPrimaryKeyClass(),cmpField.getPrimaryKeyField(),cmpField,null,cmpField.getColumnName());
  defaultFlags|=JDBCEntityBridge.ADD_TO_SET_ON_UPDATE;
  cmpField.addDefaultFlag(JDBCEntityBridge.ADD_TO_SET_ON_UPDATE);
}
