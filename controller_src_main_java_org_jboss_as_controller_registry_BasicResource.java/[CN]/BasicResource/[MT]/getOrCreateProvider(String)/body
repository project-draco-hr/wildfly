{
  final ResourceProvider provider=children.get(type);
  if (provider != null) {
    return provider;
  }
 else {
    final ResourceProvider newProvider=new DefaultResourceProvider();
    final ResourceProvider existing=children.get(type);
    if (existing != null) {
      return existing;
    }
 else {
      children.put(type,newProvider);
      return newProvider;
    }
  }
}
