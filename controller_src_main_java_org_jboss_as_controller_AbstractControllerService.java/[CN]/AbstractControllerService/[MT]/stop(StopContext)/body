{
  controller=null;
  context.asynchronous();
  Runnable r=new Runnable(){
    @Override public void run(){
      try {
        stopAsynchronous(context);
      }
  finally {
        try {
          authorizer.shutdown();
        }
  finally {
          context.complete();
        }
      }
    }
  }
;
  injectedExecutorService.getValue().execute(r);
}
