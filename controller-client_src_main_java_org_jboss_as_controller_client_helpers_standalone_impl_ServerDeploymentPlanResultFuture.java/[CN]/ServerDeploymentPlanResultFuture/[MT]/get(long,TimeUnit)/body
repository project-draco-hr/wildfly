{
  boolean cleanup=true;
  ModelNode node;
  try {
    node=nodeFuture.get(timeout,unit);
  }
 catch (  InterruptedException ie) {
    cleanup=false;
    throw ie;
  }
catch (  TimeoutException te) {
    cleanup=false;
    throw te;
  }
 finally {
    if (cleanup) {
      plan.cleanup();
    }
  }
  return getResultFromNode(node.get(ClientConstants.RESULT));
}
