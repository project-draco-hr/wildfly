{
  if (resource.isProxy() || resource.isRuntime()) {
    return;
  }
  @SuppressWarnings("deprecation") ChainedResourceTransformationContext wrappedContext=new ChainedResourceTransformationContext(context);
  for (  ChainedResourceTransformerEntry entry : entries) {
    entry.transformResource(wrappedContext,address,resource);
  }
  final ResourceTransformationContext childContext=context.addTransformedResource(PathAddress.EMPTY_ADDRESS,resource);
  childContext.processChildren(resource);
}
