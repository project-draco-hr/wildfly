{
switch (bit) {
case ACTION_BIND:
    return "bind";
case ACTION_REBIND:
  return "rebind";
case ACTION_UNBIND:
return "unbind";
case ACTION_LOOKUP:
return "lookup";
case ACTION_LIST:
return "list";
case ACTION_LIST_BINDINGS:
return "listBindings";
case ACTION_CREATE_SUBCONTEXT:
return "createSubcontext";
case ACTION_DESTROY_SUBCONTEXT:
return "destroySubcontext";
case ACTION_ADD_NAMING_LISTENER:
return "addNamingListener";
default :
throw Assert.impossibleSwitchCase(bit);
}
}
