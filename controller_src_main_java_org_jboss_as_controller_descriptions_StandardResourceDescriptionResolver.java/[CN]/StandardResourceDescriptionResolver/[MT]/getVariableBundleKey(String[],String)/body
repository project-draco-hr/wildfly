{
  StringBuilder sb=new StringBuilder(keyPrefix);
  for (  String arg : fixed) {
    if (sb.length() > 0) {
      sb.append('.');
    }
    sb.append(arg);
  }
  if (variable != null) {
    for (    String arg : variable) {
      if (sb.length() > 0) {
        sb.append('.');
      }
      sb.append(arg);
    }
  }
  return sb.toString();
}
