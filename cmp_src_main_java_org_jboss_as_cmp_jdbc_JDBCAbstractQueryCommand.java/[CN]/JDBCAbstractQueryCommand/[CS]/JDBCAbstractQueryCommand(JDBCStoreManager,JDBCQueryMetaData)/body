{
  this.log=Logger.getLogger(this.getClass().getName() + "." + manager.getMetaData().getName()+ "#"+ q.getMethod().getName());
  queryMetaData=q;
  collectionFactory=q.isLazyResultSetLoading() ? new LazyCollectionFactory() : (QueryCollectionFactory)new EagerCollectionFactory();
  setSelectEntity((JDBCEntityBridge)manager.getEntityBridge());
}
