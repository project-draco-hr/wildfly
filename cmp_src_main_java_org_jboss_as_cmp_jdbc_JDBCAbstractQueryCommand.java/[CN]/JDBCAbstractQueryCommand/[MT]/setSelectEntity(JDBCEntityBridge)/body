{
  if (queryMetaData.getMethod().getName().startsWith("find") && this.selectEntity != null && this.selectEntity != selectEntity) {
    throw new RuntimeException("Finder " + queryMetaData.getMethod().getName() + " defined on "+ this.selectEntity.getEntityName()+ " should return only instances of "+ this.selectEntity.getEntityName()+ " but the query results in instances of "+ selectEntity.getEntityName());
  }
  this.selectField=null;
  this.selectFunction=null;
  this.selectEntity=selectEntity;
  this.selectManager=(JDBCStoreManager)selectEntity.getManager();
}
