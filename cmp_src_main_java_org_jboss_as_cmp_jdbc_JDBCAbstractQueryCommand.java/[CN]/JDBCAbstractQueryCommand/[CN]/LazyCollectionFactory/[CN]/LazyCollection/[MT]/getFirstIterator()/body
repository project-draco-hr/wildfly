{
  return new Iterator(){
    private boolean hasNext;
    private Object cursor;
    public boolean hasNext(){
      return hasNext ? hasNext : (hasNext=hasNextResult());
    }
    public Object next(){
      if (!hasNext()) {
        throw new NoSuchElementException();
      }
      hasNext=false;
      cursor=readNext();
      results.add(cursor);
      return cursor;
    }
    public void remove(){
      --size;
      results.remove(cursor);
    }
  }
;
}
