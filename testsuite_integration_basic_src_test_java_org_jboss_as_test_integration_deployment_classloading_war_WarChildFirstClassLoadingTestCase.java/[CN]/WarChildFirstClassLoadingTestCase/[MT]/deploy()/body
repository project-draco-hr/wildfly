{
  WebArchive war=ShrinkWrap.create(WebArchive.class);
  war.addClasses(WarChildFirstClassLoadingTestCase.class,Stateless.class);
  war.addAsWebInfResource(new StringAsset("" + "<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n" + "\n"+ "<permissions>\n"+ "   <permission>\n"+ "       <classname>java.lang.RuntimePermission</classname>\n"+ "       <name>getClassLoader</name>\n"+ "   </permission>\n"+ "</permissions>\n"+ ""),"permissions.xml");
  return war;
}
