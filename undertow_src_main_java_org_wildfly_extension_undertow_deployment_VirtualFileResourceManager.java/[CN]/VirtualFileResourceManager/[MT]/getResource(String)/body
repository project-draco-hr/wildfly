{
  String path=p;
  if (p.startsWith("/")) {
    path=p.substring(1);
  }
  try {
    VirtualFile file=base.getChild(p);
    if (file.exists()) {
      return new VirtualFileResource(file.getPhysicalFile(),this,path);
    }
 else {
      return null;
    }
  }
 catch (  Exception e) {
    UndertowLogger.REQUEST_LOGGER.debugf(e,"Invalid path %s");
    return null;
  }
}
