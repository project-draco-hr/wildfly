{
  this.caches.remove(cacheName);
  boolean empty=this.caches.isEmpty();
  if (empty) {
    try {
      this.lockManager.stop();
    }
 catch (    Exception e) {
      log.warn(e.getMessage(),e);
    }
    try {
      this.service.stop();
    }
 catch (    Exception e) {
      log.warn(e.getMessage(),e);
    }
  }
  return empty;
}
