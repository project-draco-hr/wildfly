{
  final String handlerName=getHandlerName(operation);
  ModelNode assignedHandlers=getAssignedHandlers(model);
  if (assignedHandlers.isDefined() && assignedHandlers.asList().contains(NAME.validateOperation(operation)))   opFailed(MESSAGES.handlerAlreadyDefined(handlerName));
  assignedHandlers.add(handlerName);
}
