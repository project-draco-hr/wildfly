{
  final String handlerName=getHandlerName(operation);
  ModelNode assignedHandlers=getAssignedHandlers(model);
  if (assignedHandlers.isDefined() && assignedHandlers.asList().contains(operation.get(CommonAttributes.NAME)))   opFailed(MESSAGES.handlerAlreadyDefined(handlerName));
  assignedHandlers.add(handlerName);
}
