{
  value=new CachedConnectionManagerImpl(transactionIntegration.getValue().getTransactionManager(),transactionIntegration.getValue().getTransactionSynchronizationRegistry(),transactionIntegration.getValue().getUserTransactionRegistry());
  value.setDebug(debug);
  value.setError(error);
  value.start();
  ROOT_LOGGER.debugf("Started CcmService %s",context.getController().getName());
}
