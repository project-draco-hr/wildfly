{
  BatchedWork work=new BatchedWork(){
    @Override public void execute(    BatchBuilder batchBuilder) throws Exception {
      Object service=loadClass(moduleId,EchoInvokerService.class.getName()).newInstance();
      batchBuilder.addService(EchoInvokerService.SERVICE_NAME,(Service<?>)service).addDependency(FrameworkService.SERVICE_NAME).setInitialMode(Mode.ACTIVE).install();
    }
  }
;
  runWithLatchedBatch(work);
}
