{
  ServiceBuilder serviceBuilder=serviceTarget.addService(serviceName,service).addDependency(TxnServices.JBOSS_TXN_TRANSACTION_MANAGER,service.transactionManager).addDependency(serverServiceName,ActiveMQServer.class,service.activeMQServer).addDependency(ActiveMQActivationService.getServiceName(serverServiceName)).addDependency(JMSServices.getJmsManagerBaseServiceName(serverServiceName)).setInitialMode(ServiceController.Mode.PASSIVE);
  serviceBuilder.install();
}
