{
  Socket socket=null;
  try {
    socket=new Socket(getContainerConfiguration().getManagementAddress(),getContainerConfiguration().getManagementPort());
  }
 catch (  Exception ignored) {
    return false;
  }
 finally {
    if (socket != null) {
      try {
        socket.close();
      }
 catch (      Exception e) {
        throw new RuntimeException("Could not close isServerStarted socket",e);
      }
    }
  }
  return true;
}
