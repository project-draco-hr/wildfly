{
  final DeploymentUnitProcessorService<T> deploymentUnitProcessorService=new DeploymentUnitProcessorService<T>(deploymentUnitProcessor);
  builder.addService(serviceName,deploymentUnitProcessorService).addDependency(SERVICE_DEPLOYMENT_CHAIN_NAME).toInjector(new DeploymentChainProcessorInjector<T>(deploymentUnitProcessorService,priority));
}
