{
  if (file.isDirectory()) {
    for (    File child : listDirectoryChildren(file)) {
      if (!isZipComplete(child)) {
        return false;
      }
    }
    return true;
  }
 else   if (isEEArchive(file.getName())) {
    try {
      return ZipCompletionScanner.isCompleteZip(file);
    }
 catch (    IOException e) {
      ROOT_LOGGER.failedCheckingZipFile(e,file.getPath());
      return false;
    }
  }
 else {
    return true;
  }
}
