{
  if (rescanIncompleteTask != null) {
    rescanIncompleteTask.cancel(false);
    rescanIncompleteTask=null;
  }
  if (rescanUndeployTask != null) {
    rescanUndeployTask.cancel(false);
    rescanUndeployTask=null;
  }
  if (scanTask != null) {
    scanTask.cancel(false);
    scanTask=null;
  }
}
