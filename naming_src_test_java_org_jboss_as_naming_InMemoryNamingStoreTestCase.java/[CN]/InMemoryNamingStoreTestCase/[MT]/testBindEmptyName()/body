{
  try {
    nameStore.bind(null,new CompositeName(),new Object(),Object.class.getName());
    fail("Should have thrown and InvalidNameException");
  }
 catch (  InvalidNameException expected) {
  }
  try {
    nameStore.bind(null,new CompositeName(""),new Object(),Object.class.getName());
    fail("Should have thrown and InvalidNameException");
  }
 catch (  InvalidNameException expected) {
  }
}
