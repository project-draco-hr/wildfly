{
  if (reader.getNamespaceURI().equals("urn:security")) {
    final String localName=reader.getLocalName();
    if (localName.equals("security-domain"))     metaData.setSecurityDomain(getElementText(reader));
 else     if (localName.equals("run-as-principal"))     metaData.setRunAsPrincipal(getElementText(reader));
 else     throw unexpectedElement(reader);
  }
 else   super.processElement(metaData,reader);
}
