{
  final String prefix=COMMON_ATTRIBUTE_NAMES.get(name.split("\\.")[0]);
  final StringBuilder sb;
  if (prefix == null) {
    sb=new StringBuilder(name);
  }
 else {
    sb=new StringBuilder(prefix).append('.').append(name);
  }
  if (variable != null) {
    for (    String arg : variable) {
      if (sb.length() > 0)       sb.append('.');
      sb.append(arg);
    }
  }
  return sb.toString();
}
