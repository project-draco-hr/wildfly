{
  this.name=name;
  this.port=port;
  this.isFixedPort=isFixedPort;
  this.multicastAddress=multicastAddress;
  this.multicastPort=multicastPort;
  this.socketBindings=socketBindings;
  this.networkInterface=networkInterface;
