{
  while (reader.hasNext() && reader.nextTag() != END_ELEMENT) {
    final Element element=Element.forName(reader.getLocalName());
switch (element) {
case NAME:
      patch.setPatchId(reader.getElementText());
    break;
case DESCRIPTION:
  patch.setDescription(reader.getElementText());
break;
case CUMULATIVE:
parsePatchType(reader,Patch.PatchType.CUMULATIVE,patch);
break;
case ONE_OFF:
parsePatchType(reader,Patch.PatchType.ONE_OFF,patch);
break;
case MODULES:
parseModules(reader,patch);
break;
case BUNDLES:
parseBundles(reader,patch);
break;
case MISC_FILES:
parseMiscFiles(reader,patch);
break;
default :
throw unexpectedElement(reader);
}
}
}
