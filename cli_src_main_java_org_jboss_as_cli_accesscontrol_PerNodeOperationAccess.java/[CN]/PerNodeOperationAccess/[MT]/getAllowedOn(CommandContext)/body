{
  if (allowedOn == null) {
    if (ctx.getConfig().isAccessControl()) {
      if (stateOn == null) {
        initList(ctx.getModelControllerClient());
      }
      completeAllowedOn(ctx.getModelControllerClient());
    }
 else {
      allowedOn=Util.getNodeNames(ctx.getModelControllerClient(),null,nodeType);
    }
  }
  return allowedOn;
}
