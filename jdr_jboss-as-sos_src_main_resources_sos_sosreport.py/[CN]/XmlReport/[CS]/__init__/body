def __init__(self):
    try:
        import libxml2
    except ImportError:
        self.enabled = False
        return
    else:
        self.enabled = False
        return
    self.doc = libxml2.newDoc('1.0')
    self.root = self.doc.newChild(None, 'sos', None)
    self.commands = self.root.newChild(None, 'commands', None)
    self.files = self.root.newChild(None, 'files', None)
