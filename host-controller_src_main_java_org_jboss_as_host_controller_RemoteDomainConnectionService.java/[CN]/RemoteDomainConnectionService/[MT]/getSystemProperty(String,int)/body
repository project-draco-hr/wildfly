{
  final SecurityManager sm=System.getSecurityManager();
  if (sm == null) {
    return Integer.getInteger(name,defaultValue);
  }
 else {
    return AccessController.doPrivileged(new PrivilegedAction<Integer>(){
      @Override public Integer run(){
        return Integer.getInteger(name,defaultValue);
      }
    }
);
  }
}
