{
  WebArchive war=ShrinkWrap.create(WebArchive.class,"jaxrsnoap.war");
  war.addClasses(CDIResourceInjectionTestCase.class,CDIResource.class,CDIBean.class);
  war.addWebResource(WebXml.get("<servlet-mapping>\n" + "        <servlet-name>javax.ws.rs.core.Application</servlet-name>\n" + "        <url-pattern>/myjaxrs/*</url-pattern>\n"+ "    </servlet-mapping>\n"+ "\n"),"web.xml");
  war.addWebResource(EmptyAsset.INSTANCE,"beans.xml");
  return war;
}
