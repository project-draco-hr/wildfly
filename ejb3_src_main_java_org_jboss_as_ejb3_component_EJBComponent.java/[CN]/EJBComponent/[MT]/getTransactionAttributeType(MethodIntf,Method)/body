{
  ConcurrentMap<String,ConcurrentMap<ArrayKey,TransactionAttributeType>> perMethodIntf=txAttrs.get(methodIntf);
  if (perMethodIntf == null)   throw new IllegalStateException("Can't find tx attrs for " + methodIntf);
  ConcurrentMap<ArrayKey,TransactionAttributeType> perMethod=perMethodIntf.get(method.getName());
  if (perMethod == null)   throw new IllegalStateException("Can't find tx attrs for method name " + method.getName() + " via "+ methodIntf);
  TransactionAttributeType txAttr=perMethod.get(new ArrayKey(method.getParameterTypes()));
  if (txAttr == null)   throw new IllegalStateException("Can't find tx attr for method " + method + " via "+ methodIntf);
  return txAttr;
}
