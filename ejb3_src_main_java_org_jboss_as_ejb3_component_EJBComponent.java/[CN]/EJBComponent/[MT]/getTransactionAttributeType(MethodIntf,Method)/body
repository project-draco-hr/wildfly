{
  TransactionAttributeType txAttr=txAttrs.get(new MethodTransactionAttributeKey(methodIntf,MethodIdentifier.getIdentifierForMethod(method)));
  if (txAttr == null)   throw new IllegalStateException("Can't find tx attr for method " + method + " on view type "+ methodIntf+ " on bean named "+ this.getComponentName());
  return txAttr;
}
