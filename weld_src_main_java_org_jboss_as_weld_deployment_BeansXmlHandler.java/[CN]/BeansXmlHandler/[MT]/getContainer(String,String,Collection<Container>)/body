{
  for (  Container container : containers) {
    if (uri.length() == 0) {
      if (container.getLocalName().equals(localName)) {
        return container;
      }
    }
 else {
      if (container.getLocalName().equals(localName) && container.getUri().equals(uri)) {
        return container;
      }
    }
  }
  return null;
}
