{
  return type.equals(String.class) || type.isPrimitive() || type.equals(Void.class)|| type.equals(Boolean.class)|| type.equals(Character.class)|| type.equals(Byte.class)|| type.equals(Short.class)|| type.equals(Integer.class)|| type.equals(Long.class)|| type.equals(Float.class)|| type.equals(Double.class)|| (type.isArray() && isTypeExcluded(type.getComponentType()));
}
