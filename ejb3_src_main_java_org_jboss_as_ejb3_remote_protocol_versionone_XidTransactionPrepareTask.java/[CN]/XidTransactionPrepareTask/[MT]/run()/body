{
  try {
    this.manageTransaction();
  }
 catch (  Throwable t) {
    try {
      transactionRequestHandler.writeException(this.channel,this.invocationId,t,null);
    }
 catch (    IOException e) {
      logger.error("Could not write out message to channel due to",e);
      IoUtils.safeClose(this.channel);
    }
    return;
  }
}
