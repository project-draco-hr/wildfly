{
  StringBuffer tmp=new StringBuffer("PatternInfo[");
  tmp.append("pattern=");
  tmp.append(pattern);
  tmp.append(",type=");
  tmp.append(type);
  tmp.append(",isOverridden=");
  tmp.append(isOverridden);
  tmp.append(",qualifiers=");
  tmp.append(qualifiers);
  tmp.append("]");
  return tmp.toString();
}
