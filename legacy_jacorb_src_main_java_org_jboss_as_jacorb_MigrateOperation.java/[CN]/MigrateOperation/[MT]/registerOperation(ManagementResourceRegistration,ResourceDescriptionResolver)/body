{
  registry.registerOperationHandler(new SimpleOperationDefinitionBuilder(MIGRATE,resourceDescriptionResolver).setRuntimeOnly().setReplyParameters(MIGRATION_ERROR_ATTR).build(),new MigrateOperation(false));
  registry.registerOperationHandler(new SimpleOperationDefinitionBuilder(DESCRIBE_MIGRATION,resourceDescriptionResolver).setRuntimeOnly().setReplyParameters(MIGRATION_ERROR_ATTR).build(),new MigrateOperation(true));
}
