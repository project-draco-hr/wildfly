{
  final String subsystemXml=getSubsystemXml();
  final ModelVersion modelVersion=ModelVersion.create(1,1,0);
  final KernelServicesBuilder builder=createKernelServicesBuilder(LoggingTestEnvironment.getManagementInstance()).setSubsystemXml(subsystemXml);
  builder.createLegacyKernelServicesBuilder(LoggingTestEnvironment.getManagementInstance(),modelVersion).addMavenResourceURL("org.jboss.as:jboss-as-logging:7.1.2.Final").addMavenResourceURL("org.jboss.as:jboss-as-controller:7.1.2.Final").addParentFirstClassPattern("org.jboss.as.controller.*");
  KernelServices mainServices=builder.build();
  Assert.assertTrue(mainServices.isSuccessfulBoot());
  Assert.assertTrue(mainServices.getLegacyServices(modelVersion).isSuccessfulBoot());
}
