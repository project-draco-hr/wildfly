{
  if (pathAddress.size() == 0) {
    return Constants.ROOT_MODEL_NAME;
  }
  final StringBuilder sb=new StringBuilder(Constants.DOMAIN);
  sb.append(":");
  boolean first=true;
  for (  PathElement element : pathAddress) {
    if (first) {
      first=false;
    }
 else {
      sb.append(",");
    }
    appendEscapedCharacter(ESCAPED_KEY_CHARACTERS,sb,element.getKey());
    sb.append("=");
    appendEscapedCharacter(ECAPED_VALUE_CHARACTERS,sb,element.getValue());
  }
  try {
    return ObjectName.getInstance(sb.toString());
  }
 catch (  MalformedObjectNameException e) {
    throw new RuntimeException("Could not create ObjectName for address" + pathAddress,e);
  }
}
