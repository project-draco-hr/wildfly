{
  final AnnotationValue value=annotationInstance.value(name);
  if (booleanValue) {
    if (value == null) {
      setBoolean(timer,defaultBooleanValue);
    }
 else {
      setBoolean(timer,value.asBoolean());
    }
  }
 else {
    if (value == null) {
      setString(timer,defaultStringValue);
    }
 else {
      setString(timer,value.asString());
    }
  }
}
