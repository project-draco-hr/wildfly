{
  boolean modified=false;
  final EntityState entityState=getEntityState(ctx);
  if (entityState.isCreated()) {
    modified=areCmpFieldsDirty(ctx,entityState);
    if (!modified) {
      for (int i=0; i < cmrFields.length; ++i) {
        if (cmrFields[i].isDirty(ctx)) {
          modified=true;
          break;
        }
      }
    }
  }
  return modified;
}
