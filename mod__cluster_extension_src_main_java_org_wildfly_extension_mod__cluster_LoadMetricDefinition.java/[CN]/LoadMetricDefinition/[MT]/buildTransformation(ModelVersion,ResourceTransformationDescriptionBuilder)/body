{
  if (ModClusterModel.VERSION_1_2_0.requiresTransformation(version)) {
    builder.addChildResource(ModClusterExtension.LOAD_METRIC_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,TYPE,WEIGHT,CAPACITY,PROPERTY).addRejectCheck(CapacityCheckerAndConverter.INSTANCE,CAPACITY).setValueConverter(CapacityCheckerAndConverter.INSTANCE,CAPACITY).addRejectCheck(PropertyCheckerAndConverter.INSTANCE,PROPERTY).setValueConverter(PropertyCheckerAndConverter.INSTANCE,PROPERTY).end();
  }
}
