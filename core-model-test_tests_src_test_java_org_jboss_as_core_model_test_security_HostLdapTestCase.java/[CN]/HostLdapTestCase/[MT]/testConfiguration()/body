{
  KernelServices kernelServices=createKernelServicesBuilder(TestModelType.HOST).setXmlResource("ldap-host.xml").validateDescription().build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),"ldap-host.xml"),marshalled);
}
