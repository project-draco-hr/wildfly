{
  if ((site != null) || (rack != null) || (machine != null)) {
    this.topology=new Topology(){
      @Override public String getMachine(){
        return machine;
      }
      @Override public String getRack(){
        return rack;
      }
      @Override public String getSite(){
        return site;
      }
    }
;
  }
  return this;
}
