{
  String subsystemXml="<subsystem xmlns=\"" + Namespace.CURRENT.getUriString() + "\">"+ "    <remoting-connector />"+ "</subsystem>";
  KernelServices servicesA=super.installInController(subsystemXml);
  ModelNode modelA=servicesA.readWholeModel();
  ModelNode describeOp=new ModelNode();
  describeOp.get(OP).set(DESCRIBE);
  describeOp.get(OP_ADDR).set(PathAddress.pathAddress(PathElement.pathElement(SUBSYSTEM,JMXExtension.SUBSYSTEM_NAME)).toModelNode());
  List<ModelNode> operations=checkResultAndGetContents(servicesA.executeOperation(describeOp)).asList();
  servicesA.shutdown();
  Assert.assertEquals(2,operations.size());
  KernelServices servicesB=super.installInController(operations);
  ModelNode modelB=servicesB.readWholeModel();
  super.compare(modelA,modelB);
}
