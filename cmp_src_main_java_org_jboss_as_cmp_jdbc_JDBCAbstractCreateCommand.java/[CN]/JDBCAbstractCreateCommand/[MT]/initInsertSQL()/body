{
  StringBuffer sql=new StringBuffer(250);
  sql.append(SQLUtil.INSERT_INTO).append(entity.getQualifiedTableName()).append(" (");
  SQLUtil.getColumnNamesClause(insertFields,sql);
  sql.append(')').append(SQLUtil.VALUES).append('(');
  SQLUtil.getValuesClause(insertFields,sql).append(')');
  insertSQL=sql.toString();
  if (debug)   log.debug("Insert Entity SQL: " + insertSQL);
}
