{
  final ResourceTransformationDescriptionBuilder child=subsystemBuilder.addChildResource(PERIODIC_HANDLER_PATH).getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,AUTOFLUSH,APPEND,FILE,SUFFIX).end();
  loggingProfileBuilder.rejectChildResource(PERIODIC_HANDLER_PATH);
  return registerTransformers(child);
}
