{
  if (remove) {
    LocalLock lock=getLocalLock(lockName,false);
    if (lock != null) {
      lock.removable=true;
    }
  }
  this.clusterSupport.unlock(lockName);
  LocalLock lock=getLocalLock(lockName,false);
  if (lock != null && lock.removable && lock.lockState.get().lockHolder == null) {
    localLocks.remove(lockName,lock);
  }
}
