{
  String deploymentName=value.getDeployment() != null ? value.getDeployment().getDeploymentName() : "";
  DEPLOYMENT_CONNECTOR_LOGGER.debugf("Stopping sevice %s",ConnectorServices.RESOURCE_ADAPTER_DEPLOYMENT_SERVICE_PREFIX.append(deploymentName));
  unregisterAll(deploymentName);
  for (  ServiceName name : this.getJndiServices()) {
    if (context.getController().getServiceContainer().getService(name) != null)     context.getController().getServiceContainer().getService(name).setMode(Mode.REMOVE);
  }
}
