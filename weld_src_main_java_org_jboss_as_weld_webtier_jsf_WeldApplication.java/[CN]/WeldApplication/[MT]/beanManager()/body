{
  if (beanManager == null) {
synchronized (this) {
      if (beanManager == null) {
        try {
          beanManager=(BeanManager)new InitialContext().lookup("java:comp/BeanManager");
        }
 catch (        NamingException e) {
          return null;
        }
      }
    }
  }
  return beanManager;
}
