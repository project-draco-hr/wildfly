{
  super(reason + ": at line " + errorLine+ ", "+ "column "+ errorColumn+ ".  "+ "Encountered: \""+ fieldName+ "\""+ ((path == null) ? "" : " after: \"" + path + "\""));
  this.reason=reason;
  this.path=path;
  this.fieldName=fieldName;
  this.errorLine=errorLine;
  this.errorColumn=errorColumn;
}
