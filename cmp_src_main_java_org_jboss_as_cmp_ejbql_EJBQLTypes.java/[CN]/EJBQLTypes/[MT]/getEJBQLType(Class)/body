{
  int result;
  if (type == Boolean.class || type == Boolean.TYPE) {
    result=BOOLEAN_TYPE;
  }
 else   if (type.isPrimitive() || type == Character.class || Number.class.isAssignableFrom(type)) {
    result=NUMERIC_TYPE;
  }
 else   if (type == String.class) {
    result=STRING_TYPE;
  }
 else   if (Date.class.isAssignableFrom(type)) {
    result=DATETIME_TYPE;
  }
 else   if (EJBObject.class.isAssignableFrom(type) || EJBLocalObject.class.isAssignableFrom(type)) {
    result=ENTITY_TYPE;
  }
 else {
    result=VALUE_CLASS_TYPE;
  }
  return result;
}
