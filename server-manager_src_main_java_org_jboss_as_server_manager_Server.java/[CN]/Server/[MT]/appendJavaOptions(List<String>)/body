{
  String heap=jvmElement.getHeapSize();
  String max=jvmElement.getMaxHeap();
  if (max == null && heap != null) {
    max=heap;
  }
  if (heap == null && max != null) {
    heap=max;
  }
  if (heap != null) {
    command.add("-Xms" + heap);
  }
  if (max != null) {
    command.add("-Xmx" + max);
  }
}
