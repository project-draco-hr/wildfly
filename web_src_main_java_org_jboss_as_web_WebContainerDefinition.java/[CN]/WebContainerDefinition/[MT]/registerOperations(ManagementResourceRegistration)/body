{
  super.registerOperations(container);
  container.registerOperationHandler("add-mime",MimeMappingAdd.INSTANCE,new DefaultOperationDescriptionProvider("add-mime",WebExtension.getResourceDescriptionResolver("container.mime-mapping"),MIME_NAME,MIME_VALUE));
  container.registerOperationHandler("remove-mime",MimeMappingRemove.INSTANCE,new DefaultOperationDescriptionProvider("remove-mime",WebExtension.getResourceDescriptionResolver("container.mime-mapping"),MIME_NAME));
}
