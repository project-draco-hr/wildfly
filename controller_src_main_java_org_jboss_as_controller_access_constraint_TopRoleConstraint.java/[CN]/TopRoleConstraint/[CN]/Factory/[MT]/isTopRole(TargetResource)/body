{
  PathAddress address=target.getResourceAddress();
  if (address.size() >= 3) {
    PathElement roleMapping;
    if (MGMT_ELEMENT.equals(address.getElement(0)) && AUTHZ_ELEMENT.equals(address.getElement(1)) && ROLE_MAPPING.equals((roleMapping=address.getElement(2)).getKey())) {
      String roleName=roleMapping.getValue();
      return StandardRole.AUDITOR.name().equalsIgnoreCase(roleName) || StandardRole.SUPERUSER.name().equalsIgnoreCase(roleName);
    }
  }
  return false;
}
