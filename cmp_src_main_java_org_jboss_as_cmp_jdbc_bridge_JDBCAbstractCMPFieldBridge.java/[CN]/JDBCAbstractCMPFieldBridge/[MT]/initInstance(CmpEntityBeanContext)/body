{
  if (!readOnly) {
    Object value;
    if (fieldType == boolean.class)     value=Boolean.FALSE;
 else     if (fieldType == byte.class)     value=new Byte((byte)0);
 else     if (fieldType == int.class)     value=new Integer(0);
 else     if (fieldType == long.class)     value=new Long(0L);
 else     if (fieldType == short.class)     value=new Short((short)0);
 else     if (fieldType == char.class)     value=new Character('\u0000');
 else     if (fieldType == double.class)     value=new Double(0d);
 else     if (fieldType == float.class)     value=new Float(0f);
 else     value=null;
    setInstanceValue(ctx,value);
  }
}
