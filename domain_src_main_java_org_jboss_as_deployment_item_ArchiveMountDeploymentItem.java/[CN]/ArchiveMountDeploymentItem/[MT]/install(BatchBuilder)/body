{
  final ServiceName serviceName=ARCHIVE_MOUNT_DEPLOYMENT_SERVICE.append(root.getPathName());
  try {
    builder.addServiceValueIfNotExist(serviceName,Values.immediateValue(new ArchiveMountDeploymentService(root))).setInitialMode(ServiceController.Mode.ON_DEMAND);
  }
 catch (  DuplicateServiceException e) {
    throw new RuntimeException(e);
  }
}
