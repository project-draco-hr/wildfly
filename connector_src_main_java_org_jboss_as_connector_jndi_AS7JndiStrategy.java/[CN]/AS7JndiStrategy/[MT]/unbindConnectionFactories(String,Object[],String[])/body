{
  ensureNotEmpty(cfs,"CFS");
  ensureNotEmpty(jndis,"JNDIs");
  if (cfs.length != jndis.length)   throw new IllegalArgumentException("Number of connection factories doesn't match number of JNDI names");
  for (int i=0; i < cfs.length; i++) {
    String jndiName=jndis[i];
    Object cf=cfs[i];
    unbindObject(jndiName,cf);
  }
}
