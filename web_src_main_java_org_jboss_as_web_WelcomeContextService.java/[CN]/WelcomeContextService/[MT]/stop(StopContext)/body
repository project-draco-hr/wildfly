{
  try {
    hostInjector.getValue().getHost().removeChild(context);
    context.stop();
  }
 catch (  LifecycleException e) {
    WebLogger.WEB_LOGGER.stopWelcomeContextFailed(e);
  }
  try {
    context.destroy();
  }
 catch (  Exception e) {
    WebLogger.WEB_LOGGER.destroyWelcomeContextFailed(e);
  }
}
