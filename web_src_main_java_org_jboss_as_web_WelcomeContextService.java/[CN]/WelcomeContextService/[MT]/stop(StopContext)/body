{
  try {
    hostInjector.getValue().getHost().removeChild(context);
    context.stop();
  }
 catch (  LifecycleException e) {
    log.error("exception while stopping context",e);
  }
  try {
    context.destroy();
  }
 catch (  Exception e) {
    log.error("exception while destroying context",e);
  }
}
