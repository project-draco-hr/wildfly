{
  final ResourceTransformationDescriptionBuilder child=subsystemBuilder.addChildResource(ASYNC_HANDLER_PATH).addOperationTransformationOverride(ADD_HANDLER_OPERATION_NAME).setCustomOperationTransformer(LoggingOperationTransformer.INSTANCE).end().addOperationTransformationOverride(REMOVE_HANDLER_OPERATION_NAME).setCustomOperationTransformer(LoggingOperationTransformer.INSTANCE).end().getAttributeBuilder().addRejectCheck(RejectAttributeChecker.SIMPLE_EXPRESSIONS,QUEUE_LENGTH,OVERFLOW_ACTION).end();
  loggingProfileBuilder.rejectChildResource(ASYNC_HANDLER_PATH);
  return registerTransformers(child);
}
