{
  if (manager != null) {
    try {
      manager.stop();
    }
 catch (    Exception e) {
      log.error(e.getMessage(),e);
    }
  }
  if (cacheContainer != null) {
    try {
      cacheContainer.stop();
    }
 catch (    Exception e) {
      log.error(e.getMessage(),e);
    }
  }
  if (tempDir != null) {
    SessionTestUtil.cleanFilesystem(tempDir);
  }
}
