{
  if (name.size() == 0) {
    throw IIOPMessages.MESSAGES.invalidEmptyName();
  }
  NameComponent[] path=org.jboss.as.iiop.openjdk.naming.jndi.CNNameParser.nameToCosName(name);
  try {
    callBindOrRebind(path,name,obj,true);
  }
 catch (  CannotProceedException e) {
    javax.naming.Context cctx=getContinuationContext(e);
    cctx.rebind(e.getRemainingName(),obj);
  }
}
