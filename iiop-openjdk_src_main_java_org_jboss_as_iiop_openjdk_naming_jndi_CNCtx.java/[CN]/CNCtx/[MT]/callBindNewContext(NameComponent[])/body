{
  if (_nc == null)   throw IIOPMessages.MESSAGES.notANamingContext(path.toString());
  try {
    NamingContext nctx=_nc.bind_new_context(path);
    return new CNCtx(_orb,nctx,_env,makeFullName(path));
  }
 catch (  Exception e) {
    throw org.jboss.as.iiop.openjdk.naming.jndi.ExceptionMapper.mapException(e,this,path);
  }
}
