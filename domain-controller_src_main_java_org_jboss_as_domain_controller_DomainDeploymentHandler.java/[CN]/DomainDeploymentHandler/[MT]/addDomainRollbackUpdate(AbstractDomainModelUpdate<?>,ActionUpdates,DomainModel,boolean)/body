{
  AbstractDomainModelUpdate<?> rollback=dmu.getCompensatingUpdate(model);
  if (rollback != null) {
    au.domainRollbacks.add(0,rollback);
    if (addServerUpdate) {
      AbstractServerModelUpdate<?> smu=rollback.getServerModelUpdate();
      if (smu != null) {
        au.serverRollbacks.add(0,smu);
      }
    }
  }
}
