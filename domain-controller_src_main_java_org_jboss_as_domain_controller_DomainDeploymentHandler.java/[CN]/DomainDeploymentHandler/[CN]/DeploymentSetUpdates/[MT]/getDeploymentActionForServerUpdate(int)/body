{
  int count=0;
  for (  ActionUpdates au : actionUpdates) {
    count+=au.serverUpdates.size();
    if (index < count) {
      return au.action;
    }
  }
  throw new IndexOutOfBoundsException(index + " is larger than the index of the last server update (" + (getServerUpdates().size() - 1)+ ")");
}
