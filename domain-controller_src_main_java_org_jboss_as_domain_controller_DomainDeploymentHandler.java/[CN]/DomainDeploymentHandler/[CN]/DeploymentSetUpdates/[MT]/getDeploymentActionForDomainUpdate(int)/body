{
  int count=0;
  for (  ActionUpdates au : actionUpdates) {
    count+=au.domainUpdates.size();
    if (index < count) {
      return au.action;
    }
  }
  throw new IndexOutOfBoundsException(index + " is larger than the index of the last domain update (" + (getDomainUpdates().size() - 1)+ ")");
}
