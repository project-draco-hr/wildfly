{
  Class<?> c=beanClass;
  boolean override=true;
  boolean found=false;
  while (c != null) {
    for (    final Method m : c.getDeclaredMethods()) {
      if (m.getName().equals(method.getName()) && m.getReturnType().equals(method.getReturnType()) && Arrays.equals(m.getParameterTypes(),method.getParameterTypes())) {
        override=Modifier.isAbstract(m.getModifiers());
        found=true;
        break;
      }
    }
    if (found) {
      break;
    }
    c=c.getSuperclass();
  }
  return override && super.overrideMethod(method,identifier,creator);
}
