{
  List<String> content=readContent(filePath);
  boolean found=false;
  String value=null;
  for (  String line : content) {
    String trimmed=line.trim();
    if (trimmed.contains("#" + userName)) {
      found=true;
      value=trimmed.substring(trimmed.indexOf('=') + 1,trimmed.length());
      break;
    }
  }
  assertTrue(found);
  return value;
}
