{
  final Lock lock=getLock(timedObjectId);
  try {
    lock.lock();
    final Map<String,TimerEntity> timers=getTimers(timedObjectId);
    final TimerEntity timer=timers.get(id);
    if (timer == null) {
      return null;
    }
    return mostRecentEntityVersion(timer);
  }
  finally {
    lock.unlock();
  }
}
