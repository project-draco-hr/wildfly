{
  return target.addService(this.getServiceName(),this).addDependency(ChannelServiceName.CONNECTOR.getServiceName(this.channelName),Channel.class,this.parentChannel).addDependency(ChannelServiceName.FACTORY.getServiceName(this.channelName),ChannelFactory.class,this.parentFactory).setInitialMode(ServiceController.Mode.ON_DEMAND);
}
