{
  super(PathElement.pathElement(ModelDescriptionConstants.JVM),new StandardResourceDescriptionResolver("jvm",HostDescriptionProviders.class.getPackage().getName() + ".LocalDescriptions",HostDescriptionProviders.class.getClassLoader(),true,false),new JVMAddHandler(JvmAttributes.getAttributes(server)),JVMRemoveHandler.INSTANCE);
