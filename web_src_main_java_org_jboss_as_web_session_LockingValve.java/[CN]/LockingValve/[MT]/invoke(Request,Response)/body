{
  try {
    if (this.lock.tryLock(0,TimeUnit.SECONDS)) {
      try {
        this.next.invoke(request,response);
      }
  finally {
        this.lock.unlock();
      }
    }
 else {
      response.sendError(HttpServletResponse.SC_SERVICE_UNAVAILABLE);
    }
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
  }
}
