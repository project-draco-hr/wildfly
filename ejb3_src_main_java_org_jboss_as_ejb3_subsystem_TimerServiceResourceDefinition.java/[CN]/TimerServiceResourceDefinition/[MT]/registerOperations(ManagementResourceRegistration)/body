{
  super.registerOperations(resourceRegistration);
  if (pathManager != null) {
    final ResolvePathHandler resolvePathHandler=ResolvePathHandler.Builder.of(pathManager).setPathAttribute(TimerServiceResourceDefinition.PATH).setRelativeToAttribute(TimerServiceResourceDefinition.RELATIVE_TO).build();
    resourceRegistration.registerOperationHandler(resolvePathHandler.getOperationDefinition(),resolvePathHandler);
  }
}
