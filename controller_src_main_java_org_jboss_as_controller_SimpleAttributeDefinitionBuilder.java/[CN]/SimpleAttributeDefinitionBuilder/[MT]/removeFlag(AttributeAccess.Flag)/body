{
  if (!isFlagPresent(flag))   return this;
  if (flags != null && flags.length > 0) {
    final int length=flags.length;
    final AttributeAccess.Flag[] newFlags=new AttributeAccess.Flag[length - 1];
    int k=0;
    for (int i=0; i < length; i++) {
      if (flags[i] != flag) {
        newFlags[k]=flags[i];
        k++;
      }
    }
    if (k != length - 1) {
      flags=newFlags;
    }
  }
  return this;
}
