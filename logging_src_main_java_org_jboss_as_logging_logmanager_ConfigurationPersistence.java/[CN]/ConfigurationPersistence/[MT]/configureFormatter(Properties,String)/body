{
  if (config.getFormatterConfiguration(formatterName) != null) {
    return;
  }
  final FormatterConfiguration configuration=config.addFormatterConfiguration(getStringProperty(properties,getKey("formatter",formatterName,"module")),getStringProperty(properties,getKey("formatter",formatterName)),formatterName,getStringCsvArray(properties,getKey("formatter",formatterName,"constructorProperties")));
  configureProperties(properties,configuration,getKey("formatter",formatterName));
}
