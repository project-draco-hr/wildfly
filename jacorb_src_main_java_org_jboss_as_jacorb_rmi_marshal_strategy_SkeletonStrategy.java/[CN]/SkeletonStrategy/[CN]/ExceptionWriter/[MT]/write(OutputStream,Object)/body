{
  if (writeMethod != null) {
    try {
      writeMethod.invoke(null,new Object[]{out,excep});
    }
 catch (    IllegalAccessException e) {
      throw new RuntimeException("Internal error: " + e);
    }
catch (    java.lang.reflect.InvocationTargetException e) {
