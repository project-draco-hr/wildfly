{
  return new FailedOperationTransformationConfig().addFailedAttribute(PathAddress.pathAddress(EJB3Extension.SUBSYSTEM_PATH),new FailedOperationTransformationConfig.NewAttributesConfig(EJB3SubsystemModel.DEFAULT_SFSB_PASSIVATION_DISABLED_CACHE)).addFailedAttribute(PathAddress.pathAddress(EJB3Extension.SUBSYSTEM_PATH,EJB3SubsystemModel.TIMER_SERVICE_PATH,PathElement.pathElement(EJB3SubsystemModel.FILE_DATA_STORE,"file-data-store")),new FailedOperationTransformationConfig.RejectExpressionsConfig(FileDataStoreResourceDefinition.PATH));
}
