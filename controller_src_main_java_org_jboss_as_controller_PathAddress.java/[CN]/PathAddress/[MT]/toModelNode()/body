{
  final ModelNode node=new ModelNode().setEmptyList();
  for (  PathElement element : pathAddressList) {
    final String value;
    if (element.isMultiTarget() && !element.isWildcard()) {
      value='[' + element.getValue() + ']';
    }
 else {
      value=element.getValue();
    }
    node.add(element.getKey(),value);
  }
  return node;
}
