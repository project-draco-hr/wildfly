{
  final ServiceTarget serviceTarget=context.getServiceTarget();
  final ServiceName endpointName=RemotingServices.SUBSYSTEM_ENDPOINT;
  ManagementChannelRegistryService.addService(serviceTarget,endpointName);
  ManagementRemotingServices.installManagementChannelServices(serviceTarget,endpointName,new ModelControllerClientOperationHandlerFactoryService(),Services.JBOSS_SERVER_CONTROLLER,ManagementRemotingServices.MANAGEMENT_CHANNEL,Services.JBOSS_SERVER_EXECUTOR,verificationHandler,newControllers);
}
