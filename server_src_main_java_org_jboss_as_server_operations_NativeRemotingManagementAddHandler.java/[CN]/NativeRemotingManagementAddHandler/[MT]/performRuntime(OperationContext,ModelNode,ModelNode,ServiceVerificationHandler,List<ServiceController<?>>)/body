{
  final ServiceTarget serviceTarget=context.getServiceTarget();
  final ServiceName endpointName=RemotingServices.SUBSYSTEM_ENDPOINT;
  ManagementRemotingServices.installManagementChannelServices(serviceTarget,endpointName,new ModelControllerClientOperationHandlerFactoryService(),Services.JBOSS_SERVER_CONTROLLER,ManagementRemotingServices.MANAGEMENT_CHANNEL,verificationHandler,newControllers);
}
