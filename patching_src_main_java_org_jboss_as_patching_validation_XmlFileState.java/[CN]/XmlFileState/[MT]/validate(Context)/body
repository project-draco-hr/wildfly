{
  if (patchBuilder == null) {
    if (!file.exists()) {
      ctx.getErrorHandler().error("File doesn't exist: " + file.getAbsolutePath());
    }
 else {
      try {
        patchBuilder=(PatchBuilder)PatchXml.parse(file);
      }
 catch (      Exception e) {
        ctx.getErrorHandler().error("Failed to parse file: " + file.getAbsolutePath(),e);
      }
    }
  }
}
