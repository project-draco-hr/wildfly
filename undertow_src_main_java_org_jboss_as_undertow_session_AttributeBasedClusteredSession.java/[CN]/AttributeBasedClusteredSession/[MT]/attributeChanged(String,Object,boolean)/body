{
  if (removal) {
    if (attrModifiedMap_.containsKey(key)) {
      attrModifiedMap_.remove(key);
    }
    attrRemovedSet_.add(key);
  }
 else {
    if (attrRemovedSet_.contains(key)) {
      attrRemovedSet_.remove(key);
    }
    attrModifiedMap_.put(key,value);
  }
  sessionAttributesDirty();
}
