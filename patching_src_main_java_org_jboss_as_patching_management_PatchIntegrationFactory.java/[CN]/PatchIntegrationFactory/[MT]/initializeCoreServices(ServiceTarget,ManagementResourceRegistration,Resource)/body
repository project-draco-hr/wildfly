{
  final PatchInfoService service=new PatchInfoService();
  serviceTarget.addService(PatchInfoService.NAME,service).addDependency(JBOSS_PRODUCT_CONFIG_SERVICE,ProductConfig.class,service.getProductConfig()).setInitialMode(ServiceController.Mode.ACTIVE).install();
  final InstallationManagerService installationMgr=new InstallationManagerService();
  serviceTarget.addService(InstallationManagerService.NAME,installationMgr).addDependency(JBOSS_PRODUCT_CONFIG_SERVICE,ProductConfig.class,installationMgr.getInjectedProductConfig()).setInitialMode(ServiceController.Mode.ACTIVE).install();
  registration.registerSubModel(PatchResourceDefinition.INSTANCE);
  resource.registerChild(PatchResourceDefinition.PATH,Resource.Factory.create());
}
