{
  final PatchInfoService service=new PatchInfoService();
  serviceTarget.addService(PatchInfoService.NAME,service).addDependency(JBOSS_DIRECTORY_STRUCTURE_SERVICE,DirectoryStructure.class,service.getDirectoryStructure()).addDependency(JBOSS_PRODUCT_CONFIG_SERVICE,ProductConfig.class,service.getProductConfig()).setInitialMode(ServiceController.Mode.ACTIVE).install();
  registration.registerSubModel(PatchResourceDefinition.INSTANCE);
  resource.registerChild(PatchResourceDefinition.PATH,Resource.Factory.create());
}
