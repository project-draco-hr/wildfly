{
  File f=new File(keystoreURL);
  if (!f.exists()) {
    throw new Exception("Keystore [" + keystoreURL + "] doesn't exist."+ "\nkeystore could be created: keytool -genkey -alias vault -keyalg RSA -keysize 1024  -keystore "+ keystoreURL);
  }
 else   if (!f.canWrite() || !f.isFile()) {
    throw new Exception("Keystore [" + keystoreURL + "] is not writable or not a file.");
  }
}
