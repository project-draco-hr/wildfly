{
  Resource result;
  if (registry != null && registry.isRuntimeOnly()) {
    try {
      result=context.readResource(PathAddress.EMPTY_ADDRESS,false);
    }
 catch (    RuntimeException e) {
      result=PlaceholderResource.INSTANCE;
    }
  }
 else {
    result=context.readResource(PathAddress.EMPTY_ADDRESS,false);
  }
  return result;
}
