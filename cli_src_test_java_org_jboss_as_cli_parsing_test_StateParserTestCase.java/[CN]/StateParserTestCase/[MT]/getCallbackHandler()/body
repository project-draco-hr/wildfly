{
  return new ParsingStateCallbackHandler(){
    @Override public void enteredState(    ParsingContext ctx){
      if (temp == null) {
        temp=result;
      }
      ParsedTerm newTerm=new ParsedTerm(temp);
      temp=newTerm;
    }
    @Override public void leavingState(    ParsingContext ctx){
      temp.parent.addChild(temp);
      temp=temp.parent;
    }
    @Override public void character(    ParsingContext ctx) throws OperationFormatException {
      temp.append(ctx.getCharacter());
    }
  }
;
}
