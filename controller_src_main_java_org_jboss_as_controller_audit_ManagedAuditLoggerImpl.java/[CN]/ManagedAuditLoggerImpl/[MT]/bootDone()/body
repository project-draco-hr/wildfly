{
  config.lock();
  try {
    config.setBooting(false);
    if (childImpls != null) {
      for (      ManagedAuditLogger child : childImpls) {
        child.bootDone();
      }
    }
    if (config.getLoggerStatus() == Status.QUEUEING) {
      setLoggerStatus(AuditLogger.Status.DISABLED);
    }
  }
  finally {
    config.unlock();
  }
}
