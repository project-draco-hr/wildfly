{
  try {
switch (getLoggerStatus()) {
case QUEUEING:
      queuedItems.add(item);
    break;
case LOGGING:
  writeLogItem(item);
break;
case DISABLE_NEXT:
writeLogItem(item);
config.setLoggerStatus(Status.DISABLED);
case DISABLED:
break;
}
}
  finally {
if (handlerUpdateTask != null && !config.isManualCommit()) {
handlerUpdateTask.applyChanges();
handlerUpdateTask=null;
}
}
}
