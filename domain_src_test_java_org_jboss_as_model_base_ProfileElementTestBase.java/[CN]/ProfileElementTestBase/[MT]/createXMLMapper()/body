{
  XMLMapper mapper=XMLMapper.Factory.create();
  MockRootElementParser.registerXMLElementReaders(mapper,getTargetNamespace());
  mapper.registerRootElement(new QName(getTargetNamespace(),Element.PROFILE.getLocalName()),new TestXMLElementReader<ProfileElement>(callback));
  MockAnyElementParser.registerXMLElementReaders(mapper);
  return mapper;
}
