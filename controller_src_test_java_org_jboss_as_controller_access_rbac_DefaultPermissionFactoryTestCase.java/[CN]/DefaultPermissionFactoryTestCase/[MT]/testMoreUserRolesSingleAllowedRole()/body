{
  testResource(PERMISSIVE,new StandardRole[]{StandardRole.MONITOR,StandardRole.OPERATOR},new StandardRole[]{StandardRole.MONITOR},true);
  testResource(PERMISSIVE,new StandardRole[]{StandardRole.MONITOR,StandardRole.OPERATOR},new StandardRole[]{StandardRole.OPERATOR},true);
  testResource(PERMISSIVE,new StandardRole[]{StandardRole.MONITOR,StandardRole.OPERATOR},new StandardRole[]{StandardRole.ADMINISTRATOR},false);
  testAttribute(PERMISSIVE,new StandardRole[]{StandardRole.MONITOR,StandardRole.OPERATOR},new StandardRole[]{StandardRole.MONITOR},true);
  testAttribute(PERMISSIVE,new StandardRole[]{StandardRole.MONITOR,StandardRole.OPERATOR},new StandardRole[]{StandardRole.OPERATOR},true);
  testAttribute(PERMISSIVE,new StandardRole[]{StandardRole.MONITOR,StandardRole.OPERATOR},new StandardRole[]{StandardRole.ADMINISTRATOR},false);
}
