{
  if (xmlName == null) {
    xmlName=name;
  }
  if (maxSize < 1) {
    maxSize=Integer.MAX_VALUE;
  }
  if (validator == null) {
    validator=new ModelTypeValidator(valueType,allowNull,allowExpression);
  }
  return new PrimitiveListAttributeDefinition(name,xmlName,allowNull,allowExpression,valueType,minSize,maxSize,validator,alternatives,requires,attributeMarshaller,resourceOnly,deprecated,accessConstraints,nullSignficant,flags);
}
