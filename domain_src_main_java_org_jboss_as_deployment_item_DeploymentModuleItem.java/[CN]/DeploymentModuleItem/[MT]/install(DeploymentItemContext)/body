{
  final BatchBuilder batchBuilder=context.getBatchBuilder();
  final DeploymentModuleService deploymentModuleService=new DeploymentModuleService(moduleConfig);
  batchBuilder.addService(DeploymentModuleService.SERVICE_NAME.append(deploymentName),deploymentModuleService).addDependency(DeploymentModuleLoaderProvider.SERVICE_NAME,DeploymentModuleLoaderProvider.class,new TranslatingInjector<DeploymentModuleLoaderProvider,DeploymentModuleLoader>(new DeploymentModuleLoaderProviderTranslator(deploymentRoot),deploymentModuleService.getDeploymentModuleLoaderInjector()));
}
