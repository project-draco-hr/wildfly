{
  super(reader,type);
switch (type) {
case LINK_LOCAL_ADDRESS:
case LOOPBACK:
case MULTICAST:
case POINT_TO_POINT:
case PUBLIC_ADDRESS:
case SITE_LOCAL_ADDRESS:
case UP:
case VIRTUAL:
    break;
default :
  throw new IllegalArgumentException(type.getLocalName() + " is not a valid simple criteria type");
}
}
