{
  if (entry == null) {
    throw new IllegalArgumentException("Cannot remove a null entry from the cache");
  }
  if (count < 1) {
    throw new IllegalStateException("Trying to remove an entry from an empty cache");
  }
  entry.key=null;
  entry.object=null;
  if (count == 1) {
    head=null;
    tail=null;
  }
 else {
    if (entry.prev == null) {
      head=entry.next;
      head.prev=null;
      entry.next=null;
    }
 else     if (entry.next == null) {
      tail=entry.prev;
      tail.next=null;
      entry.prev=null;
    }
 else {
      entry.next.prev=entry.prev;
      entry.prev.next=entry.next;
      entry.prev=null;
      entry.next=null;
    }
  }
  --count;
  entryRemoved(entry);
}
