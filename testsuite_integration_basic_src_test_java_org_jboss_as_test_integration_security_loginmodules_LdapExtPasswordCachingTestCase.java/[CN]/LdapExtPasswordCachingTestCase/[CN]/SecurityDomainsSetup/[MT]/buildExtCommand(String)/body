{
  File java=new File(System.getProperty("java.home"),"/bin/java");
  File javaExe=new File(System.getProperty("java.home"),"/bin/java.exe");
  String jre;
  if (java.exists())   jre=java.getAbsolutePath();
 else   jre=javaExe.getAbsolutePath();
  String cmd=jre + " -cp " + ExternalPasswordProvider.class.getProtectionDomain().getCodeSource().getLocation().getPath()+ " org.jboss.as.test.integration.security.loginmodules.ExternalPasswordProvider ";
  return "{" + extOption + "}"+ cmd;
}
