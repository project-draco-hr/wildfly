{
  Object[] relations;
  CachedRow row=(CachedRow)rowsById.get(pk);
  if (row != null && row.relations != null && row.locker == null) {
    promoteRow(row);
    relations=new Object[row.relations.length];
    System.arraycopy(row.relations,0,relations,0,relations.length);
  }
 else {
    relations=null;
  }
  return relations;
}
