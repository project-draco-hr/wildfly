{
  CachedRow row=(CachedRow)rowsById.remove(pk);
  if (row == null || row.locker != null && !tx.equals(row.locker)) {
    if (row == null) {
      throw CmpMessages.MESSAGES.removeRejected(pk,tx);
    }
 else {
      throw CmpMessages.MESSAGES.removeRejected(pk,tx,row.locker);
    }
  }
  dereference(row);
  row.locker=null;
}
