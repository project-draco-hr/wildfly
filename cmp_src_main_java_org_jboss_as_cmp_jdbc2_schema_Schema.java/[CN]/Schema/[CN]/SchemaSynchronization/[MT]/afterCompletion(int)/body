{
  if (status == Status.STATUS_MARKED_ROLLBACK || status == Status.STATUS_ROLLEDBACK || status == Status.STATUS_ROLLING_BACK) {
    for (int i=0; i < views.entityViews.length; ++i) {
      Table.View view=views.entityViews[i];
      if (view != null) {
        view.rolledback();
      }
    }
  }
 else {
    for (int i=0; i < views.entityViews.length; ++i) {
      Table.View view=views.entityViews[i];
      if (view != null) {
        view.committed();
      }
    }
  }
}
