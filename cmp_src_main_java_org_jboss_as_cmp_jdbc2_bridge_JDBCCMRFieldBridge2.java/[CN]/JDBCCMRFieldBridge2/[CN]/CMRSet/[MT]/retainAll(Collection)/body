{
  Set value=state.getLoadedValue(ctx);
  if (c == null || c.isEmpty()) {
    if (value.isEmpty()) {
      return false;
    }
 else {
      clear();
    }
  }
  boolean modified=false;
  Set idSet=argumentToIdSet(c);
  Object[] valueCopy=value.toArray();
  for (int i=0; i < valueCopy.length; ++i) {
    Object id=valueCopy[i];
    if (!idSet.contains(id)) {
      removeById(id);
      modified=true;
    }
  }
  return modified;
}
