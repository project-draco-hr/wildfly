{
  try {
    container.newInstance(managedBean);
  }
 catch (  IllegalAccessException e) {
    throw MESSAGES.instanceCreationFailed(e);
  }
catch (  InvocationTargetException e) {
    throw MESSAGES.instanceCreationFailed(e);
  }
catch (  NamingException e) {
    throw MESSAGES.instanceCreationFailed(e);
  }
}
