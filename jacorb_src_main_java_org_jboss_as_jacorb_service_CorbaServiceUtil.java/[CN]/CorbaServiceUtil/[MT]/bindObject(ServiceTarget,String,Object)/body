{
  final BinderService binderService=new BinderService(contextName);
  target.addService(ContextNames.JBOSS_CONTEXT_SERVICE_NAME.append(contextName),binderService).addDependency(ContextNames.JBOSS_CONTEXT_SERVICE_NAME,NamingStore.class,binderService.getNamingStoreInjector()).addInjection(binderService.getManagedObjectInjector(),new ValueManagedReferenceFactory(Values.immediateValue(value))).setInitialMode(ServiceController.Mode.ACTIVE).install();
}
