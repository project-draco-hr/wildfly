{
  final InjectedValue<ChannelFactory> factory=new InjectedValue<ChannelFactory>();
  return target.addService(ChannelFactoryService.getServiceName(null),new ValueService<ChannelFactory>(factory)).addDependency(ChannelFactoryService.getServiceName(stack),ChannelFactory.class,factory).setInitialMode(ServiceController.Mode.ON_DEMAND).install();
}
