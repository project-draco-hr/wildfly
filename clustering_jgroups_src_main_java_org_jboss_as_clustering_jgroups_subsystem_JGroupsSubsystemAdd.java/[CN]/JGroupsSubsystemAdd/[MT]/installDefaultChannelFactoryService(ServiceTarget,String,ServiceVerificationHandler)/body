{
  InjectedValue<ChannelFactory> factory=new InjectedValue<ChannelFactory>();
  ValueService<ChannelFactory> service=new ValueService<ChannelFactory>(factory);
  ServiceBuilder<ChannelFactory> channelFactoryBuilder=target.addService(ChannelFactoryService.getServiceName(null),service).addDependency(ChannelFactoryService.getServiceName(stack),ChannelFactory.class,factory).setInitialMode(ServiceController.Mode.ON_DEMAND);
  return channelFactoryBuilder.install();
}
