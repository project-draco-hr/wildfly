{
  ServerGroupDeploymentElement toRemove=standalone.getDeployment(deploymentName);
  if (toRemove != null) {
    if (startStopHandler == null && toRemove.isStart()) {
      throw new UpdateFailedException("Cannot remove deployment " + deploymentName + " as its "+ Attribute.START+ " attribute is 'true'. Deployment must be undeployed before removal.");
    }
    standalone.removeDeployment(deploymentName);
  }
}
