{
  List<Future<?>> futures=new ArrayList<Future<?>>();
  for (  Runnable r : concurrentTasks) {
    futures.add(executorService.submit(r));
  }
  for (int i=0; i < futures.size(); i++) {
    Future<?> future=futures.get(i);
    try {
      future.get();
    }
 catch (    InterruptedException e) {
      logger.errorf("%s caught InterruptedException waiting for task %s; returning",ConcurrentUpdateTask.class.getSimpleName(),concurrentTasks.get(i).toString());
      Thread.currentThread().interrupt();
      return;
    }
catch (    ExecutionException e) {
      logger.errorf(e,"%s caught ExecutionException waiting for task %s",ConcurrentUpdateTask.class.getSimpleName(),concurrentTasks.get(i).toString());
    }
  }
}
