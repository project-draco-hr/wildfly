{
  super.setUp();
  delegate=new JvmElementTestCommon(new ContentAndElementGetter(getXMLMapper(),getTargetNamespace(),getTargetNamespaceLocation()){
    @Override public String getFullContent(    String testContent){
      testContent=ModelParsingSupport.wrapJvms(testContent);
      String fullcontent=ModelParsingSupport.getXmlContent(Element.HOST.getLocalName(),targetNamespace,targetNamespaceLocation,testContent);
      return fullcontent;
    }
    @Override public JvmElement getTestJvmElement(    String fullcontent) throws XMLStreamException, FactoryConfigurationError, UpdateFailedException {
      HostModel root=ModelParsingSupport.parseHostModel(mapper,fullcontent);
      JvmElement jvm=root.getJvm("test");
      assertNotNull(jvm);
      return jvm;
    }
  }
);
}
