{
  try {
    if (readFullModelDescription == null) {
      readFullModelDescription=childFirstClassLoaderServices.getClass().getMethod("readFullModelDescription",childFirstClassLoader.loadClass(ModelNode.class.getName()));
    }
    return convertModelNodeFromChildCl(readFullModelDescription.invoke(childFirstClassLoaderServices,convertModelNodeToChildCl(pathAddress)));
  }
 catch (  Exception e) {
    unwrapInvocationTargetRuntimeException(e);
    throw new RuntimeException(e);
  }
}
