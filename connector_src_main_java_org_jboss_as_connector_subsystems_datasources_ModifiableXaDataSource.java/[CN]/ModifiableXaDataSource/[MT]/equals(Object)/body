{
  if (this == obj)   return true;
  if (!super.equals(obj))   return false;
  if (!(obj instanceof ModifiableXaDataSource))   return false;
  ModifiableXaDataSource other=(ModifiableXaDataSource)obj;
  if (driver == null) {
    if (other.driver != null)     return false;
  }
 else   if (!driver.equals(other.driver))   return false;
  if (newConnectionSql == null) {
    if (other.newConnectionSql != null)     return false;
  }
 else   if (!newConnectionSql.equals(other.newConnectionSql))   return false;
  if (xaDataSourceClass == null) {
    if (other.xaDataSourceClass != null)     return false;
  }
 else   if (!xaDataSourceClass.equals(other.xaDataSourceClass))   return false;
  if (xaDataSourceProperty == null) {
    if (other.xaDataSourceProperty != null)     return false;
  }
 else   if (!xaDataSourceProperty.equals(other.xaDataSourceProperty))   return false;
  if (xaPool == null) {
    if (other.xaPool != null)     return false;
  }
 else   if (!xaPool.equals(other.xaPool))   return false;
  return true;
}
