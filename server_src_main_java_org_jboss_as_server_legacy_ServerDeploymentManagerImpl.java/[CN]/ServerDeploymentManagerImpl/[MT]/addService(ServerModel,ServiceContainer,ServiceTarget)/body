{
  ServerDeploymentManagerImpl service=new ServerDeploymentManagerImpl(serverConfiguration,serviceContainer);
  target.addService(SERVICE_NAME_LOCAL,service).addDependency(ServerDeploymentRepository.SERVICE_NAME,ServerDeploymentRepository.class,service.injectedDeploymentRepository).addDependency(ShutdownHandler.SERVICE_NAME,ShutdownHandler.class,service.injectedShutdownHandler).addInjection(service.injectedDeploymentExecutor,Executors.newCachedThreadPool()).install();
}
