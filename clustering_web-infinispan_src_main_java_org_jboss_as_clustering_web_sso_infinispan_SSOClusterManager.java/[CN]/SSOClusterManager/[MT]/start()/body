{
  @SuppressWarnings("rawtypes") AdvancedCache cache=this.cacheRef.getValue().getAdvancedCache().with(this.getClass().getClassLoader());
  this.cache=cache;
  this.credentialCache=cache;
  this.sessionCache=new AtomicMapCache<SessionKey,FullyQualifiedSessionId,Void>(cache);
  this.cache.addListener(this);
}
