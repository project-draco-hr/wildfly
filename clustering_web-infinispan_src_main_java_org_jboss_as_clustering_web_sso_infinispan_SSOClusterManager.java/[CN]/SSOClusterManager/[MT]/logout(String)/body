{
  if (log.isTraceEnabled())   log.tracef("Registering logout of SSO %s in clustered cache",ssoId);
  Operation<Void> operation=new Operation<Void>(){
    @Override public Void invoke(    Cache<SSOKey,Object> cache){
      Cache<SSOKey,Object> removeCache=cache.getAdvancedCache().withFlags(Flag.SKIP_REMOTE_LOOKUP);
      removeCache.remove(new SessionKey(ssoId));
      removeCache.remove(new CredentialKey(ssoId));
      return null;
    }
  }
;
  this.invoker.invoke(this.cache,operation);
}
