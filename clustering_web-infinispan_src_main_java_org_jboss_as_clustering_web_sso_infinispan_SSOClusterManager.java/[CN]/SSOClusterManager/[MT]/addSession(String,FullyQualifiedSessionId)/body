{
  if (log.isTraceEnabled())   log.tracef("addSession(): adding Session %s to cached session set for SSO %s",sessionId.getSessionId(),ssoId);
  final SessionKey key=new SessionKey(ssoId);
  SessionOperation<Void> operation=new SessionOperation<Void>(){
    @Override public Void invoke(    Cache<SessionKey,Map<FullyQualifiedSessionId,Void>> cache){
      cache.putIfAbsent(key,null).put(sessionId,null);
      return null;
    }
  }
;
  this.batch(this.sessionCache,operation);
}
