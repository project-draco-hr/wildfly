{
  String subsystemXml="<subsystem xmlns='urn:jboss:domain:osgi:1.0' activation='lazy'>" + "  <capabilities>" + "    <capability name='org.acme.module1'/>"+ "    <capability name='org.acme.module2' startlevel='1'/>"+ "  </capabilities>"+ "</subsystem>";
  List<ModelNode> operations=parse(subsystemXml);
  Assert.assertEquals(3,operations.size());
  ModelNode addSubsystem=operations.get(0);
  Assert.assertEquals(ModelDescriptionConstants.ADD,addSubsystem.get(ModelDescriptionConstants.OP).asString());
  assertOSGiSubsystemAddress(addSubsystem.get(ModelDescriptionConstants.OP_ADDR));
  checkData(operations,1,CommonAttributes.CAPABILITY,"org.acme.module1",CommonAttributes.STARTLEVEL,"undefined");
  checkData(operations,2,CommonAttributes.CAPABILITY,"org.acme.module2",CommonAttributes.STARTLEVEL,"1");
}
