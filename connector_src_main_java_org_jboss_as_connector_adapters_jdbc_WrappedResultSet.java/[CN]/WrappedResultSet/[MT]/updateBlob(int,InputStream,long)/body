{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateBlob(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnIndex,inputStream,length);
    resultSet.updateBlob(columnIndex,inputStream,length);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
