{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateNCharacterStream(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnLabel,reader);
    resultSet.updateNCharacterStream(columnLabel,reader);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
