{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateNString(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnLabel,string);
    resultSet.updateNString(columnLabel,string);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
