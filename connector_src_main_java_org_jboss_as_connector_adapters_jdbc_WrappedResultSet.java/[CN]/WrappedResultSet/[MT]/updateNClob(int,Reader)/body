{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateNClob(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnIndex,reader);
    resultSet.updateNClob(columnIndex,reader);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
