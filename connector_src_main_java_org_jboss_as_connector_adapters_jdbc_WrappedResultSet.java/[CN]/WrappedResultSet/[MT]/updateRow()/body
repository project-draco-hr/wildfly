{
  statement.lock();
  try {
    checkTransaction();
    try {
      if (spy)       spyLogger.debugf("%s [%s] updateRow()",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET);
      resultSet.updateRow();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    statement.unlock();
  }
}
