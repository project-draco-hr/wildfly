{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] getNCharacterStream(%s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnIndex);
    return resultSet.getNCharacterStream(columnIndex);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
