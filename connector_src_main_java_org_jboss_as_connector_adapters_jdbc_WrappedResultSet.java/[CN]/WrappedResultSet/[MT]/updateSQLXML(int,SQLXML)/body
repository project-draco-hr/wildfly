{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateSQLXML(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnIndex,xmlObject);
    resultSet.updateSQLXML(columnIndex,xmlObject);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
