{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateClob(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnLabel,reader,length);
    resultSet.updateClob(columnLabel,reader,length);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
