{
  checkState();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateCharacterStream(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnName,reader,length);
    resultSet.updateCharacterStream(columnName,reader,length);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
