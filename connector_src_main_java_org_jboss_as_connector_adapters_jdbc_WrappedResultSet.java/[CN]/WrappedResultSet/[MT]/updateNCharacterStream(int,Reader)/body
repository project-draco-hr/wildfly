{
  ResultSet resultSet=getUnderlyingResultSet();
  try {
    if (spy)     spyLogger.debugf("%s [%s] updateNCharacterStream(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_RESULTSET,columnIndex,x);
    resultSet.updateNCharacterStream(columnIndex,x);
  }
 catch (  Throwable t) {
    throw checkException(t);
  }
}
