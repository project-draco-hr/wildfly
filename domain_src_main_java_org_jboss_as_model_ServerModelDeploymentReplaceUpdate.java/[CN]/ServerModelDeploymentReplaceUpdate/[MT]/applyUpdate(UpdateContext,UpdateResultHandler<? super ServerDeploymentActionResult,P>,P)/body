{
  if (deploymentElement != null) {
    startStopHandler.redeploy(newDeployment,deploymentElement.getRuntimeName(),deploymentElement.getSha1Hash(),updateContext.getServiceContainer(),resultHandler,param);
  }
 else   if (resultHandler != null) {
    resultHandler.handleFailure(new IllegalStateException("Unknown deployment " + newDeployment),param);
  }
}
