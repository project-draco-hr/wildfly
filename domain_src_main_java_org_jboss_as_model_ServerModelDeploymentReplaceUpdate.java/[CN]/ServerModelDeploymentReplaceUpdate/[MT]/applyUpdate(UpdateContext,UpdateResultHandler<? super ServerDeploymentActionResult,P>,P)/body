{
  if (deploymentElement != null) {
    startStopHandler.undeploy(toReplace,updateContext,resultHandler,param);
    startStopHandler.deploy(newDeployment,deploymentElement.getRuntimeName(),deploymentElement.getSha1Hash(),updateContext,resultHandler,param);
  }
 else   if (resultHandler != null) {
    throw new IllegalStateException("Unknown deployment " + newDeployment);
  }
}
