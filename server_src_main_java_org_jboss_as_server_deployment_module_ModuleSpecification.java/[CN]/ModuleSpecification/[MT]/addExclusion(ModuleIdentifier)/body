{
  exclusions.add(exclusion);
  Iterator<ModuleDependency> it=systemDependencies.iterator();
  while (it.hasNext()) {
    ModuleDependency dep=it.next();
    if (dep.getIdentifier().equals(exclusion)) {
      it.remove();
    }
  }
  it=localDependencies.iterator();
  while (it.hasNext()) {
    ModuleDependency dep=it.next();
    if (dep.getIdentifier().equals(exclusion)) {
      it.remove();
    }
  }
}
