{
synchronized (lock) {
    if (shutdown) {
      return;
    }
    final Map<String,ManagedProcess> processes=this.processes;
    final ManagedProcess process=processes.get(processName);
    if (process == null) {
      log.warnf("Attempted to start non-existent process '%s'",processName);
      return;
    }
    process.start();
  }
}
