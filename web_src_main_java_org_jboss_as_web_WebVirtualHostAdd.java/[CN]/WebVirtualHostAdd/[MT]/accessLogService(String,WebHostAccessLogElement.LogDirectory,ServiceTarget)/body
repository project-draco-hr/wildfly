{
  if (directory == null) {
    RelativePathService.addService(WebSubsystemElement.JBOSS_WEB_HOST.append(hostName,"access-log"),hostName,DEFAULT_RELATIVE_TO,target);
  }
 else {
    final String relativeTo=directory.getRelativeTo() != null ? directory.getRelativeTo() : DEFAULT_RELATIVE_TO;
    final String path=directory.getPath() != null ? directory.getPath() : hostName;
    RelativePathService.addService(WebSubsystemElement.JBOSS_WEB_HOST.append(hostName,"access-log"),path,relativeTo,target);
  }
}
