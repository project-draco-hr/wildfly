{
  Status status=currentStatus;
  if (status == Status.MORE) {
    status=StreamUtils.readWord(inputStream,b);
    final String address=b.toString();
    if (status != Status.MORE) {
      return status;
    }
    status=StreamUtils.readWord(inputStream,b);
    final String port=b.toString();
    handler.handleReconnectServerManager(address,port);
  }
  return status;
}
