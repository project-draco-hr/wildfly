{
  boolean showModel=model.hasDefined(CommonAttributes.SHOW_MODEL) ? model.get(CommonAttributes.SHOW_MODEL).asBoolean() : false;
  ServiceController<?> controller=verificationHandler != null ? MBeanServerService.addService(context.getServiceTarget(),showModel,verificationHandler) : MBeanServerService.addService(context.getServiceTarget(),showModel);
  if (newControllers != null) {
    newControllers.add(controller);
  }
}
