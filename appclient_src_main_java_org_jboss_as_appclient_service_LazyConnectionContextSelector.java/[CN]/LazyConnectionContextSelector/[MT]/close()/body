{
  try {
    if (connection != null) {
      connection.close();
    }
  }
 catch (  IOException e) {
    AppClientLogger.ROOT_LOGGER.exceptionClosingConnection(e);
  }
  try {
    if (endpoint != null) {
      endpoint.close();
    }
  }
 catch (  IOException e) {
    AppClientLogger.ROOT_LOGGER.exceptionClosingConnection(e);
  }
}
