{
  lock();
  try {
    PreparedStatement statement=getUnderlyingStatement();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setBlob(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT,parameterIndex,inputStream);
      statement.setBlob(parameterIndex,inputStream);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
