{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setNull(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT,parameterIndex,sqlType,typeName);
      ps.setNull(parameterIndex,sqlType,typeName);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
