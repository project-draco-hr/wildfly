{
  lock();
  try {
    PreparedStatement statement=getUnderlyingStatement();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setRowId(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT,parameterIndex,x);
      statement.setRowId(parameterIndex,x);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
