{
  lock();
  try {
    checkTransaction();
    try {
      checkConfiguredQueryTimeout();
      if (spy)       spyLogger.debugf("%s [%s] executeQuery()",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT);
      ResultSet resultSet=ps.executeQuery();
      return registerResultSet(resultSet);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
