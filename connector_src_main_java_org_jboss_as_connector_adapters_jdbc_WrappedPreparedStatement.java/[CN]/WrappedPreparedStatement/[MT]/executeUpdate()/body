{
  lock();
  try {
    checkTransaction();
    try {
      checkConfiguredQueryTimeout();
      if (spy)       spyLogger.debugf("%s [%s] executeUpdate()",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT);
      return ps.executeUpdate();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
