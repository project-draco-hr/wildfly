{
  lock();
  try {
    PreparedStatement statement=getUnderlyingStatement();
    try {
      if (spy)       spyLogger.debugf("%s [%s] isPoolable()",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT);
      return statement.isPoolable();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
