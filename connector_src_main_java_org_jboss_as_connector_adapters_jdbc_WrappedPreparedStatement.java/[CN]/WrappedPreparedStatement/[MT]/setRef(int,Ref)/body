{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setRef(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT,parameterIndex,value);
      ps.setRef(parameterIndex,value);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
