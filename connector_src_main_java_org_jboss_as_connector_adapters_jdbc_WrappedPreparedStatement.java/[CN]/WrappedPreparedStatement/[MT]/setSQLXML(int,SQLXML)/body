{
  lock();
  try {
    PreparedStatement statement=getUnderlyingStatement();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setSQLXML(%s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT,parameterIndex,xmlObject);
      statement.setSQLXML(parameterIndex,xmlObject);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
