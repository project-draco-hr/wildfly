{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] setCharacterStream(%s, %s, %s)",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT,parameterIndex,reader,length);
      ps.setCharacterStream(parameterIndex,reader,length);
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
