{
  lock();
  try {
    checkState();
    try {
      if (spy)       spyLogger.debugf("%s [%s] clearParameters()",jndiName,Constants.SPY_LOGGER_PREFIX_PREPARED_STATEMENT);
      ps.clearParameters();
    }
 catch (    Throwable t) {
      throw checkException(t);
    }
  }
  finally {
    unlock();
  }
}
