{
  SubsystemParser parser=testSubsystem("simple-with-supplements.xml","org.jboss.as.simple","full",false);
  String marshalled=marshall(parser);
  String expected="<?xml version=\"1.0\" ?>" + "<subsystem xmlns=\"urn:jboss:domain:simple-with-text-and-comments:1.0\">" + "   <some-element value=\"false\"/>"+ "  <childA childA-attr=\"child-one\">Hello</childA>"+ "  <childB ohildB-attr=\"child two\">"+ "    <childB1/>"+ "  </childB>"+ "</subsystem>";
  Assert.assertEquals(normalizeXML(expected),normalizeXML(marshalled));
}
