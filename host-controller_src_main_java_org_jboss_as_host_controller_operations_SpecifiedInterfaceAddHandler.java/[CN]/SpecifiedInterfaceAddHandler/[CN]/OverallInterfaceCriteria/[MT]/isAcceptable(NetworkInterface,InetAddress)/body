{
  HashSet<InetAddress> addresses=new HashSet<InetAddress>();
  for (  InterfaceCriteria criteria : interfaceCriteria) {
    InetAddress bindAddress=criteria.isAcceptable(networkInterface,address);
    if (bindAddress == null)     return null;
    log.tracef("%s accepted, provided address: %s",criteria,bindAddress);
    addresses.add(bindAddress);
  }
  InetAddress bindAddress=null;
  if (addresses.size() > 0) {
    if (addresses.size() == 1)     bindAddress=addresses.iterator().next();
 else {
      if (addresses.size() > 2)       log.warnf("More than two unique criteria addresses were seen: %s\n",addresses.toString());
 else {
        addresses.remove(address);
        if (addresses.size() == 1)         bindAddress=addresses.iterator().next();
 else         log.warnf("Two unique criteria addresses were seen: %s\n",addresses.toString());
      }
    }
  }
  return bindAddress;
}
