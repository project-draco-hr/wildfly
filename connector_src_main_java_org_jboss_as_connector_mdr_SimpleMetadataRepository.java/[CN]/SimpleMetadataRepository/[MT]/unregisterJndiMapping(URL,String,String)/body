{
  if (deployment == null)   throw new IllegalArgumentException("Deployment is null");
  if (clz == null)   throw new IllegalArgumentException("Clz is null");
  if (jndi == null)   throw new IllegalArgumentException("Jndi is null");
  Map<String,List<String>> mappings=jndiMappings.get(deployment);
  if (mappings != null) {
    List<String> l=mappings.get(clz);
    if (l != null) {
      l.remove(jndi);
      if (l.size() == 0) {
        mappings.remove(clz);
      }
    }
    if (mappings.size() == 0) {
      jndiMappings.remove(deployment);
    }
  }
}
