{
  if (deployment == null)   throw new IllegalArgumentException("Deployment is null");
  if (md == null)   throw new IllegalArgumentException("Metadata is null");
  if (raTemplates.containsKey(deployment))   throw new AlreadyExistsException(deployment + " already registered");
  raTemplates.put(deployment,md);
  raRoots.put(deployment,root);
  ironJacamar.put(deployment,ijmd);
}
