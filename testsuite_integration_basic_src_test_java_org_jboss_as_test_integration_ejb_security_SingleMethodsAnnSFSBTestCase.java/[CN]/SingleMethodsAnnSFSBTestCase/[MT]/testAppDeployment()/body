{
  try {
    createSecurityDomain();
  }
 catch (  Exception e) {
    log.warn("Problems during creation of security domain",e);
  }
  log.info("Security domain: ejb3-tests created");
  final JavaArchive jar=ShrinkWrap.create(JavaArchive.class,MODULE + ".jar").addClass(SingleMethodsAnnOnlyCheckSFSB.class).addClass(SimpleAuthorizationRemote.class).addAsResource("ejb3/security/users.properties","users.properties").addAsResource("ejb3/security/roles.properties","roles.properties").addAsManifestResource("ejb3/security/EMPTY_MANIFEST.MF","MANIFEST.MF");
  log.info(jar.toString(true));
  return jar;
}
