{
  try {
    getControllerClient().close();
  }
 catch (  IOException e) {
    throw new RuntimeException("Could not close connection",e);
  }
 finally {
    if (connector != null) {
      try {
        connector.close();
      }
 catch (      IOException e) {
        throw new RuntimeException("Could not close JMX connection",e);
      }
    }
  }
}
