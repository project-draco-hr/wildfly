{
  final WebArchive war=ShrinkWrap.create(WebArchive.class,"ejb3security.war").addPackage(WhoAmIBean.class.getPackage()).addPackage(HttpRequest.class.getPackage()).addClass(Base64.class).addAsResource("security/users.properties","users.properties").addAsResource("security/roles.properties","roles.properties").addAsWebInfResource("ejb3/security/web.xml","web.xml");
  log.info(war.toString(true));
  return war;
}
