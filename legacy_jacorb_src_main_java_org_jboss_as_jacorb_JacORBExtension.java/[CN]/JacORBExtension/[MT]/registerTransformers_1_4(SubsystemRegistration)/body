{
  ResourceTransformationDescriptionBuilder builder=ResourceTransformationDescriptionBuilder.Factory.createSubsystemInstance();
  builder.getAttributeBuilder().addRejectCheck(RejectAttributeChecker.DEFINED,JacORBSubsystemDefinitions.PERSISTENT_SERVER_ID);
  TransformationDescription.Tools.register(builder.build(),subsystem,ModelVersion.create(1,4,0));
}
