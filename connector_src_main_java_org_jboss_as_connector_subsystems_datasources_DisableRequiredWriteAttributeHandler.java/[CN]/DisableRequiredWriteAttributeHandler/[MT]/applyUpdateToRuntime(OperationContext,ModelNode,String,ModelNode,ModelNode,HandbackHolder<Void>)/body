{
  ModelNode submodel=context.readResource(PathAddress.EMPTY_ADDRESS).getModel();
  if ((submodel.hasDefined(Constants.ENABLED.getName()) && submodel.get(Constants.ENABLED.getName()).asBoolean()) || Constants.JNDINAME.getName().equals(attributeName)) {
    return true;
  }
 else {
    return false;
  }
}
