{
  final ParsedCommandLine args=ctx.getParsedCommandLine();
  try {
    if (!this.name.isValueComplete(args)) {
      return staticArgs.get(name);
    }
  }
 catch (  CommandFormatException e) {
    return null;
  }
  if (allArgs == null) {
    allArgs=loadArguments(ctx);
  }
  return allArgs.get(name);
}
