{
synchronized (this) {
    if (groups.contains(serverGroup)) {
      responseCount++;
      if (failed) {
        this.failed=true;
      }
      DomainControllerLogger.DOMAIN_DEPLOYMENT_LOGGER.tracef("Recorded group result for '%s': failed = %s",serverGroup,failed);
      notifyAll();
    }
 else {
      throw MESSAGES.unknownServerGroup(serverGroup);
    }
  }
}
