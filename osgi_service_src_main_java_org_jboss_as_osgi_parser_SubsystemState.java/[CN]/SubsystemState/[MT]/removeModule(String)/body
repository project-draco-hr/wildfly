{
  ModuleIdentifier identifier=ModuleIdentifier.fromString(id);
synchronized (modules) {
    for (Iterator<OSGiModule> it=modules.iterator(); it.hasNext(); ) {
      OSGiModule module=it.next();
      if (module.getIdentifier().equals(identifier)) {
        it.remove();
        return module;
      }
    }
    return null;
  }
}
