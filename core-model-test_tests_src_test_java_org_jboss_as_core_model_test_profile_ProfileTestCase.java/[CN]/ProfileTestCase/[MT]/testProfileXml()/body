{
  KernelServices kernelServices=createKernelServicesBuilder(TestModelType.DOMAIN).setXmlResource("domain.xml").build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),"domain.xml"),marshalled);
  kernelServices=createKernelServicesBuilder(TestModelType.DOMAIN).setXml(marshalled).build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  ProfileUtils.executeDescribeProfile(kernelServices,"testA");
}
