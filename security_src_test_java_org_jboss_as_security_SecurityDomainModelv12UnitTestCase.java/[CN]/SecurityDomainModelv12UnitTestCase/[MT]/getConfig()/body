{
  PathAddress subsystemAddress=PathAddress.pathAddress(SecurityExtension.PATH_SUBSYSTEM);
  PathAddress securityDomain=subsystemAddress.append(SecurityExtension.SECURITY_DOMAIN_PATH);
  return new FailedOperationTransformationConfig().addFailedAttribute(subsystemAddress,new RejectExpressionsConfig(SecuritySubsystemRootResourceDefinition.DEEP_COPY_SUBJECT_MODE)).addFailedAttribute(securityDomain,new RejectExpressionsConfig(SecurityDomainResourceDefinition.CACHE_TYPE)).addFailedAttribute(securityDomain.append(SecurityExtension.JSSE_PATH),new RejectExpressionsConfig(JSSEResourceDefinition.ADDITIONAL_PROPERTIES)).addFailedAttribute(subsystemAddress.append(SecurityExtension.VAULT_PATH),new RejectExpressionsConfig(VaultResourceDefinition.OPTIONS));
}
