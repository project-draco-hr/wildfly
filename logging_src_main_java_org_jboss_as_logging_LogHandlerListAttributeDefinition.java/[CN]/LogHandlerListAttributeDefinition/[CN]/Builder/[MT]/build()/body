{
  if (xmlName == null)   xmlName=name;
  if (maxSize < 1)   maxSize=Integer.MAX_VALUE;
  if (propertyName == null)   propertyName=name;
  if (attributeMarshaller == null)   attributeMarshaller=HandlersAttributeMarshaller.INSTANCE;
  return new LogHandlerListAttributeDefinition(name,xmlName,propertyName,CommonAttributes.HANDLER,allowNull,minSize,maxSize,alternatives,requires,attributeMarshaller,resourceOnly,deprecated,accessConstraints,nullSignficant,parser,flags);
}
