{
  jgSupport.restoreProperties();
  if (threadPool != null) {
    threadPool.shutdownNow();
  }
  for (  DistributableSessionManager<?> manager : managers) {
    if (manager != null) {
      try {
        manager.stop();
      }
 catch (      Exception e) {
        log.error(e.getMessage(),e);
      }
    }
  }
  for (  CacheContainer cacheContainer : cacheContainers) {
    if (cacheContainer != null) {
      try {
        cacheContainer.stop();
      }
 catch (      Exception e) {
        log.error(e.getMessage(),e);
      }
    }
  }
}
