{
  final ServiceName cacheServiceName=CacheService.getServiceName(containerName,cacheName);
  final ContextNames.BindInfo binding=this.createCacheBinding(jndiName,containerName,cacheName);
  final BinderService binder=new BinderService(binding.getBindName());
  return target.addService(binding.getBinderServiceName(),binder).addAliases(ContextNames.JAVA_CONTEXT_SERVICE_NAME.append(binding.getBindName())).addDependency(cacheServiceName,Cache.class,new ManagedReferenceInjector<Cache>(binder.getManagedObjectInjector())).addDependency(binding.getParentContextServiceName(),ServiceBasedNamingStore.class,binder.getNamingStoreInjector()).setInitialMode(ServiceController.Mode.PASSIVE).install();
}
