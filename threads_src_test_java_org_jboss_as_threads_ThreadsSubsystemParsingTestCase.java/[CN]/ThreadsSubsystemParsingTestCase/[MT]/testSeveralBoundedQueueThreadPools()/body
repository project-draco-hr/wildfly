{
  List<ModelNode> updates=createSubSystem("<bounded-queue-thread-pool name=\"test-poolA\"><max-threads count=\"1\"/><queue-length count=\"1\"/></bounded-queue-thread-pool>" + "<bounded-queue-thread-pool name=\"test-poolB\"><max-threads count=\"1\"/><queue-length count=\"1\"/></bounded-queue-thread-pool>");
  assertEquals(3,updates.size());
  for (  ModelNode update : updates) {
    try {
      executeForResult(update);
    }
 catch (    OperationFailedException e) {
      throw new RuntimeException(e.getFailureDescription().toString());
    }
  }
  ModelNode subsystem=model.require("subsystem").require("threads");
  ModelNode threadFactory=subsystem.require("bounded-queue-thread-pool");
  assertEquals(2,threadFactory.keys().size());
}
