{
  try {
    messageHandler.handleMessage(this,pis);
  }
 catch (  RuntimeException e) {
    CONNECTION_LOGGER.failedToReadMessage(e);
  }
catch (  IOException e) {
    CONNECTION_LOGGER.failedToReadMessage(e);
  }
catch (  NoClassDefFoundError e) {
    CONNECTION_LOGGER.failedToReadMessage(e);
  }
catch (  Error e) {
    CONNECTION_LOGGER.failedToReadMessage(e);
    throw e;
  }
 finally {
    StreamUtils.safeClose(pis);
  }
}
