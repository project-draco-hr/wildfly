{
  if (cliCommand != null)   return cliCommand;
  File targetDir=new File("../../build/target");
  if ((!targetDir.exists()) || (!targetDir.isDirectory()))   throw new Exception("Missing AS target directory.");
  String[] children=targetDir.list();
  String asDir=null;
  for (  String child : children) {
    if (child.startsWith("jboss-as")) {
      asDir=child;
      break;
    }
  }
  if (asDir == null)   throw new Exception("Missing AS target directory.");
  cliCommand="java -Djboss.home.dir=target/jbossas " + "-Djboss.modules.dir=../../build/target/" + asDir + "/modules "+ "-Djline.WindowsTerminal.directConsole=false "+ "-jar ./target/jbossas/jboss-modules.jar "+ "-mp ../../build/target/"+ asDir+ "/modules "+ "-logmodule org.jboss.logmanager org.jboss.as.cli";
  return cliCommand;
}
