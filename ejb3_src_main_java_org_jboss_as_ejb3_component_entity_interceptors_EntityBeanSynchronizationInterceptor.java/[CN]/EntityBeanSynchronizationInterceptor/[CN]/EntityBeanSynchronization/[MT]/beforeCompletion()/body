{
synchronized (threadLock) {
    try {
      if (!componentInstance.isRemoved() && !componentInstance.isDiscarded()) {
        componentInstance.store();
      }
    }
 catch (    Throwable t) {
      lock.pushOwner(lockOwner);
      try {
        handleThrowableInTxSync(componentInstance,t);
      }
  finally {
        lock.popOwner();
      }
    }
  }
}
