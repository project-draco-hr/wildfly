{
synchronized (threadLock) {
    try {
      componentInstance.store();
    }
 catch (    Throwable t) {
      lock.pushOwner(getLockOwner(componentInstance.getComponent().getTransactionSynchronizationRegistry()));
      try {
        handleThrowableInTxSync(componentInstance,t);
      }
  finally {
        lock.popOwner();
      }
    }
  }
}
