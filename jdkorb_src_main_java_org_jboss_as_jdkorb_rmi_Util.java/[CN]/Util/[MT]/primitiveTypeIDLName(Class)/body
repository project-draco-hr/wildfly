{
  if (type == Void.TYPE)   return "void";
  if (type == Boolean.TYPE)   return "boolean";
  if (type == Character.TYPE)   return "wchar";
  if (type == Byte.TYPE)   return "octet";
  if (type == Short.TYPE)   return "short";
  if (type == Integer.TYPE)   return "long";
  if (type == Long.TYPE)   return "long long";
  if (type == Float.TYPE)   return "float";
  if (type == Double.TYPE)   return "double";
  throw ORBMessages.MESSAGES.notAPrimitive(type.getName());
}
