{
  super(ID);
  setDefaultHandler(new CharacterHandler(){
    @Override public void handle(    ParsingContext ctx) throws CommandFormatException {
      if (Character.isWhitespace(ctx.getCharacter())) {
        ctx.leaveState();
      }
 else {
        ctx.getCallbackHandler().character(ctx);
      }
    }
  }
);
}
