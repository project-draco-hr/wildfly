{
  final SubsystemRegistration subsystem=context.registerSubsystem(BatchSubsystemDefinition.NAME,MANAGEMENT_API_MAJOR_VERSION,MANAGEMENT_API_MINOR_VERSION,MANAGEMENT_API_MICRO_VERSION);
