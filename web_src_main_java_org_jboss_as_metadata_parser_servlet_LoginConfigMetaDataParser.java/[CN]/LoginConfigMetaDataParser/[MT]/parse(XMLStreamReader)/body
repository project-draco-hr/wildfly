{
  LoginConfigMetaData loginConfig=new LoginConfigMetaData();
  final int count=reader.getAttributeCount();
  for (int i=0; i < count; i++) {
    final String value=reader.getAttributeValue(i);
    if (reader.getAttributeNamespace(i) != null) {
      continue;
    }
    final Attribute attribute=Attribute.forName(reader.getAttributeLocalName(i));
switch (attribute) {
case ID:
{
        loginConfig.setId(value);
        break;
      }
default :
    throw unexpectedAttribute(reader,i);
}
}
while (reader.hasNext() && reader.nextTag() != END_ELEMENT) {
final Element element=Element.forName(reader.getLocalName());
switch (element) {
case AUTH_METHOD:
  loginConfig.setAuthMethod(reader.getElementText());
break;
case REALM_NAME:
loginConfig.setRealmName(reader.getElementText());
break;
case FORM_LOGIN_CONFIG:
loginConfig.setFormLoginConfig(FormLoginConfigMetaDataParser.parse(reader));
break;
default :
throw unexpectedElement(reader);
}
}
return loginConfig;
}
