{
  File cliLogFile=new File(CLI_LOG_FILE);
  if (cliLogFile.exists()) {
    cliLogFile.delete();
  }
  InputStream is=null;
  try {
    is=getClass().getResourceAsStream(CLI_LOG_CFG);
    LogManager.getLogManager().readConfiguration(is);
  }
  finally {
    if (is != null) {
      is.close();
    }
  }
}
