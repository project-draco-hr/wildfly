{
  setUp();
  String deploymentName="as7_1452.rar";
  ResourceAdapterArchive raa=ShrinkWrap.create(ResourceAdapterArchive.class,deploymentName);
  JavaArchive ja=ShrinkWrap.create(JavaArchive.class,"as7_1452.jar");
  ja.addPackage("org.jboss.as.test.smoke.embedded.deployment.rar.configproperty").addClasses(AbstractMgmtTestBase.class,MgmtOperationException.class);
  raa.addAsLibrary(ja);
  raa.addAsManifestResource("rar/" + deploymentName + "/META-INF/ra.xml","ra.xml").addAsManifestResource(new StringAsset("Dependencies: org.jboss.as.controller-client,org.jboss.dmr,org.jboss.as.cli\n"),"MANIFEST.MF");
  ;
  return raa;
}
