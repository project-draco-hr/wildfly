{
  byte[] bytes=new byte[8];
  readFully(in,bytes,0,8);
  return (((long)bytes[0] << 56) + ((long)(bytes[1] & 255) << 48) + ((long)(bytes[2] & 255) << 40)+ ((long)(bytes[3] & 255) << 32)+ ((long)(bytes[4] & 255) << 24)+ ((bytes[5] & 255) << 16)+ ((bytes[6] & 255) << 8)+ ((bytes[7] & 255) << 0));
}
