{
  System.setProperty("jboss.as.test.transformation.hack","true");
  try {
    ModelVersion modelVersion=ModelVersion.create(1,1,0);
    KernelServicesBuilder builder=createKernelServicesBuilder(AdditionalInitialization.MANAGEMENT).setSubsystemXmlResource(subsystemXml);
    builder.createLegacyKernelServicesBuilder(null,modelVersion).addMavenResourceURL("org.jboss.as:jboss-as-connector:" + mavenVersion).setExtensionClassName("org.jboss.as.connector.subsystems.resourceadapters.ResourceAdaptersExtension");
    KernelServices mainServices=builder.build();
    org.junit.Assert.assertTrue(mainServices.isSuccessfulBoot());
    KernelServices legacyServices=mainServices.getLegacyServices(modelVersion);
    org.junit.Assert.assertTrue(legacyServices.isSuccessfulBoot());
    org.junit.Assert.assertNotNull(legacyServices);
    checkSubsystemModelTransformation(mainServices,modelVersion);
  }
  finally {
    System.clearProperty("jboss.as.test.transformation.hack");
  }
}
