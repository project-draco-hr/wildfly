{
  super.setup(denied);
  add(denied,InvocationType.DEPENDENCY_INJECTION,MethodType.GET_EJB_LOCAL_OBJECT);
  add(denied,InvocationType.DEPENDENCY_INJECTION,MethodType.GET_EJB_OBJECT);
