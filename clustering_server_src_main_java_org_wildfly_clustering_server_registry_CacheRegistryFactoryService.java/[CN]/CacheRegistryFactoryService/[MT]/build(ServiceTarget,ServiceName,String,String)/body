{
  CacheRegistryFactoryService<K,V> service=new CacheRegistryFactoryService<>();
  return AsynchronousService.addService(target,name,service).addDependency(CacheNodeFactoryProvider.getServiceName(containerName,cacheName),CacheNodeFactory.class,service.factory).addDependency(CacheGroupProvider.getServiceName(containerName,cacheName),Group.class,service.group).addDependency(CacheService.getServiceName(containerName,cacheName),Cache.class,service.cache);
}
