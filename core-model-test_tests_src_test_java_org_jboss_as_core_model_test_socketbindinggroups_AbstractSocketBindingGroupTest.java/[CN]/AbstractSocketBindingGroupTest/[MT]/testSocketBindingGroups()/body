{
  KernelServices kernelServices=createKernelServicesBuilder().setXmlResource(getXmlResource()).build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
  String marshalled=kernelServices.getPersistedSubsystemXml();
  ModelTestUtils.compareXml(ModelTestUtils.readResource(this.getClass(),getXmlResource()),marshalled);
  kernelServices=createKernelServicesBuilder().setXml(marshalled).build();
  Assert.assertTrue(kernelServices.isSuccessfulBoot());
}
