{
  final List<SetupAction> sucessfulActions=new ArrayList<SetupAction>();
  for (  SetupAction action : setupActions) {
    try {
      action.setup();
      sucessfulActions.add(action);
    }
 catch (    Throwable e) {
      for (      SetupAction s : sucessfulActions) {
        try {
          s.teardown();
        }
 catch (        Throwable t) {
        }
      }
      throw new RuntimeException(e);
    }
  }
}
