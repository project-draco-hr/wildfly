{
  DeploymentManager manager=getDeploymentManager("nobody","nopass");
  try {
    Target[] targets=manager.getTargets();
    try {
      manager.getAvailableModules(ModuleType.EAR,targets);
      fail("TargetException expected");
    }
 catch (    TargetException ex) {
    }
  }
  finally {
    manager.release();
  }
}
