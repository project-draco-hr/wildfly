{
switch (affectsType) {
case ORIGINAL:
    appliesToStructure.registerIgnoredPath(path);
  break;
case UPDATED:
updatedStructure.registerIgnoredPath(path);
break;
case BOTH:
appliesToStructure.registerIgnoredPath(path);
updatedStructure.registerIgnoredPath(path);
break;
case NONE:
break;
default :
throw new IllegalStateException();
}
return this;
}
