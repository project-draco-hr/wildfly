{
switch (affectsType) {
case ORIGINAL:
    appliesToStructure.registerStandardModuleSearchPath(name,standardPath);
  break;
case UPDATED:
updatedStructure.registerStandardModuleSearchPath(name,standardPath);
break;
case BOTH:
appliesToStructure.registerStandardModuleSearchPath(name,standardPath);
updatedStructure.registerStandardModuleSearchPath(name,standardPath);
break;
case NONE:
break;
default :
throw new IllegalStateException();
}
return this;
}
