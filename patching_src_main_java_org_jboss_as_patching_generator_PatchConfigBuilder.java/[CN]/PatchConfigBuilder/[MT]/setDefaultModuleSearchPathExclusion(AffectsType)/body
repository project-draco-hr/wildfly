{
switch (affectsType) {
case ORIGINAL:
    appliesToStructure.excludeDefaultModuleRoot();
  break;
case UPDATED:
updatedStructure.excludeDefaultModuleRoot();
break;
case BOTH:
appliesToStructure.excludeDefaultModuleRoot();
updatedStructure.excludeDefaultModuleRoot();
break;
case NONE:
break;
default :
throw new IllegalStateException();
}
return this;
}
