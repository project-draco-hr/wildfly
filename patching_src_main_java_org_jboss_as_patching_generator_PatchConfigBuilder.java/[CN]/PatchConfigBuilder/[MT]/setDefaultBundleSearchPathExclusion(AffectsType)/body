{
switch (affectsType) {
case ORIGINAL:
    appliesToStructure.excludeDefaultBundleRoot();
  break;
case UPDATED:
updatedStructure.excludeDefaultBundleRoot();
break;
case BOTH:
appliesToStructure.excludeDefaultBundleRoot();
updatedStructure.excludeDefaultBundleRoot();
break;
case NONE:
break;
default :
throw new IllegalStateException();
}
return this;
}
