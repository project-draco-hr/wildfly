def is_root(self):
    p = subprocess.Popen('whoami /groups', shell=True, stdout=subprocess.PIPE)
    stdout = p.communicate()[0]
    if ('S-1-16-12288' in stdout):
        return True
    else:
        cmd = 'net localgroup administrators | find "%USERNAME"'
        print cmd
        return (subprocess.call(cmd, shell=True) == 0)
