{
  ResourceTransformationDescriptionBuilder child=parent.addChildRedirection(INSTANCE.getPathElement(),PathElement.pathElement(EJB3SubsystemModel.CLUSTER_PASSIVATION_STORE));
  child.getAttributeBuilder().setValueConverter(AttributeConverter.Factory.createHardCoded(new ModelNode(true),true),EJB3SubsystemModel.PASSIVATE_EVENTS_ON_REPLICATE).setValueConverter(AttributeConverter.Factory.createHardCoded(new ModelNode("default"),true),EJB3SubsystemModel.CLIENT_MAPPINGS_CACHE).setValueConverter(AttributeConverter.Factory.createHardCoded(new ModelNode().set(Integer.MAX_VALUE),true),EJB3SubsystemModel.IDLE_TIMEOUT).setValueConverter(AttributeConverter.Factory.createHardCoded(new ModelNode().set(TimeUnit.SECONDS.name()),true),EJB3SubsystemModel.IDLE_TIMEOUT_UNIT);
}
