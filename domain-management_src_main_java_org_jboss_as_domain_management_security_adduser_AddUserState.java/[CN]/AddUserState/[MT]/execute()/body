{
  final String password=stateValues.getPassword();
  State nextState;
  if (password == null) {
    nextState=new ErrorState(theConsole,DomainManagementLogger.ROOT_LOGGER.noPasswordExiting(),null,stateValues);
  }
 else {
    nextState=update(stateValues);
  }
  if (nextState == null && stateValues.isInteractive()) {
    nextState=new ConfirmationChoice(theConsole,DomainManagementLogger.ROOT_LOGGER.serverUser(),DomainManagementLogger.ROOT_LOGGER.yesNo(),new DisplaySecret(theConsole,stateValues),null);
  }
  return nextState;
}
