{
  final File moduleXml=new File(file,"module.xml");
  if (moduleXml.exists()) {
    findJars(file,jars);
  }
 else {
    final File[] files=file.listFiles();
    if (files != null) {
      for (      final File child : files) {
        if (child.getName().equals(".overlays")) {
          continue;
        }
        if (child.isDirectory()) {
          findModuleRoots(child,jars);
        }
      }
    }
  }
}
