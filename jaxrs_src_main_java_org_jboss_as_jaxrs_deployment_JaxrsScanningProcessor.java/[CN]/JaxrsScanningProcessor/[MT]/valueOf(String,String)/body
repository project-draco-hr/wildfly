{
  if (value == null) {
    throw JaxrsMessages.MESSAGES.invalidParamValue(paramName,value);
  }
  if (value.toLowerCase(Locale.ENGLISH).equals("true")) {
    return true;
  }
 else   if (value.toLowerCase(Locale.ENGLISH).equals("false")) {
    return false;
  }
 else {
    throw JaxrsMessages.MESSAGES.invalidParamValue(paramName,value);
  }
}
