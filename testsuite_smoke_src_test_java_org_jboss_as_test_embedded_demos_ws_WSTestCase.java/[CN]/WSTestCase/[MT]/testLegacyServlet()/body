{
  final String request="<soapenv:Envelope xmlns:soapenv=\"http://schemas.xmlsoap.org/soap/envelope/\" xmlns:arc=\"http://archive.ws.demos.as.jboss.org/\">" + "  <soapenv:Header/>" + "  <soapenv:Body>"+ "    <arc:echoString>"+ "      <arg0>Foo</arg0>"+ "    </arc:echoString>"+ "  </soapenv:Body>"+ "</soapenv:Envelope>";
  String s=performCall("",request);
  Assert.assertNotNull(s);
  Assert.assertTrue(s.contains("echoStringResponse"));
  Assert.assertTrue(s.contains("Foo"));
}
