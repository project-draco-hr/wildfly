{
  WebArchive war=ShrinkWrap.create(WebArchive.class,"war-example.war");
  war.addClasses(HttpRequest.class,DefaultContextServiceTestServlet.class,TestServletRunnable.class);
  war.addAsManifestResource(createPermissionsXmlAsset(new RuntimePermission("org.jboss.security.*"),new RuntimePermission("getClassLoader"),new RuntimePermission("modifyThread")),"permissions.xml");
  return war;
}
