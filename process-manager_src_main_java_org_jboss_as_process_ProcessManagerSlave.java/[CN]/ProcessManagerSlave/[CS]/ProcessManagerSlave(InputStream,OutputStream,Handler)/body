{
  if (input == null) {
    throw new IllegalArgumentException("input is null");
  }
  if (output == null) {
    throw new IllegalArgumentException("output is null");
  }
  if (handler == null) {
    throw new IllegalArgumentException("handler is null");
  }
  this.input=input;
  this.output=output;
  this.handler=handler;
}
