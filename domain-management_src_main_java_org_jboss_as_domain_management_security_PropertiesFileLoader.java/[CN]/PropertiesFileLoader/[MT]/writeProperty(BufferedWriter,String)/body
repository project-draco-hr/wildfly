{
  String escapedKey=escapeString(key,ESCAPE_ARRAY);
  final String newLine;
  if (Boolean.valueOf(toSave.getProperty(key + "!disable"))) {
    newLine="#" + escapedKey + "="+ toSave.getProperty(key);
  }
 else {
    newLine=escapedKey + "=" + toSave.getProperty(key);
  }
  write(writer,newLine,true);
}
