{
  DomainClient domainClient=testSupport.getDomainMasterLifecycleUtil().getDomainClient();
  try {
    tearDownRoles(domainClient);
  }
  finally {
    try {
      removeDeployment1(domainClient);
    }
  finally {
      testSupport=null;
      SimpleRbacProviderTestSuite.stopSupport();
    }
  }
}
