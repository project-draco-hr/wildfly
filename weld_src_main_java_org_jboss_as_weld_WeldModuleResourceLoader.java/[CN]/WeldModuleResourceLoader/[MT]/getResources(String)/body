{
  try {
    final HashSet<URL> resources=new HashSet<URL>();
    Enumeration<URL> urls=module.getClassLoader().getResources(name);
    while (urls.hasMoreElements()) {
      resources.add(urls.nextElement());
    }
    return resources;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
