{
  if (anyLocal)   throw new IllegalStateException(element + " cannot be combined with " + Element.ANY_ADDRESS.getLocalName());
  if (anyLocalV4)   throw new IllegalStateException(element + " cannot be combined with " + Element.ANY_IPV4_ADDRESS.getLocalName());
  if (anyLocalV6)   throw new IllegalStateException(element + " cannot be combined with " + Element.ANY_IPV6_ADDRESS.getLocalName());
}
