{
  if (has(node,CACHED_CONNECTION_MANAGER_DEBUG) || has(node,CACHED_CONNECTION_MANAGER_ERROR)) {
    writer.writeEmptyElement(Element.CACHED_CONNECTION_MANAGER.getLocalName());
    if (has(node,CACHED_CONNECTION_MANAGER_DEBUG))     writeAttribute(writer,Attribute.DEBUG,node.require(CACHED_CONNECTION_MANAGER_DEBUG));
    if (has(node,CACHED_CONNECTION_MANAGER_ERROR))     writeAttribute(writer,Attribute.ERROR,node.require(CACHED_CONNECTION_MANAGER_ERROR));
  }
}
