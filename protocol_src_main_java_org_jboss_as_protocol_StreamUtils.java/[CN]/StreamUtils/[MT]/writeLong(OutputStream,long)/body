{
  out.write((byte)(v >>> 56) & 0xFF);
  out.write((byte)(v >>> 48) & 0xFF);
  out.write((byte)(v >>> 40) & 0xFF);
  out.write((byte)(v >>> 32) & 0xFF);
  out.write((byte)(v >>> 24) & 0xFF);
  out.write((byte)(v >>> 16) & 0xFF);
  out.write((byte)(v >>> 8) & 0xFF);
  out.write((byte)(v >>> 0) & 0xFF);
}
