{
  try {
    if (executeOperation == null) {
      executeOperation=childFirstClassLoaderServices.getClass().getMethod("executeOperation",childFirstClassLoader.loadClass(ModelNode.class.getName()),InputStream[].class);
    }
    return converter.convertModelNodeFromChildCl(executeOperation.invoke(childFirstClassLoaderServices,converter.convertModelNodeToChildCl(operation),inputStreams));
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
