{
  ModelVersion version=model.getVersion();
  String subsystemXml=readResource("subsystem-jgroups-transform.xml");
  KernelServicesBuilder builder=createKernelServicesBuilder(AdditionalInitialization.MANAGEMENT).setSubsystemXml(subsystemXml);
  builder.createLegacyKernelServicesBuilder(null,controller,version).addMavenResourceURL(mavenResourceURLs);
  KernelServices mainServices=builder.build();
  Assert.assertTrue(mainServices.isSuccessfulBoot());
  Assert.assertTrue(mainServices.getLegacyServices(version).isSuccessfulBoot());
  checkSubsystemModelTransformation(mainServices,version);
}
