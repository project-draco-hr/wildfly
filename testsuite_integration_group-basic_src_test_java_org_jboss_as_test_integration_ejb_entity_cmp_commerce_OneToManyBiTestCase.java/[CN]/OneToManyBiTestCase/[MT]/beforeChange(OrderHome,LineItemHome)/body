{
  a1=orderHome.create();
  a2=orderHome.create();
  b1=a1.getLineItems();
  b2=a2.getLineItems();
  for (int i=0; i < b1x.length; i++) {
    b1x[i]=lineItemHome.create();
    b1.add(b1x[i]);
  }
  for (int i=0; i < b2x.length; i++) {
    b2x[i]=lineItemHome.create();
    b2.add(b2x[i]);
  }
  for (int i=0; i < b1x.length; i++) {
    assertTrue(b1.contains(b1x[i]));
  }
  for (int i=0; i < b2x.length; i++) {
    assertTrue(b2.contains(b2x[i]));
  }
}
