{
  if (xmlName == null) {
    xmlName=name;
  }
  if (validator == null) {
    validator=new ObjectTypeValidator(allowNull,valueTypes);
  }
  return new ObjectTypeAttributeDefinition(name,xmlName,suffix,valueTypes,allowNull,validator,corrector,alternatives,requires,attributeMarshaller,resourceOnly,deprecated,accessConstraints,nullSignficant,parser,flags);
}
