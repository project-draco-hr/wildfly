{
  final ArrayList ret=new ArrayList(constants.length + attributes.length + operations.length);
  for (int i=0; i < constants.length; ++i)   ret.add(constants[i]);
  for (int i=0; i < attributes.length; ++i)   ret.add(attributes[i]);
  for (int i=0; i < operations.length; ++i)   ret.add(operations[i]);
  return ret;
}
