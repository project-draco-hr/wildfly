{
  requireNoAttributes(reader);
  while (reader.hasNext() && reader.nextTag() != XMLStreamConstants.END_ELEMENT) {
switch (EJB3SubsystemXMLElement.forName(reader.getLocalName())) {
case BEAN_INSTANCE_POOL_REF:
{
        final String poolName=readStringAttributeElement(reader,EJB3SubsystemXMLAttribute.POOL_NAME.getLocalName());
        final ModelNode setDefaultSLSBPoolOperation=this.createSetDefaultWriteAttributeOperation(EJB3SubsystemRootResourceDefinition.DEFAULT_SLSB_INSTANCE_POOL,poolName,reader.getLocation());
        operations.add(setDefaultSLSBPoolOperation);
        break;
      }
default :
{
      throw unexpectedElement(reader);
    }
}
}
}
