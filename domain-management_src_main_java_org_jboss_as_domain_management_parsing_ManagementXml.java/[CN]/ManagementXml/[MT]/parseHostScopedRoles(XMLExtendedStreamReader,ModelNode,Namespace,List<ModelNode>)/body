{
  ParseUtils.requireNoAttributes(reader);
  String scopedRoleType=HostScopedRolesResourceDefinition.PATH_ELEMENT.getKey();
  while (reader.hasNext() && reader.nextTag() != END_ELEMENT) {
    requireNamespace(reader,expectedNs);
    final Element element=Element.forName(reader.getLocalName());
switch (element) {
case ROLE:
{
        parseScopedRole(reader,address,expectedNs,list,scopedRoleType,Element.HOST,HostScopedRolesResourceDefinition.BASE_ROLE,HostScopedRolesResourceDefinition.HOSTS,false);
        break;
      }
default :
{
      throw unexpectedElement(reader);
    }
}
}
}
