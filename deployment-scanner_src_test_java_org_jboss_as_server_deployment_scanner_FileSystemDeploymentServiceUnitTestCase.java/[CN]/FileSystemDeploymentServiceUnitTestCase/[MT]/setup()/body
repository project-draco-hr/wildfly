{
  executor.clear();
  File root=testSupport.getTempDir();
  for (int i=0; i < 200; i++) {
    tmpDir=new File(root,String.valueOf(count++));
    if (!tmpDir.exists() && tmpDir.mkdirs()) {
      break;
    }
  }
  if (!tmpDir.exists()) {
    throw new RuntimeException("cannot create tmpDir");
  }
}
