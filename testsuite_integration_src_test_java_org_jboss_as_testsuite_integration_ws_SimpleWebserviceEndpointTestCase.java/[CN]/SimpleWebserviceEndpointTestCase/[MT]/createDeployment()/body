{
  final WebArchive war=ShrinkWrap.create(WebArchive.class,"ws-endpoint-example.war");
  war.addPackage(SimpleWebserviceEndpointImpl.class.getPackage());
  war.addClass(SimpleWebserviceEndpointImpl.class);
  war.addAsWebInfResource(WebXml.get("<servlet>" + "    <servlet-name>TestService</servlet-name>" + "    <servlet-class>org.jboss.as.testsuite.integration.ws.SimpleWebserviceEndpointImpl</servlet-class>"+ "  </servlet>"+ "  <servlet-mapping>"+ "    <servlet-name>TestService</servlet-name>"+ "    <url-pattern>/SimpleService</url-pattern>"+ "  </servlet-mapping>"),"web.xml");
  log.info(war.toString(true));
  return war;
}
