{
  ServerManager sm=null;
  try {
    ServerManagerEnvironment config=determineEnvironment(args,stdin,stdout,stderr);
    if (config == null) {
      stderr.println("no config");
      abort(null);
      return;
    }
 else {
      sm=new ServerManager(config);
      sm.start();
    }
  }
 catch (  Throwable t) {
    stderr.println("aborting");
    t.printStackTrace(stderr);
    abort(t);
    return;
  }
  sm.startServers();
}
