{
  ServerManager sm=null;
  try {
    ServerManagerEnvironment config=determineEnvironment(args,stdin,stdout,stderr);
    if (config == null) {
      abort(null);
      return null;
    }
 else {
      sm=new ServerManager(config);
      sm.start();
    }
  }
 catch (  Throwable t) {
    t.printStackTrace(stderr);
    abort(t);
    return null;
  }
  try {
    sm.startServers();
    return sm;
  }
 catch (  RuntimeException e) {
    e.printStackTrace(stderr);
    throw e;
  }
catch (  Error e) {
    e.printStackTrace(stderr);
    throw e;
  }
}
