{
  ServerManager sm=null;
  try {
    ServerManagerBootstrapConfig config=processCommandLine(args);
    if (config == null) {
      abort(null);
      return;
    }
 else {
      sm=new ServerManager(config);
      sm.start();
    }
  }
 catch (  Throwable t) {
    abort(t);
    return;
  }
  sm.startServers();
}
