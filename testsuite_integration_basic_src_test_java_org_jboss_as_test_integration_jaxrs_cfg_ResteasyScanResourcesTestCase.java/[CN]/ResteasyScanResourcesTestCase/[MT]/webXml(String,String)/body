  return WebXml.get("<servlet-mapping>\n" + "        <servlet-name>javax.ws.rs.core.Application</servlet-name>\n" + "        <url-pattern>/myjaxrs/*</url-pattern>\n"+ "</servlet-mapping>\n"+ "\n"+ "<context-param>\n"+ "        <param-name>" + paramName + "</param-name>\n"+ "        <param-value>"+ paramValue+ "</param-value>\n"+ "</context-param>\n"+ "\n");
