{
  StringBuffer buf=new StringBuffer();
  if (first != null) {
    buf.append(first);
  }
  if (mi != '\u0000') {
    if (first != null) {
      buf.append(" ");
    }
    buf.append(mi).append(".");
  }
  if (last != null) {
    if (first != null || mi != '\u0000') {
      buf.append(" ");
    }
    buf.append(last);
  }
  return buf.toString();
}
