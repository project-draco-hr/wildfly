{
  if (buffer.isEmpty()) {
    candidates.addAll(commands);
    return 0;
  }
  char firstChar=buffer.charAt(0);
  if (firstChar == '.' || firstChar == ':' || firstChar == '/') {
    return -1;
  }
  for (  String command : commands) {
    if (command.startsWith(buffer))     candidates.add(command);
  }
  return 0;
}
