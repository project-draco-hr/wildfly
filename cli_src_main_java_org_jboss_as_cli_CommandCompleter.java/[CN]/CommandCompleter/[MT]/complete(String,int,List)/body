{
  if (buffer.length() < 1)   return -1;
  if (buffer.charAt(0) != '/')   return -1;
  if (buffer.length() == 1) {
    candidates.addAll(commands);
  }
 else {
    String start=buffer.substring(1);
    for (    String command : commands) {
      if (command.startsWith(start))       candidates.add(command);
    }
  }
  Collections.sort(candidates);
  return 1;
}
