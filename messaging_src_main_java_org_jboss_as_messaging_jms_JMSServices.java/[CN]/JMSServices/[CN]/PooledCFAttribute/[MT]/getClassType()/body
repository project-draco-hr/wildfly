{
switch (def.getType()) {
case BOOLEAN:
    return Boolean.class.getName();
case BIG_DECIMAL:
  return Double.class.getName();
case LONG:
return Long.class.getName();
case INT:
return Integer.class.getName();
case STRING:
return String.class.getName();
default :
throw MESSAGES.invalidAttributeType(def.getName(),def.getType());
}
}
