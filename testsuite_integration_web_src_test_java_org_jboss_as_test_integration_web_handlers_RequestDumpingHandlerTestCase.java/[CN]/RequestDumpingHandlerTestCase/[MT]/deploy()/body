{
  WebArchive war=ShrinkWrap.create(WebArchive.class,DEPLOYMENT_WS + ".war").addPackage(WebSocketTestCase.class.getPackage()).addClass(TestSuiteEnvironment.class).addAsManifestResource(createPermissionsXmlAsset(new PropertyPermission("management.address","read"),new PropertyPermission("node0","read"),new SocketPermission("*:8080","connect,resolve")),"permissions.xml").addAsManifestResource(new StringAsset("io.undertow.websockets.jsr.UndertowContainerProvider"),"services/javax.websocket.ContainerProvider").addAsWebInfResource(RequestDumpingHandlerTestCase.class.getPackage(),"jboss-web-req-dump.xml","jboss-web.xml");
  return war;
}
