{
  ResourceTransformationDescriptionBuilder builder=TransformationDescriptionBuilder.Factory.createInstance(JvmResourceDefinition.GLOBAL.getPathElement()).getAttributeBuilder().setDiscard(DiscardAttributeChecker.UNDEFINED,LAUNCH_COMMAND).addRejectCheck(RejectAttributeChecker.DEFINED,LAUNCH_COMMAND).end();
  TransformationDescription.Tools.register(builder.build(),parent);
}
