{
  SQLException sqe=null;
  Statement s=null;
  ResultSet rs=null;
  try {
    s=c.createStatement();
    rs=s.executeQuery(VALID_QUERY);
  }
 catch (  SQLException e) {
    sqe=e;
  }
 finally {
    try {
      if (rs != null)       rs.close();
    }
 catch (    SQLException ignore) {
      log.warn("JDBC resource for " + this + " could not be closed");
    }
    try {
      if (s != null)       s.close();
    }
 catch (    SQLException ignore) {
      log.warn("JDBC resource for " + this + " could not be closed");
    }
  }
  return sqe;
}
