{
  int count=context.getAttachmentStreamCount();
  List<InputStream> result=new ArrayList<InputStream>(count);
  for (int i=0; i < count; i++) {
    try {
      result.add(context.getAttachmentStream(count));
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
  return result;
}
