{
  final ChannelOpenListenerService channelOpenListenerService=new ChannelOpenListenerService(channelName,OptionMap.EMPTY);
  addController(newControllers,serviceTarget.addService(channelOpenListenerService.getServiceName(),channelOpenListenerService).addDependency(RemotingServices.ENDPOINT,Endpoint.class,channelOpenListenerService.getEndpointInjector()).addDependency(operationHandlerName,ManagementOperationHandler.class,channelOpenListenerService.getOperationHandlerInjector()).setInitialMode(Mode.ACTIVE).install());
}
