def open_file(self, name):
    try:
        self.zipfile.close()
        self.zipfile = zipfile.ZipFile(self.name(), mode='r')
        name = self.prepend(name)
        file_obj = self.zipfile.open(name)
        return file_obj
    finally:
        self.zipfile.close()
        self.zipfile = zipfile.ZipFile(self.name(), mode='a')
