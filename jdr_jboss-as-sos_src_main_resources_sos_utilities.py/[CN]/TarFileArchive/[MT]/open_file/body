def open_file(self, name):
    try:
        self.tarfile.close()
        self.tarfile = tarfile.open(self.name(), mode='r')
        name = self.prepend(name)
        file_obj = self.tarfile.extractfile(name)
        file_obj = StringIO(file_obj.read())
        return file_obj
    finally:
        self.tarfile.close()
        self.tarfile = tarfile.open(self.name(), mode='a')
