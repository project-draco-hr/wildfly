{
  ModelNode operation=Util.createOperation(ADD,pathAddress(pathElement(CORE_SERVICE,MANAGEMENT),pathElement(ACCESS,ModelDescriptionConstants.AUTHORIZATION),pathElement(SERVER_GROUP_SCOPED_ROLE,roleName)));
  operation.get(BASE_ROLE).set(baseRole);
  operation.get(SERVER_GROUPS).add(AbstractServerGroupScopedRolesTestCase.SERVER_GROUP_A);
  RbacUtil.executeOperation(client,operation,expectedOutcome);
}
