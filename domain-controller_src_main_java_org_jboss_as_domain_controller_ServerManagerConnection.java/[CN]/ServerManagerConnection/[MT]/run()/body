{
  try {
    for (; ; ) {
      if (!socketConnection.isConnected())       break;
      ServerManagerConnectionProtocol.IncomingCommand.processNext(this,socketIn);
    }
  }
 catch (  Throwable t) {
    log.error(t);
    throw new RuntimeException(t);
  }
 finally {
    socketConnection.close();
  }
}
